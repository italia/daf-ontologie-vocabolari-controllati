  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-accommodation-facilities/accommodation-typology/B152">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
