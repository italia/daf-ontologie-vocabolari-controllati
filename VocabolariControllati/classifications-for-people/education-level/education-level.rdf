  <rdf:Description rdf:about="http://dati.gov.it/onto/controlledvocabulary/EducationLevel/ZZUSV">
    <skos:prefLabel xml:lang="en">Diploma of upper secondary education (2-3 years)</skos:prefLabel>
    <skos:inScheme rdf:resource="http://dati.gov.it/onto/controlledvocabulary/EducationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
