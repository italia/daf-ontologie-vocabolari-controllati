  <rdf:Description rdf:about="http://dati.gov.it/onto/controlledvocabulary/EducationLevel/ZZITS">
    <rdf:type rdf:resource="http://dati.gov.it/onto/cpvapit/EducationLevel"/>
    <cpvapit:educationLevelDesc xml:lang="en">Diploma of Higher Technical</cpvapit:educationLevelDesc>
    <dct:identifier>ZZ_ITS</dct:identifier>
    <skos:prefLabel xml:lang="it">Diploma di tecnico superiore ITS (corsi biennali) (dal 2013)</skos:prefLabel>
    <cpvapit:educationLevelDesc xml:lang="it">Diploma di tecnico superiore ITS (corsi biennali) (dal 2013)</cpvapit:educationLevelDesc>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:inScheme rdf:resource="http://dati.gov.it/onto/controlledvocabulary/EducationLevel"/>
    <cpvapit:educationLevelID>ZZ_ITS</cpvapit:educationLevelID>
    <skos:prefLabel xml:lang="en">Diploma of Higher Technical</skos:prefLabel>
    <skos:definition xml:lang="it">Il titolo di studio è rilasciato dagli Istituti Tecnici Superiori (ITS). I corsi durano generalmente due anni (raramente estendibili a tre) e si accede dopo il conseguimento di un diploma di scuola secondaria di II grado (di 5 anni).</skos:definition>
    <skos:notation>ZZ_ITS</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
