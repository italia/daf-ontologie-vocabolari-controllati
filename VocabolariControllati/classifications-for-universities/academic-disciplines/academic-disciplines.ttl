        ad:SSD-L-LIN-18,
        ad:SSD-L-LIN-19 ;
    skos:notation "10/G1" ;
    skos:prefLabel "Academic recruitment field: Historical and general linguistics"@en,
        "Settore concorsuale: Glottologia e linguistica"@it ;
    skos:topConceptOf <https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines> ;
