  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-05-B1">
    <skos:prefLabel xml:lang="it">Zoologia e antropologia</skos:prefLabel>
    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-BIO-05"/>
