    <skos:prefLabel xml:lang="it">Geologia strutturale, geologia stratigrafica, sedimentologia e paleontologia</skos:prefLabel>
    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-GEO-02"/>
