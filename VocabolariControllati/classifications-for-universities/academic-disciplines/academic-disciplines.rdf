    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-L-LIN-06"/>
    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-L-LIN-07"/>
