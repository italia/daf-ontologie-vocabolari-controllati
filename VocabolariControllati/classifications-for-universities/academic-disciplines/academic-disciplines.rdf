    <skos:broader rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/MSC-13-A"/>
    <skos:prefLabel xml:lang="en">Public economics</skos:prefLabel>
  </rdf:Description>
