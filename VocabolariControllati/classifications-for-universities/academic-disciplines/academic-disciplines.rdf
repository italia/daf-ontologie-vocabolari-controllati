    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-BIO-08"/>
    <skos:notation>05/B1</skos:notation>
    <skos:prefLabel xml:lang="en">Zoology and anthropology</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Zoologia e antropologia</skos:prefLabel>
    <clvapit:hasRankOrder>3</clvapit:hasRankOrder>
  </rdf:Description>
