    <skos:prefLabel xml:lang="en">Histology</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Istologia</skos:prefLabel>
    <skos:relatedMatch rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/006-018"/>
