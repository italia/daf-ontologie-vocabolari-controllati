    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-IUS-06"/>
    <skos:notation>12/E3</skos:notation>
    <skos:prefLabel xml:lang="en">Economics, financial and agri-food markets law and regulation, navigation and air law</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Diritto dell'economia, dei mercati finanziari e agroalimentari e della navigazione</skos:prefLabel>
    <clvapit:hasRankOrder>3</clvapit:hasRankOrder>
  </rdf:Description>
