  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-12-E2">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
