  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-09-G1">
    <skos:relatedMatch rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/012-004"/>
