  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-02-C1">
    <skos:notation>02/C1</skos:notation>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/HER/AcademicRecruitementField"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
