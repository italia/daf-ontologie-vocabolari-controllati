  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-14-D1">
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines"/>
    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-SPS-09"/>
