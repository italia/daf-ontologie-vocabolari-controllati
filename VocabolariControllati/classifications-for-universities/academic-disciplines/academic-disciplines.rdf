  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SC-09-A3">
    <skos:prefLabel xml:lang="it">Progettazione industriale, costruzioni meccaniche e metallurgia</skos:prefLabel>
    <skos:narrower rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-ING-IND-15"/>
