  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/9">
    <xkos:depth rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xkos:depth>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/HER/ItalianAcademicRole"/>
    <skos:definition xml:lang="it">Il ricercatore a tempo determinato svolge attività di ricerca e di didattica integrativa. I contratti hanno durata massima triennale e possono essere rinnovati per una durata complessiva di sei anni. Si tratta di una figura ad esaurimento.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:identifier>9</dct:identifier>
    <skos:prefLabel xml:lang="it">Ricercatore a tempo determinato</skos:prefLabel>
    <skos:notation>9</skos:notation>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"/>
  </rdf:Description>
