  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/12">
    <dct:identifier>12</dct:identifier>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/HER/ItalianAcademicRole"/>
    <skos:definition xml:lang="it">Il ricercatore a tempo indeterminato è una figura che contribuisce allo sviluppo della ricerca scientifica universitaria e assolve compiti didattici integrativi. Si tratta di una figura ad esaurimento.</skos:definition>
    <skos:prefLabel xml:lang="it">Ricercatore a tempo indeterminato</skos:prefLabel>
    <xkos:depth rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xkos:depth>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <owl:versionInfo xml:lang="it">Ruolo a esaurimento</owl:versionInfo>
