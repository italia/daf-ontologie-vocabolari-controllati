        <skos:prefLabel xml:lang="de">Unternehmensgründung</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Starting a company</skos:prefLabel>
        <skos:definition xml:lang="it">Questo evento della vita di un'impresa racchiude nello specifico servizi pubblici relativi all'avvio di un'azienda</skos:definition>
        <skos:definition xml:lang="en">This business event groups public services related to the starting of a company</skos:definition>
      </skos:Concept>
    </skos:hasTopConcept>
    <skos:hasTopConcept>
      <skos:Concept rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event/2">
