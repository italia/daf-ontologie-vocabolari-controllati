        <skos:prefLabel xml:lang="it">Avvio nuova attività professionale</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Starting a new activity </skos:prefLabel>
        <skos:prefLabel xml:lang="de">Neue berufliche Tätigkeit aufnehmen</skos:prefLabel>
        <skos:definition xml:lang="it">Questo evento della vita di un'impresa racchiude nello specifico servizi pubblici relativi all'avvio di una nuova attività di business per singoli professionisti.</skos:definition>
        <skos:definition xml:lang="en">This business event groups public services related to the starting of a new business activity for single professionals </skos:definition>
      </skos:Concept>
    </skos:hasTopConcept>
    <skos:hasTopConcept>
      <skos:Concept rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event/3">
        <rdf:type rdf:resource="https://w3id.org/italia/onto/CPSV/BusinessEvent"/>
        <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event"/>
        <skos:notation>3</skos:notation>
        <dct:identifier>3</dct:identifier>
        <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
        <skos:prefLabel xml:lang="it">Richiesta licenze/permessi/certificati</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Beantragung von Lizenzen, Genehmigungen und Zertifikaten</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Applying for licenses, permits and certificates</skos:prefLabel>
        <skos:definition xml:lang="it">Questo evento della vita di un'impresa raggruppa servizi pubblici relativi alle richieste di licenze, permessi, certificati o altri documenti ufficiali necessari prima di iniziare un'attività di business.</skos:definition>
        <skos:definition xml:lang="en">This business event groups public services related to the request of a licence, permit, certificate or other official document that have to be acquired before starting to execute a certain business activity.</skos:definition>
      </skos:Concept>
    </skos:hasTopConcept>
    <skos:hasTopConcept>
