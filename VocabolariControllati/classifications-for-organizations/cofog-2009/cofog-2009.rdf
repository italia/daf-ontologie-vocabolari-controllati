  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-organizations/cofog-2009/0722">
    <skos:prefLabel>Servizi medici specialistici</skos:prefLabel>
    <clvapit:hasRankOrder>3</clvapit:hasRankOrder>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-organizations/cofog-2009/072"/>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/COV/PublicOrgActivityType"/>
