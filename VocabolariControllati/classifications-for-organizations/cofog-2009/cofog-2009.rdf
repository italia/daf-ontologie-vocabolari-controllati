  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-organizations/cofog-2009/0811">
    <rdf:type rdf:resource="http://www.datiopen.istat.it/Ontologie/meta/gsim/ClassificationItem"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
