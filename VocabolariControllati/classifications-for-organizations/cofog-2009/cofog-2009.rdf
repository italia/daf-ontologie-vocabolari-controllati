    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-organizations/cofog-2009/065"/>
    <clvapit:hasRankOrder>3</clvapit:hasRankOrder>
