    <skos:prefLabel>Protezione dell'ambiente non altrove classificata</skos:prefLabel>
    <skos:notation>05.6.1</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
