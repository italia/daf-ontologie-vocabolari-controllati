    <skos:relatedMatch rdf:resource="http://thes.bncf.firenze.sbn.it/termine.php?id=5455&amp;menuR=1&amp;menuS=2%20e"/>
    <clvapit:hasRankOrder>2</clvapit:hasRankOrder>
  </rdf:Description>
