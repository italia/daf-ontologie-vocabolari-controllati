    <skos:prefLabel xml:lang="en">history of science and technology</skos:prefLabel>
    <skos:prefLabel xml:lang="it">storia della scienza e delle tecniche </skos:prefLabel>
    <clvapit:hasRankOrder>2</clvapit:hasRankOrder>
  </rdf:Description>
