    <skos:relatedMatch rdf:resource="http://thes.bncf.firenze.sbn.it/termine.php?id=9955&amp;menuR=1&amp;menuS=2"/>
    <clvapit:hasRankOrder>2</clvapit:hasRankOrder>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/015">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines"/>
