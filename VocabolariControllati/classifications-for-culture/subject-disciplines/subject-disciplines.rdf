    <skos:prefLabel xml:lang="en">historical-artistic sciences</skos:prefLabel>
    <skos:prefLabel xml:lang="it">scienze storico-artistiche</skos:prefLabel>
    <skos:relatedMatch rdf:resource="http://culturaitalia.it/pico/thesaurus/4.1#discipline_artistiche_e_storico_artistiche"/>
    <skos:topConceptOf rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines"/>
