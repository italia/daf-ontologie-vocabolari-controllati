  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/013-008-001">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>013.008.001</skos:notation>
    <skos:prefLabel xml:lang="it">archeologia greca</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines"/>
    <skos:broader rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/013-008"/>
    <skos:prefLabel xml:lang="en">greek archaeology</skos:prefLabel>
