    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines"/>
    <skos:notation>020.010</skos:notation>
    <skos:prefLabel xml:lang="en">epistemology</skos:prefLabel>
    <skos:prefLabel xml:lang="it">epistemologia</skos:prefLabel>
    <skos:relatedMatch rdf:resource="http://culturaitalia.it/pico/thesaurus/4.1#epistemologia"/>
