  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/020-010">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/020"/>
