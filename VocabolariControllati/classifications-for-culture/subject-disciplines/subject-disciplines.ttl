    skos:prefLabel "theoretical philosophy"@en,
        "filosofia teoretica "@it ;
    skos:relatedMatch <http://thes.bncf.firenze.sbn.it/termine.php?id=9655&menuR=1&menuS=2> ;
    clvapit:hasRankOrder "2" .

<https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/020-002> a skos:Concept ;
