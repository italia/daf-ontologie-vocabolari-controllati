    skos:prefLabel "mathematical analysis"@en,
        "analisi matematica"@it ;
    skos:relatedMatch <http://thes.bncf.firenze.sbn.it/termine.php?id=2599&menuR=2&menuS=2> ;
    clvapit:hasRankOrder "2" .

<https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/001-006> a skos:Concept ;
