    skos:prefLabel "greek history"@en,
        "storia greca"@it ;
    clvapit:hasRankOrder "2" .

<https://w3id.org/italia/controlled-vocabulary/classifications-for-culture/subject-disciplines/013-003> a skos:Concept ;
