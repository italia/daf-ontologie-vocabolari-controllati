        <rdfs:label xml:lang="en">Email</rdfs:label>
        <rdfs:label xml:lang="it">Email</rdfs:label>
        <prov:wasInfluencedBy rdf:resource="http://www.w3.org/2006/vcard/ns#Email"></prov:wasInfluencedBy>
    </owl:Class>



