        <rdfs:label xml:lang="en">contact point available</rdfs:label>
        <rdfs:label xml:lang="it">punto di contatto disponibile</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



