    <!-- https://w3id.org/italia/onto/SM#hasReview -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/SM/hasReview">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"></rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/SM/Review"></rdfs:range>
        <rdfs:comment xml:lang="en">Property that links a thing of the modelled domain to a review</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega qualsiasi cosa del dominio modellato a una recensione</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/SM"></rdfs:isDefinedBy>
        <rdfs:label xml:lang="it">ha recensione</rdfs:label>
        <rdfs:label xml:lang="en">has review</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/italia/onto/SM/available -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/SM/available">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/SM/ContactPoint"></rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/AccessCondition/AccessCondition"></rdfs:range>
        <rdfs:comment xml:lang="en">Property used to represent the availability of a contact point. It is through this property that is possibile to express opening hours of a specific contact point</rdfs:comment>
