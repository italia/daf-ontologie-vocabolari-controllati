        <rdfs:label xml:lang="it">Telefono</rdfs:label>
        <rdfs:subClassOf rdf:resource="&l0;Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/smapit/hasTelephoneType"/>
                <owl:onClass rdf:resource="http://dati.gov.it/onto/smapit/TelephoneType"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/smapit/isTelephoneOf"/>
