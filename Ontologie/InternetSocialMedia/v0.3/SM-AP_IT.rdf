                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A post on a social media.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un post su un social media.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns#Post"/>
