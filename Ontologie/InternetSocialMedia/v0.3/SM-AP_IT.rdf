    <!-- http://dati.gov.it/onto/smapit/hasCertifiedEmail -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasCertifiedEmail">
        <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/smapit/hasEmail"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasCreator -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasCreator">
        <rdfs:label xml:lang="en">has creator</rdfs:label>
        <rdfs:label xml:lang="it">ha autore</rdfs:label>
        <rdfs:comment xml:lang="en">This property states that a post has been created by a certain user account.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà asserisce che un post è stato creato da un certo account utente.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isCreatorOf"/>
        <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasEmail -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasEmail">
        <rdfs:label xml:lang="en">has email</rdfs:label>
        <rdfs:label xml:lang="it">ha email</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an online contact  point with one or more email addresses of various types.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà collega un punto di contatto online con uno o più indirizzi email di vario tipo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isEmailOf"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasEmailType -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasEmailType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has email type</rdfs:label>
        <rdfs:label xml:lang="it">ha tipo email</rdfs:label>
        <rdfs:comment xml:lang="en">This property links the Email to its type.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà collega l&apos;email al suo tipo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/EmailType"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isEmailTypeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasImage -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasImage">
        <rdfs:label xml:lang="en">has image</rdfs:label>
        <rdfs:label xml:lang="it">ha immagine</rdfs:label>
        <rdfs:comment xml:lang="en">This property links anything to an image.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà collega qualsiasi cosa ad un&apos;immagine.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Image"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isImageOf"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasOnlineContactPoint -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasOnlineContactPoint">
        <rdfs:label xml:lang="en">has online contact point</rdfs:label>
        <rdfs:label xml:lang="it">ha punto di contatto online</rdfs:label>
        <rdfs:comment xml:lang="en">This property links any thing of the domain being modelled with the online contact point.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà lega qualsiasi cosa del dominio modellato a un punto di contatto online.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isOnlineContactPointOf"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasReply -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasReply">
        <rdfs:label xml:lang="en">has reply</rdfs:label>
        <rdfs:label xml:lang="it">ha risposta</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an Item to other follow-up content Items made on the same topic. Use of this property can result in a single branch of replies from the original Item, or in a tree-like structure with branches forking at different Items along the way.</rdfs:comment>
        <rdfs:comment xml:lang="it">La proprietà &apos;ha risposta&apos; permette di dire che un post ha una risposta, ovvero, permette di creare thread di post ed organizzarla in una struttura ad albero.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isReplyOf"/>
        <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns#has_reply"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasTelephone -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasTelephone">
        <rdfs:label xml:lang="en">has telephone</rdfs:label>
        <rdfs:label xml:lang="it">ha telefono</rdfs:label>
        <rdfs:comment xml:lang="en">This property links the online contact point with the telephone element</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega un punto di contatto online con il telefono.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Telephone"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isTelephoneOf"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasTelephoneType -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasTelephoneType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has telephone type</rdfs:label>
        <rdfs:label xml:lang="it">ha tipo di telefono</rdfs:label>
        <rdfs:comment xml:lang="en">This property links the Telephone to its type (voice, fax)</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà lega un telefono al suo tipo (voce, fax).</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Telephone"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/TelephoneType"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isTelephoneTypeOf"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasUserAccount -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasUserAccount">
        <rdfs:label xml:lang="en">has user account</rdfs:label>
        <rdfs:label xml:lang="it">ha account utente</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an online contactPoint to its user account.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà collega un punto di contatto online al suo account utente.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isUserAccountOf"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasWebSite -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasWebSite">
        <rdfs:label xml:lang="en">has Web site</rdfs:label>
        <rdfs:label xml:lang="it">ha pagina Web</rdfs:label>
        <rdfs:comment xml:lang="en">Relation between anything and its web site.</rdfs:comment>
        <rdfs:comment xml:lang="it">Relazione tra una qualsiasi entità e la sua pagina Web.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/WebSite"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isWebSiteOf"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isAccountIssuedBy -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isAccountIssuedBy">
        <rdfs:label xml:lang="en">is accunt issued by</rdfs:label>
        <rdfs:label xml:lang="it">è account rilasciato da</rdfs:label>
        <rdfs:comment xml:lang="en">This property represents the relation between a social media and the user account issued by such a social media.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione tra un social media e gli account utenti che sono stati rilasciati su tale media.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/SocialMedia"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/issuesAccount"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isCreatorOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isCreatorOf">
        <rdfs:label xml:lang="en">is creator of</rdfs:label>
        <rdfs:label xml:lang="it">è autore di</rdfs:label>
        <rdfs:comment xml:lang="en">The fact that a user account creates a post.</rdfs:comment>
        <rdfs:comment xml:lang="it">La relazione che permette di asserire che un account utente ha creato un certo post.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
        <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isEmailOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isEmailOf">
        <rdfs:label xml:lang="en">is email of</rdfs:label>
        <rdfs:label xml:lang="it">è email di</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse Property of &quot;has email&quot;.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha email&quot;.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isEmailTypeOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isEmailTypeOf">
        <rdfs:label xml:lang="en">is email type of</rdfs:label>
        <rdfs:label xml:lang="it">è tipo di email di</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of has email type. Hence, this property links an email type to an email.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &apos;ha tipo di email&apos;. Di conseguenza, questa proprietà collega un individuo della classe &apos;Tipo di email&apos; ad un individuo della classe &apos;Email&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/EmailType"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isImageOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isImageOf">
        <rdfs:label xml:lang="en">is image of</rdfs:label>
        <rdfs:label xml:lang="it">è immagine di</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an image to anything. It is defined as the inverse property of &apos;has image&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;immagine a qualsiasi cosa. E&apos; la proprietà inversa di &apos;ha immagine&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Image"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isOnlineContactPointOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isOnlineContactPointOf">
        <rdfs:label xml:lang="en">is online contact point of</rdfs:label>
        <rdfs:label xml:lang="it">è punto di contatto online di</rdfs:label>
        <rdfs:comment xml:lang="en">This property is the inverse one for &quot;has online contact point&quot; and links an online contact point to any thing of the modelled domain.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;inversa di &quot;ha punto di contatto online&quot; e collega un punto di contatto online con qualsiasi cosa del dominio modellato.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isReplyOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isReplyOf">
        <rdfs:label xml:lang="en">is reply of</rdfs:label>
        <rdfs:label xml:lang="it">è risposta di</rdfs:label>
        <rdfs:comment xml:lang="en">The property &apos;is reply of&apos; asserts that a given post is the answer to another post.</rdfs:comment>
        <rdfs:comment xml:lang="it">La proprietà &apos;è risposta di&apos; permette di asserire che un certo post è la risposta ad un altro post.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
        <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns#reply_of"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isTelephoneOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isTelephoneOf">
        <rdfs:label xml:lang="en">is telephone of</rdfs:label>
        <rdfs:label xml:lang="it">è telefono di</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has telephone&quot;.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha telefono&quot;</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Telephone"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isTelephoneTypeOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isTelephoneTypeOf">
        <rdfs:label xml:lang="en">is telephone type of</rdfs:label>
        <rdfs:label xml:lang="it">è tipo di telefono di</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has telephone type&quot;.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha tipo di telefono&quot;.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Telephone"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/TelephoneType"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isUserAccountOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isUserAccountOf">
        <rdfs:label xml:lang="en">is user account of</rdfs:label>
        <rdfs:label xml:lang="it">è account utente di</rdfs:label>
        <rdfs:comment xml:lang="en">This property links a user account to its owner, which, in turn, is an agent. This property is the inverse of &apos;has user account&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa proprietà collega un account utente al suo proprietario, che è un istanza di agente. E&apos; la proprietà inversa di &apos;ha account utente&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/isWebSiteOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/isWebSiteOf">
        <rdfs:label xml:lang="en">is Web site of</rdfs:label>
        <rdfs:label xml:lang="it">è sito Web di</rdfs:label>
        <rdfs:comment xml:lang="en">Relation between a Web site and the entity this Web site is associated with. This property is the inverse of &apos;has Web site&apos;.</rdfs:comment>
