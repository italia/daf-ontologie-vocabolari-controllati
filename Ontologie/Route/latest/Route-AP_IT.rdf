        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <prov:wasInfluencedBy rdf:resource="http://vocab.gtfs.org/terms#RouteType"/>
        <l0:controlledVocabulary rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-routes/route-types"/>
