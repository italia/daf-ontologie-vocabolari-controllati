                                                              owl:onProperty clv:long ;
                                                              owl:cardinality "1"^^xsd:nonNegativeInteger
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf clv:Geometry .


