           rdfs:label "Route Type"@en ,
                      "Tipo di percorso"@it ;
           owl:versionInfo "stabile"@it ,
                           "stable"@en ;
           prov:wasInfluencedBy <http://vocab.gtfs.org/terms#RouteType> ;
           l0:controlledVocabulary <https://w3id.org/italia/controlled-vocabulary/classifications-for-routes/route-types> .


