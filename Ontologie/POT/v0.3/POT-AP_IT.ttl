                                               owl:versionInfo "stabile"@it ;

                                               rdfs:isDefinedBy <https://w3id.org/italia/onto/POT> ;

                                               rdfs:range <https://w3id.org/italia/onto/POT/Currency> ;

                                               rdfs:domain <https://w3id.org/italia/onto/POT/PriceSpecification> .



