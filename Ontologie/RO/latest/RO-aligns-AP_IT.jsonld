        {
          "@id": "http://www.ontologydesignpatterns.org/ont/d0.owl"
        },
        {
          "@id": "http://www.w3.org/ns/org#"
        }
      ],
      "owl:versionIRI": {
        "@id": "https://w3id.org/italia/onto/RO-aligns/0.2"
      }
    },
    {
