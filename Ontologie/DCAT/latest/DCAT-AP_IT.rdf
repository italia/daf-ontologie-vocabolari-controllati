  <rdf:Description rdf:about="http://www.w3.org/2006/vcard/ns#hasEmail">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/dcatapitOrganization"/>
