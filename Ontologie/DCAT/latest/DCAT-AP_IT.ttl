                                       )
                         ] ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:unionOf ( xsd:date
                                        xsd:dateTime
                                      )
                        ] ;
             rdfs:comment "Questa proprietà è utilizzata per rappresntare la data ufficiale di rilascio (es. pubblicazione) del Catalogo o del Dataset."@it ,
                          "This property contains the date of formal issuance (e.g., publication) of either the Catalogue or the Dataset,"@en ;
             rdfs:isDefinedBy terms: ;
             rdfs:label "data di rilascio"@it ,
                        "release date"@en .


###  http://purl.org/dc/terms/modified
terms:modified rdf:type owl:DatatypeProperty ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:unionOf ( xsd:date
                                          xsd:dateTime
                                        )
                          ] .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:DatatypeProperty .


###  http://schema.org/endDate
<http://schema.org/endDate> rdf:type owl:DatatypeProperty ;
