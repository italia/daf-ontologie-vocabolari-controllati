@prefix : <http://dati.gov.it/onto/dcatapit#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@base <http://dati.gov.it/onto/dcatapit> .

<http://dati.gov.it/onto/dcatapit> rdf:type owl:Ontology ;
                                    owl:versionIRI <http://dati.gov.it/onto/dcatapit/2.0> ;
                                    dct:description "This is version 2.0 (2016-03-18) of the ontology of the Italian application profile for the metadata that describe catalogues and data of  Italian Public Administrations (DCAT-AP_IT)."@en ;
                                    dct:rights "Attribuzione - http://creativecommons.org/licenses/by/3.0/it/legalcode"@it ;
                                    dct:title "DCAT-AP IT - Ontologia del profilo nazionale  dei metadati dei dati della PA"@it ;
                                    rdfs:label "DCAT-AP IT - Ontologia del profilo nazionale  dei metadati dei dati della PA"@it , "DCAT-AP IT - The Italian application profile for metadata of dataset"@en ;
                                    dct:description <http://www.dati.gov.it/sites/default/files/DCAT-AP_IT_UML.png> ;
                                    dct:creator "AgID - Agenzia per l'Italia Digitale" ;
                                    rdfs:comment "Questa è la versione 2.0 dell'ontologia  - Ultima modifica del 18 marzo 2016. L'ontologia è definita sulla base del profilo DCAT-AP 1.1 rilasciato dalla Commissione Europea, nel contesto del programma ISA, lo scorso Novembre 2015."@it ;
                                    dct:title "DCAT-AP IT - The Italian application profile for metadata of datasets"@en ;
                                    dct:license <http://creativecommons.org/licenses/by/3.0/it/> ;
                                    dct:modified "2016-03-18" ;
                                    rdfs:comment "This is 2.0 version  of the ontology - last update on 18th March 2016. The ontology is built upon DCAT-AP version 1.1 released by the European Commission, in the context of the ISA programme, on November 2015."@en ;
                                    dct:description "Versione 2.0 del 18-03-2016 dell'ontologia del Profilo italiano dei metadati che descrivono dati, e cataloghi di dati, delle pubbliche amministrazioni (DCAT-AP_IT)."@it ;
                                    dct:contributor "Working Group on National metadata profile DCAT-AP IT - Regione Piemonte, Provincia Autonoma di Trento, CISIS, Protezione Civile and ISTAT"@en ,
                                                    "Gruppo di lavoro su Profilo Nazionale Metadati - Regione Piemonte, Provincia autonoma di Trento, CISIS, Protezione Civile e Istat"@it ;
                                    dct:rights "Attribution - http://creativecommons.org/licenses/by/3.0/it/legalcode"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/author
dct:author rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/contributor
dct:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dct:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
terms:description rdf:type owl:AnnotationProperty ;
                  rdfs:range rdf:PlainLiteral ;
                  rdfs:domain owl:Thing .


###  http://purl.org/dc/terms/modified
terms:modified rdf:type owl:AnnotationProperty ;
               rdfs:domain _:genid13 ,
                           _:genid147 ,
                           _:genid16 ,
                           _:genid20 ,
                           _:genid238 .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty ;
            rdfs:range rdf:PlainLiteral ;
            rdfs:domain owl:Thing .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#subClassOf
rdfs:subClassOf rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty ;
                rdfs:range rdfs:Literal ;
                rdfs:domain owl:Thing .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral
rdf:PlainLiteral rdf:type rdfs:Datatype .


###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#dateTime
xsd:dateTime rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#decimal
xsd:decimal rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/accrualPeriodicity
terms:accrualPeriodicity rdf:type owl:ObjectProperty ;
                         rdfs:domain :Dataset ;
                         rdfs:range terms:Frequency ;
                         rdfs:comment "Property used to represent the frequancy of update of the dataset."@en ,
                                      "Proprietà utilizzata per rappresentare la frequenza dcon cui il Dataset viene aggiornato. I valori da utilizzare per questa proprietà sono gli URI dei concetti del vocabolario MDR Frequency Named Authority List (http://publications.europa.eu/mdr/authority/frequency)."@it ;
                         rdfs:isDefinedBy terms: ;
                         rdfs:label "accrual periodicity"@en ,
                                    "frequenza di aggiornamento"@it .


###  http://purl.org/dc/terms/conformsTo
terms:conformsTo rdf:type owl:ObjectProperty ;
                 rdfs:domain :Dataset ;
                 rdfs:range :Standard ;
                 rdfs:comment "Property used to link a dataset to the standard to which the dataset conforms."@en ,
                              "Questa proprietà si riferisce a una regola implementativa o altra specifica a cui il Dataset è conforme."@t ;
                 rdfs:isDefinedBy terms: ;
                 rdfs:label "conformità"@it ,
                            "conforms to"@en .


###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:ObjectProperty ;
              rdfs:domain :Dataset ;
              rdfs:range :Agent ;
              rdfs:comment "Property used to represent the creator of the dataset; that is, the authority responsible for creating/making the dataset. Avoid the use of names of single persons."@en ,
                           "Proprietà utilizzata per rappresentare una o più entità (organizzazione/i) che hanno materialmente prodotto il Dataset. Evitare di indicare nomi di singole persone."@it ;
              rdfs:isDefinedBy terms: ;
              rdfs:label "autore"@it ,
                         "creator"@en .


###  http://purl.org/dc/terms/format
terms:format rdf:type owl:ObjectProperty ;
             rdfs:domain :Distribution ;
             rdfs:range terms:MediaTypeOrExtent ;
             rdfs:comment "Property used to represent the format of the distribution of the dataset. In order to represent the format it is required the use of the controlled vocabulary available at http://publications.europa.eu/mdr/authority/file-type/."@en ,
                          "Proprietà utilizzata per rappresentare il formato del file di distribuzione del dataset. Per i formati si richiede l'uso del vocabolario controllato disponibile in http://publications.europa.eu/mdr/authority/file-type/."@it ;
             rdfs:isDefinedBy terms: ;
             rdfs:label "format"@en ,
                        "formato"@it .


###  http://purl.org/dc/terms/isVersionOf
terms:isVersionOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :Dataset ;
                  rdfs:range :Dataset ;
                  rdfs:comment "Questa proprietà si riferisce a un Dataset correlato che è una versione, un’edizione o un adattamento del Dataset descritto."@it ,
                               "This property refers to a related Dataset of which the described Dataset is a version, edition, or adaptation."@en ;
                  rdfs:isDefinedBy terms: ;
                  rdfs:label "is version of"@en ,
                             "è versione di"@it .


###  http://purl.org/dc/terms/language
terms:language rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Dataset
                                           <https://dati.gov.it/onto/dcatapit#Catalog>
                                         )
                           ] ;
               rdfs:range terms:LinguisticSystem ;
               rdfs:comment """Questa proprietà si riferisce a una lingua utilizzata nei metadati testuali che descrivono i titoli, le descrizioni, … dei Dataset o del Catalogo. Questa proprietà può essere ripetuta se i metadati sono forniti in più lingue.
Deve essere utilizzato il vocabolario http://publications.europa.eu/mdr/authority/language/"""@it ,
                            "This property refers to a language of the Dataset or of the Catalogue. This property can be repeated if there are multiple languages in the Dataset or in the Catalogue."@en ;
               rdfs:isDefinedBy terms: ;
               rdfs:label "language"@en ,
                          "lingua"@it .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:ObjectProperty ;
              rdfs:domain :Distribution ;
              rdfs:range :LicenseDocument ;
              rdfs:comment terms: ,
                           "Questa proprietà si riferisce a una licenza con la quale la Distribuzione è resa disponibile."@it ,
                           "This property refers to the licence under which the distribution of the dataset is made available."@en ;
              rdfs:label "license"@en ,
                         "licenza"@it .


###  http://purl.org/dc/terms/publisher
terms:publisher rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Dataset
                                            <https://dati.gov.it/onto/dcatapit#Catalog>
                                          )
                            ] ;
                rdfs:range :Agent ;
                rdfs:comment "Questa proprietà è utilizzata per rappresentare un agente (tipicamente un'organizzazione) che è responsabile di rendere disponibile (pubblicare) il dataset o il catalogo. Evitare di specificare nomi di singole persone."@it ,
                             "This property is used to represent an agent (typically an organization) which is responsible for making available (for publishing) the dataset or the catalogue. Avoid the use of names of single persons."@en ;
                rdfs:isDefinedBy terms: ;
                rdfs:label "editore"@it ,
                           "publisher"@en .


###  http://purl.org/dc/terms/rightsHolder
terms:rightsHolder rdf:type owl:ObjectProperty ;
                   rdfs:domain :Dataset ;
                   rdfs:range :Agent ;
                   rdfs:comment "Sulla base anche di quanto indicato all’art.2 lettera i) del D. Lgs. n. 36/2006, il titolare del dataset è la pubblica amministrazione o l’organismo di diritto pubblico che ha originariamente formato per uso proprio o commissionato ad altro soggetto pubblico o privato il documento che rappresenta il dato, o che ne ha la disponibilità. Il titolare è pertanto responsabile della gestione complessiva del dataset in virtù dei propri compiti istituzionali.Si fa presente che, nell’ambito della presente ontologia, l’accezione di documento suddetta può essere intesa riferita al dataset. E' bene evitare di specificare nomi di singole persone."@it ,
                                "This property is used to represent an agent (typically an organization) which is responsible for the overall management of the dataset in the light of its institutional objectives. This property represents one of the extensions introduced by the Italian profile of the DCaT-AP 1.1. Do not include names of single persons."@en ;
                   rdfs:isDefinedBy <http://purl.org/dc/terms//> ;
                   rdfs:label "rights holder"@en ,
                              "titolare"@it .


###  http://purl.org/dc/terms/spatial
terms:spatial rdf:type owl:ObjectProperty ;
              rdfs:domain :Dataset ;
              rdfs:range terms:Location ;
              rdfs:comment "Questa proprietà è utilizzata per rappresentare una zona geografica (copertura spaziale) del dataset."@it ,
                           "This property refers to a geographical area covered by the Dataset"@en ;
              rdfs:isDefinedBy terms: ;
              rdfs:label "copertura geografica"@it ,
                         "spatial coverage"@en .


###  http://purl.org/dc/terms/subject
terms:subject rdf:type owl:ObjectProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range skos:Concept ;
              rdfs:comment """Questa proprietà è definita in http://purl.org/dc/terms/ ed è utilizzata in DCAT-AP_IT per rappresentare delle parole chiave controllate per i dataset, ovvero una classificazione più dettagliata del tema di riferimento dei dati. Questa proprietà rappresenta una delle estensioni introdotte dal profilo italiano DCAT-AP IT. Un Dataset può essere associato a più sottotemi.
I valori da utilizzare per questa proprietà sono gli URI dei concetti del vocabolario Eurovoc (http://eurovoc.europa.eu/drupal/?q=it/navigation&cl=it). Il mapping tra i temi del dataset e Eurovoc è disponibile presso il portale europeo dei dati. Questa proprietà è fortemente raccomandata."""@it ,
                           "This property is defined in http://purl.org/dc/terms/ and it is used in DCAT-AP_IT to represent controlled keywords (a fine-grained classification of the theme) for a dataset. This property represents one of the Italian externsions of DCAT-AP. A dataset can have more than one subject. The values are the URI of the controlled vocabulary Eurovoc (http://eurovoc.europa.eu/drupal/?q=it/navigation&cl=en). The mapping between the dataset themes and Eurovoc is available through the european data poral. This property is strongly recommended."@en ;
              rdfs:isDefinedBy terms: ;
              rdfs:label "sottotema"@it ,
                         "subtheme"@en .


###  http://purl.org/dc/terms/temporal
terms:temporal rdf:type owl:ObjectProperty ;
               rdfs:domain :Dataset ;
               rdfs:range terms:PeriodOfTime ;
               rdfs:comment "Questa proprietà è utilizzata per rappresentare un'estensione temporale che il dataset possiede."@it ,
                            "This property refers to a temporal period that the Dataset covers."@en ;
               rdfs:isDefinedBy terms: ;
               rdfs:label "estensione temporale"@it ,
                          "temporal coverage"@en .


###  http://purl.org/dc/terms/type
terms:type rdf:type owl:ObjectProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range skos:Concept ;
           terms:description "Questa proprietà è definita in http://purl.org/dc/terms/ ed è aggiunta a DCAT-AP_IT al fine di modellare i tipi delle \"cose\". In particolare, è utilizzata per rappresentare il tipo di licenza di una distribuzione di un dataset. Il vocabolario che deve essere utilizzato per specificare il tipo della licenza è il seguente: http://purl.org/adms/licencetype/1.0"@it ,
                             "This property is defined in http://purl.org/dc/terms/ and it is added in DCAT-AP_IT to model the type of \"things\". In particular, it is used to specify the type of license of a distribution of a dataset. The vocabulary to be used to specify the type of license is the following: http://purl.org/adms/licencetype/1.0"@en ;
           rdfs:isDefinedBy terms: ;
           rdfs:label "tipo"@it ,
                      "type"@en .


###  http://sdati.gov.it/onto/dcatapit#startDate
<http://sdati.gov.it/onto/dcatapit#startDate> rdf:type owl:ObjectProperty ;
                                              rdfs:domain terms:PeriodOfTime ;
                                              rdfs:range [ rdf:type owl:Class ;
                                                           owl:unionOf ( xsd:date
                                                                         xsd:dateTime
                                                                       )
                                                         ] .


###  http://www.w3.org/2006/vcard/ns#hasEmail
vcard:hasEmail rdf:type owl:ObjectProperty ;
               rdfs:domain :Organization ;
               rdfs:range vcard:Email ;
               rdfs:comment "Proprietà utilizzata per specificare l'indirizzo email primario da utilizzare nelle comunicazioni con l'oggetto (nel contesto DCAT_AP IT l'oggetto è il dipartimento/ufficio identificato come punto di contatto stabile per un dataset).  Si raccomanda di utilizzare e-mail istituzionali riferibili all’ufficio/servizio/UO e non alla singola persona."@it ,
                            "To specify the primary electronic mail address for communications with the contact point of the dataset."@en ;
               rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
               rdfs:label "ha email"@it ,
                          "has email"@en .


###  http://www.w3.org/2006/vcard/ns#hasTelephone
vcard:hasTelephone rdf:type owl:ObjectProperty ;
                   rdfs:domain :Organization ;
                   rdfs:range vcard:TelephoneType ;
                   rdfs:comment "Questa proprietà è definita in http://www.w3.org/2006/vcard/ns ed è stata aggiunta al fine di poter legare l'Organizzazione, che rappresenta il punto di contatto, con il numero di telefono della stessa."@it ,
                                "This property is defined in http://www.w3.org/2006/vcard/ns and it is used to link the Organization representing the contact point to its phone number."@en ;
                   rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
                   rdfs:label "ha telefono"@it ,
                              "has telephone"@en .


###  http://www.w3.org/2006/vcard/ns#hasURL
vcard:hasURL rdf:type owl:ObjectProperty ;
             rdfs:domain :Organization ;
             rdfs:range owl:Thing ;
             terms:description "Property defined in http://www.w3.org/2006/vcard/ns and used to represent the primary URL of the contact point of the dataset. It is recommended to indicate the URL of the main contact page or the contact form. It is discouraged the use of the main URL of the institutional web site of the organization that provides the dataset. In case additional secondary URLs are to be added, it is recommended to extend DCAT-AP_IT with properties such as foaf:page."@en ,
                               "Proprietà utilizzata per rappresentare l'URL primario del punto di contatto per il dataset. Si raccomanda di indicare l'URL di una pagina primaria di contatti o di un form per il contatto e non l'URL del sito web istituzionale dell'organizzazione che fornisce il dataset. Nel caso si vogliano specificare ulteriori URL secondari si può estendere il profilo DCAT-AP_IT con proprietà come per esempio foaf:page."@it ;
             rdfs:isDefinedBy <http://www.w3.org/2006/vcard/ns> ;
             rdfs:label "sito"@it ,
                        "web site"@en .


###  http://www.w3.org/ns/adms#identifier
adms:identifier rdf:type owl:ObjectProperty ;
                rdfs:domain :Dataset ;
                rdfs:range adms:Identifier ;
                rdfs:comment "Questa proprietà si riferisce a un eventuale identificativo secondario del Dataset (un identificativo che il dataset assume in altri contesti come per esempio il DOI, l'ISBN, ecc.)."@it ,
                             "This property is used to represent a secondary identifier of the dataset such a DOI, ISBN, etc."@en ;
                rdfs:isDefinedBy adms: ;
                rdfs:label "altro identificativo"@it ,
                           "other identifier"@en .


###  http://www.w3.org/ns/dcat#accessURL
dcat:accessURL rdf:type owl:ObjectProperty ;
               rdfs:domain :Distribution ;
               rdfs:range rdfs:Resource ;
               rdfs:comment "Questa proprietà contiene un URL tramite cui si può accedere alla Distribuzione del Dataset.  La risorsa all'URL di accesso potrebbe contenere informazioni su come ottenere il dataset."@it ,
                            "This property contains a URL that gives access to a Distribution of the Dataset. The resource at the access URL may contain information about how to get the Dataset."@en ;
               rdfs:isDefinedBy dcat: ;
               rdfs:label "URL di accesso"@it ,
                          "access URL"@en .


###  http://www.w3.org/ns/dcat#contactPoint
dcat:contactPoint rdf:type owl:ObjectProperty ;
                  rdfs:domain :Dataset ;
                  rdfs:range vcard:Kind ;
                  rdfs:comment "Questa proprietà è utilizzata per rappresentare informazioni di contatto di un dataset. Tali informazioni possono essere usate per abilitare l'invio di commenti o richieste a coloro che gestiscono il dataset."@it ,
                               "This property is used to represent contact information that can be used for sending comments about the dataset."@en ;
                  rdfs:isDefinedBy dcat: ;
                  rdfs:label "contact point"@en ,
                             "punto di contatto"@it .


###  http://www.w3.org/ns/dcat#dataset
dcat:dataset rdf:type owl:ObjectProperty ;
             rdfs:domain <https://dati.gov.it/onto/dcatapit#Catalog> ;
             rdfs:range :Dataset ;
             rdfs:comment "Property used to link the catalogue with the dataset that is included in the Catalogue."@en ,
                          "Proprietà utilizzata per collegare il catalogo ai dataset che sono inclusi nel catalogo stesso"@it ;
             rdfs:isDefinedBy dcat: ;
             rdfs:label "dataset"@en ,
                        "dataset"@it ;
             owl:versionInfo "1.2"@en .


###  http://www.w3.org/ns/dcat#distribution
dcat:distribution rdf:type owl:ObjectProperty ;
                  rdfs:domain :Dataset ;
                  rdfs:range :Distribution ;
                  rdfs:comment "Property used to link the dataset with its available distributions"@en ,
                               "Proprietà utilizzata per collegare il dataset con eventuali distribuzioni disponibili"@it ;
                  rdfs:isDefinedBy dcat: ;
                  rdfs:label "distribution"@en ,
                             "distribuzione"@it .


###  http://www.w3.org/ns/dcat#downloadURL
dcat:downloadURL rdf:type owl:ObjectProperty ;
                 rdfs:domain :Distribution ;
                 rdfs:range rdfs:Resource ;
                 rdfs:comment "Questa proprietà è utilizzata per rappresentare l'URL che fornisce un link diretto al file scaricabile in un determinato formato."@it ,
                              "This property is used to represent a URL that is a direct link to a downloadable file in a given format."@en ;
                 rdfs:isDefinedBy dcat: ;
                 rdfs:label "URL di download"@it ,
                            "download URL"@en ;
                 owl:versionInfo "1.2"@en ,
                                 "1.2"@it .


###  http://www.w3.org/ns/dcat#landingPage
dcat:landingPage rdf:type owl:ObjectProperty ;
                 rdfs:domain :Dataset ;
                 rdfs:range foaf:Document ;
                 rdfs:comment "Questa proprietà si riferisce a una pagina web, riferibile al fornitore e non ad una terza parte, che fornisce l’accesso al Dataset, le sue Distribuzioni e/o informazioni aggiuntive."@it ,
                              "This property refers to a web page that provides access to the Dataset, its Distributions and/or additional information."@en ;
                 rdfs:isDefinedBy dcat: ;
                 rdfs:label "landing page"@en ,
                            "pagina di accesso"@it .


###  http://www.w3.org/ns/dcat#theme
dcat:theme rdf:type owl:ObjectProperty ;
           rdfs:domain :Dataset ;
           rdfs:range skos:Concept ;
           rdfs:comment "Questa proprietà è utilizzata per rappresentare il tema che classifica il dataset. Un dataset può essere associato a temi multipli. I valori da utilizzare per questa proprietà sono gli URI dei concetti del vocabolario EU Data Theme (URI vocabolario: http://publications.europa.eu/resource/authority/data-theme) descritti alla pagina http://publications.europa.eu/mdr/authority/data-theme."@it ,
                        "This property refers to a category of the Dataset. A Dataset may be associated with multiple themes."@en ;
           rdfs:isDefinedBy dcat: ;
           rdfs:label "tema"@it ,
                      "theme"@en .


###  http://www.w3.org/ns/dcat#themeTaxonomy
dcat:themeTaxonomy rdf:type owl:ObjectProperty ;
                   rdfs:domain <https://dati.gov.it/onto/dcatapit#Catalog> ;
                   rdfs:range skos:ConceptScheme ;
                   rdfs:comment "Questa proprietà è utilizzata per rappresentare un sistema di organizzazione della conoscenza utile per la classificazione dei dataset contenuti nel catalogo."@it ,
                                "This property refers to a knowledge organization system used to categorized the Catalogue's Datasets."@en ;
                   rdfs:isDefinedBy dcat: ;
                   rdfs:label "tema del catalogo"@it ,
                              "theme taxonomy"@en .


###  http://www.w3.org/ns/locn#geographicName
<http://www.w3.org/ns/locn#geographicName> rdf:type owl:ObjectProperty ;
                                           rdfs:domain terms:Location ;
                                           rdfs:range skos:Concept ;
                                           rdfs:comment <http://www.w3.org/ns/locn> ,
                                                        "A geographic name is a proper noun applied to a spatial object. Examples include: \"Roma\" (n Italian), \"Rome\" (in English), etc."@en ,
                                                        "Un nome geografico è un nome proprio dato a un oggetto spaziale. Esempi includono: \"Roma\" (in italiano), \"Rome\" (in inglese), ecc. Utilizzare questa proprietà solo se non è disponibile un URI relativo alla localizzazione geografica da indicare utilizzando la proprietà geographicalIdentifier"@it ;
                                           rdfs:label "geographical name"@en ,
                                                      "nome geografico"@it .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:ObjectProperty ;
              rdfs:domain <https://dati.gov.it/onto/dcatapit#Catalog> ;
              rdfs:range foaf:Document ;
