foaf:name rdf:type owl:DatatypeProperty ;
          rdfs:domain owl:Thing ;
          rdfs:range rdfs:Literal ;
          rdfs:comment "Property used to represent a name of a Thing."@en ,
                       "Proprietà utilizzata per rappresentare il nome di una cosa."@it ;
          rdfs:isDefinedBy foaf: ;
          rdfs:label "name"@en ,
                     "nome"@it .


