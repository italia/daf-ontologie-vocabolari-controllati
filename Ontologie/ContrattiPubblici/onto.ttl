    rdfs:label "Data stimata di fine lavori"@it ;
    rdfs:label "Estimated End Date"@en ;
    rdfs:comment "The property that detemins the estimated end Date."@en ;
    rdfs:comment "La proprietà che determina la data  stimata di fine lavori."@en ;
    rdfs:range xsd:date .

  :lotEndDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:endTime ;
