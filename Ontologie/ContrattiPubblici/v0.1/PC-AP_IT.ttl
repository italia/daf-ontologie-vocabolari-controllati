@prefix : <http://w3id.org/italia/onto/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix l0: <http://dati.gov.it/onto/l0/> .
@prefix ti: <http://dati.gov.it/onto/tiapit> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix covapit: <http://dati.gov.it/onto/covapit/> .
@base <http://w3id.org/italia/onto/pcapit> .

<http://w3id.org/italia/onto/pcapit> rdf:type owl:Ontology ;
                                      owl:versionIRI : ;
                                      owl:imports covapit: ,
                                                  ti: ,
                                                  l0: ;
                                      rdfs:comment "This is the ontology of the Italian public contracts."@en ;
                                      rdfs:label "Ontologia dei contratti pubblici italiani"@it ;
                                      owl:versionInfo "unstable"@en ;
                                      dct:issued "2018-04-27"^^xsd:date ;
                                      rdfs:label "Italian public contracts ontology"@en ;
                                      owl:versionInfo "instabile"@it ;
                                      dct:license <https://creativecommons.org/licenses/by/4.0/> ;
                                      rdfs:isDefinedBy : ;
                                      dc:creator "CSI Piemonte"@en ,
                                                 "Synapta Srl"@en ;
                                      rdfs:comment "Questa è l'ontologia dei contratti pubblici italiani."@it ;
                                      owl:versionInfo "Versione 0.1 - 27 Aprile 2018"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://dati.gov.it/onto/l0/controlledVocabulary
l0:controlledVocabulary rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#float
xsd:float rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/italia/onto/actualizes
:actualizes rdf:type owl:ObjectProperty ;
            rdfs:domain :Procedure ;
            rdfs:range :Intervention ;
            rdfs:comment "La proprietà che lega una procedura di realizzazione a un intervento che realizza."@it ,
                         "The property that links the procedure to the intervention actualized by the procedure."@en ;
            rdfs:label "actualizes"@en ,
                       "realizza"@it .


###  http://w3id.org/italia/onto/approvedBy
:approvedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :PaymentCertificate ;
            rdfs:range :WorkProgressReport ;
            rdfs:comment "La proprietà che collega il certificato di pagamento al SAL (Stato Avanzamento Lavori) documento che contiene la delibera al pagamento stesso."@it ,
                         "The property that links the certificate of payment to the work progress report whereby the payment is approved."@en ;
            rdfs:label "approved by"@en ,
                       "deliberato in SAL"@it .


###  http://w3id.org/italia/onto/bidder
:bidder rdf:type owl:ObjectProperty ;
        rdfs:domain :Tender ;
        rdfs:range covapit:Organization ;
        rdfs:comment "La proprietà che indica l'organizzazione offerente di un'offerta."@it ,
                     "The property that links the tender to its bidder."@en ;
        rdfs:label "bidder"@en ,
                   "offerente"@it .


###  http://w3id.org/italia/onto/changedBy
:changedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasOriginalSummary ;
           rdfs:domain :EconomicSummary ;
           rdfs:range :ChangeEvent ;
           rdfs:comment "Questa proprietà è l'inversa di \"prospetto economico di origine\" e indica un evento che ha contribuito al cambiamento di questo prospetto economico."@it ,
                        "This property is the inverse of \"has original summary\" and determines the change event which resulted in a change to this economic summary."@en ;
           rdfs:label "cambiato da"@it ,
                      "changed by"@en .


###  http://w3id.org/italia/onto/hasAwardCriteria
:hasAwardCriteria rdf:type owl:ObjectProperty ;
                  rdfs:domain :Procedure ;
                  rdfs:range :AwardCriteria ;
                  rdfs:comment "La proprietà che determina il criterio di aggiudicazione della procedura di realizzazione. Tale proprietà si applica solo nel caso di procedure che prevedono gare con bando e conseguente aggiudicazione, e fa riferimento a un vocabolario controllato."@it ,
                               "The property that determines the award criteria of the procedure. This property should be used in the case of procedures that include public calls followed by an awarding."@en ;
                  rdfs:label "ha criterio di aggiudicazione"@it ,
                             "has award criteria"@en .


###  http://w3id.org/italia/onto/hasCPV
:hasCPV rdf:type owl:ObjectProperty ;
        rdfs:domain :Contract ,
                    :Intervention ,
                    :Notice ,
                    :Procedure ;
        rdfs:range :CPV ;
        rdfs:comment "La proprietà che associa un intervento, procedura di realizzazione, bando o contratto a un identificativo del vocabolario controllato dei CPV (Common Procurement Vocabulary), il sistema di classificazione per gli appalti pubblici."@it ,
                     "The property that links an intervention, procedure, notice or contract to the identifier within the controlled vocabulary of CPV (Common Procurement Vocabulary), a code describing the main object of a public procurement contract."@en ;
        rdfs:label "ha CPV"@it ,
                   "has CPV"@en .


###  http://w3id.org/italia/onto/hasChangeDocument
:hasChangeDocument rdf:type owl:ObjectProperty ;
                   rdfs:domain :Contract ,
                               :Intervention ,
                               :Procedure ,
                               :Project ;
                   rdfs:range :ModificationDocument ;
                   rdfs:comment "La proprietà che indica il documento che descrive una modifica alle caratteristiche di un Progetto, Intervento, Procedura di realizzazione o Contratto."@it ,
                                "The property that determines the document describing a change to the characteristics of a project, intervention, procedure or contract."@en ;
                   rdfs:label "ha documento di modifica"@it ,
                              "has change document"@en .


###  http://w3id.org/italia/onto/hasContract
:hasContract rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasDocument ;
             rdfs:domain :Procedure ;
             rdfs:range :Contract ;
             rdfs:comment "La proprietà che determina il legame tra la procedura di realizzazione e il relativo contratto."@it ,
                          "The property that determines the link between a contract and the relative procedure."@en ;
             rdfs:label "ha contratto"@it ,
                        "has contract"@en .


###  http://w3id.org/italia/onto/hasContractingAuthority
:hasContractingAuthority rdf:type owl:ObjectProperty ;
                         rdfs:domain :Procedure ;
                         rdfs:range covapit:Organization ;
                         rdfs:comment "La proprietà che indica l'istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto."@it ,
                                      "The property that determines the institution which issues a public contract, receives tenders and chooses a suitable supplier on the base of the conditions given by the contract."@en ;
                         rdfs:label "ha stazione appaltante"@it ,
                                    "has contracting authority"@en .


###  http://w3id.org/italia/onto/hasDocument
:hasDocument rdf:type owl:ObjectProperty ;
             rdfs:domain :Procedure ;
             rdfs:range :Document ;
             rdfs:comment "La proprietà che indica gli atti emessi dalla stazione appaltante nell'espletamento della procedura di realizzazione."@it ,
                          "The properties that links the procedure to the documents issued by the contracting authority during the course of the procedure."@en ;
             rdfs:label "ha atto"@it ,
                        "has document"@it .


###  http://w3id.org/italia/onto/hasEconomicSummary
:hasEconomicSummary rdf:type owl:ObjectProperty ;
                    rdfs:domain :Contract ,
                                :Intervention ,
                                :Procedure ,
                                :Project ;
                    rdfs:range :EconomicSummary ;
                    rdfs:comment "Questa proprietà indica il prospetto economico di un progetto, intervento, procedura di realizzazione, o contratto."@it ,
                                 "This property represents the economic summary of a project, intervention, procedure or contract."@en ;
                    rdfs:label "ha prospetto economico"@it ,
                               "has economic summary"@en .


###  http://w3id.org/italia/onto/hasFrameworkAgreement
:hasFrameworkAgreement rdf:type owl:ObjectProperty ;
                       rdfs:domain :Procedure ;
                       rdfs:range :FrameworkAgreement ;
                       rdfs:comment "La proprietà che lega un'istanza di procedura di realizzazione a un accordo quadro o convenzione."@it ,
                                    "The properties that links the procedure to its framework agreement."@en ;
                       rdfs:label "ha accordo quadro"@it ,
                                  "has framework agreement"@en .


###  http://w3id.org/italia/onto/hasNotice
:hasNotice rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasDocument ;
           rdfs:domain :Procedure ;
           rdfs:range :Notice ;
           rdfs:comment "La proprietà che indica il collegamento tra la procedura di realizzazione e un bando di gara o avviso. Diverse procedure di realizzazione possono fare riferimento ad un medesimo bando, allo stesso modo in cui, per quanto riguarda i dati applicativi SIMOG, più lotti (ciascuno dei quali corrisponde a una procedura di realizzazione) fanno riferimento allo stesso bando di gara."@it ,
                        "The property that determines links the procedure to the related announcement or notice. More procedure may be linked to the same announcement or notice."@en ;
           rdfs:label "ha bando"@it ,
                      "has notice"@en .


###  http://w3id.org/italia/onto/hasNoticeType
:hasNoticeType rdf:type owl:ObjectProperty ;
               rdfs:domain :Notice ;
               rdfs:range :NoticeType ;
               rdfs:comment "La proprietà che indica il tipo di bando."@it ,
                            "The property that links a notice to its type."@en ;
               rdfs:label "ha tipo di bando"@it ,
                          "has notice type"@en .


###  http://w3id.org/italia/onto/hasOriginalSummary
:hasOriginalSummary rdf:type owl:ObjectProperty ;
                    rdfs:domain :ChangeEvent ;
                    rdfs:range :EconomicSummary ;
                    rdfs:comment "Questa proprietà è l'inversa di \"cambiata da\" e collega un evento di cambiamento al prospetto economico precedente al cambiamento stesso."@it ,
                                 "This property is the inverse of \"changed by\" and links a change event to the economic summary preceding the change event."@en ;
                    rdfs:label "ha prospetto economico di origine"@it ,
                               "has original summary"@en .


###  http://w3id.org/italia/onto/hasOutcome
:hasOutcome rdf:type owl:ObjectProperty ;
            rdfs:domain :Procedure ;
            rdfs:range :ProcedureOutcome ;
            rdfs:comment "La proprietà che collega la procedura di realizzazione all'esito (aggiudicazione, affidamento o accordo-quadro) della procedura stessa."@it ,
                         "The property that links the procedure to its outcome (open tender award, direct award or framework agreement)."@en ;
            rdfs:label "ha esito"@it ,
                       "has outcome"@en .


###  http://w3id.org/italia/onto/hasPaymentCertificate
:hasPaymentCertificate rdf:type owl:ObjectProperty ;
                       rdfs:domain :Procedure ;
                       rdfs:range :PaymentCertificate ;
                       rdfs:comment "La proprietà che collega la procedura di realizzazione al certificato di pagamento deliberato in un SAL."@it ,
                                    "The property that links the procedure to the certificate of payment approved by the related progress of work report."@en ;
                       rdfs:label "ha certificato di pagamento"@it ,
                                  "has certificate of payment"@en .


###  http://w3id.org/italia/onto/hasProcedureType
:hasProcedureType rdf:type owl:ObjectProperty ;
                  rdfs:domain :Procedure ;
                  rdfs:range :ProcedureType ;
                  rdfs:comment "La proprietà che specifica i criteri di scelta di un contraente validi in una procedura di realizzazione."@it ,
                               "The property that links the procedure to its type."@en ;
                  rdfs:label "ha scelta contraente"@it ,
                             "has type of procedure"@en .


###  http://w3id.org/italia/onto/hasProject
:hasProject rdf:type owl:ObjectProperty ;
            rdfs:domain :Intervention ;
            rdfs:range :Project ;
            rdfs:comment "Questa proprietà è l'inversa di \"è progetto di\" e lega un intervento a un progetto."@it ,
                         "This property is the inverse of \"is project for\" and links an intervention to a project."@en ;
            rdfs:label "ha progetto"@it ,
                       "has project"@en .


###  http://w3id.org/italia/onto/hasResolution
:hasResolution rdf:type owl:ObjectProperty ;
               rdfs:domain :Contract ;
               rdfs:range :Resolution ;
               rdfs:comment "La proprietà che collega il contratto all'evento che determina la risoluzione del contatto stesso"@it ,
                            "The property that links the contract to the event that determines its resolution."@en ;
               rdfs:label "ha risoluzione"@it ,
                          "has resolution"@en .


###  http://w3id.org/italia/onto/hasResolutionToContract
:hasResolutionToContract rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :hasDocument ;
                         rdfs:domain :Procedure ;
                         rdfs:range :ResolutionToContract ;
                         rdfs:comment "La proprietà che determina il legame tra la procedura di realizzazione e una determina a contrarre relativa."@it ,
                                      "The property that links the procedure to the document whereby the contracting authority expresses its resoultion to contract."@en ;
                         rdfs:label "ha determina a contrarre"@it ,
                                    "has resolution to contract"@en .


###  http://w3id.org/italia/onto/hasResultingSummary
:hasResultingSummary rdf:type owl:ObjectProperty ;
                     owl:inverseOf :resultedFrom ;
                     rdfs:domain :ChangeEvent ;
                     rdfs:range :EconomicSummary ;
                     rdfs:comment "Questa proprietà è l'inversa di \"risultante da\" e indica un prospetto economico creato o ridefinito in seguito a un evento di cambiamento."@it ,
                                  "This property is the inverse of \"resulted from\" and determines the economic summary which was created or changed as a result of the change event."@en ;
                     rdfs:label "ha prospetto risultante"@it ,
                                "has resulting summary"@en .


###  http://w3id.org/italia/onto/hasWorkProgressReport
:hasWorkProgressReport rdf:type owl:ObjectProperty ;
                       rdfs:domain :Procedure ;
                       rdfs:range :WorkProgressReport ;
                       rdfs:comment "La proprietà che collega la procedura di realizzazione al documento che ne certifica lo Stato di Avanzamento Lavori (SAL)."@it ,
                                    "The property that links the procedure to its work progress report."@en ;
                       rdfs:label "ha SAL"@it ,
                                  "has work progress report"@en .


###  http://w3id.org/italia/onto/isPrimaryTopicOf
:isPrimaryTopicOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :ChangeEvent ,
                              :Resolution ;
                  rdfs:range :ModificationDocument ;
                  rdfs:comment "La proprietà che collega un evento (per esempio un evento di cambiamento del propetto economico, oppure un evento di risoluzione di un contratto) al documento di modifica che ne descrive le caratteristiche."@it ,
                               "The property that links an event (e.g., an event that modifies the economic summary of a procedure or the event that determines the resolution of a contract) to the document describing the characteristics of the event."@en ;
                  rdfs:label "is primary topic of"@en ,
                             "è argomento principale di"@it .


###  http://w3id.org/italia/onto/isProjectFor
:isProjectFor rdf:type owl:ObjectProperty ;
              rdfs:domain :Project ;
              rdfs:range :Intervention ;
              rdfs:comment "Questa proprietà è l'inversa di \"ha progetto\" e lega un progetto a un intervento."@it ,
                           "This property is the inverse of \"has project\" and links a project to an intervention. "@en ;
              rdfs:label "is project for"@en ,
                         "è progetto per"@it .


###  http://w3id.org/italia/onto/resolutionReason
:resolutionReason rdf:type owl:ObjectProperty ;
                  rdfs:domain :ResolutionDocument ;
                  rdfs:range :ResolutionReason ;
                  rdfs:comment "La proprietà che collega la risoluzione del contratto all'identificativo nel vocabolario controllato delle motivazioni delle risoluzioni."@it ,
                               "The property that links the resolution to the identifier within the controlled vocabulary of resolution reasons."@en ;
                  rdfs:label "ha motivazione"@it ,
                             "has resolution motivation"@en .


###  http://w3id.org/italia/onto/resultedFrom
:resultedFrom rdf:type owl:ObjectProperty ;
              rdfs:domain :EconomicSummary ;
              rdfs:range :ChangeEvent ;
              rdfs:comment "Questa proprietà è l'inversa di \"prospetto risultante\" e lega un prospetto economico all'evento di cambiamento che lo ha ridefinito."@it ,
                           "This property is the inverse of \"has resulting summary\" and links an economic summary to the change event that ridefined it."@en ;
              rdfs:label "resulted from"@en ,
                         "risultante da"@it .


###  http://w3id.org/italia/onto/selectedTender
:selectedTender rdf:type owl:ObjectProperty ;
                rdfs:domain :ProcedureOutcome ;
                rdfs:range :Tender ;
                rdfs:comment "La proprietà che indica l'offerta selezionata per la realizzazione dell'intervento, in seguito a un processo di aggiudicazione, affidamento o adesione ad un accordo quadro."@it ,
                             "The property that determines the selected tender submitted by a bidder."@en ;
                rdfs:label "ha offerta selezionata"@it ,
                           "has selected tender"@en .


###  http://w3id.org/italia/onto/suspensionReason
:suspensionReason rdf:type owl:ObjectProperty ;
                  rdfs:domain :SuspensionDocument ;
                  rdfs:range :SuspensionReason ;
                  rdfs:comment "La proprietà che collega la sospensione all'dentificativo nel vocabolario controllato delle motivazioni delle sospensioni."@it ,
                               "The property that links the suspension to the identifier within the controlled vocabulary of suspension reasons."@en ;
                  rdfs:label "ha motivazione"@it ,
                             "has suspension reason"@en .


###  http://w3id.org/italia/onto/variantReason
:variantReason rdf:type owl:ObjectProperty ;
               rdfs:domain :VariantDocument ;
               rdfs:range :VariantReason ;
               rdfs:comment "La proprietà che collega la variante all'identificativo nel vocabolario controllato delle motivazioni delle varianti."@it ,
                            "The property that links the variant to the identifier within the controlled vocabulary of variant reasons."@en ;
               rdfs:label "ha motivazione"@it ,
                          "has variant reason"@en .


#################################################################
#    Data properties
#################################################################

###  http://dati.gov.it/onto/tiapitdate
ti:date rdf:type owl:DatatypeProperty ;
        rdfs:isDefinedBy <htto://dati.gov.it/onto/tiapit/> .


###  http://dati.gov.it/onto/tiapitduration
ti:duration rdf:type owl:DatatypeProperty .


###  http://dati.gov.it/onto/tiapitendTime
ti:endTime rdf:type owl:DatatypeProperty .


###  http://dati.gov.it/onto/tiapitissued
ti:issued rdf:type owl:DatatypeProperty .


###  http://dati.gov.it/onto/tiapitstartTime
ti:startTime rdf:type owl:DatatypeProperty .


###  http://dati.gov.it/onto/l0/description
l0:description rdf:type owl:DatatypeProperty ;
               rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/identifier
l0:identifier rdf:type owl:DatatypeProperty ;
              rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/name
l0:name rdf:type owl:DatatypeProperty ;
        rdfs:isDefinedBy l0: .


###  http://w3id.org/italia/onto/additionalDays
:additionalDays rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf ti:duration ;
                rdfs:domain :VariantDocument ;
                rdfs:range xsd:float ;
                rdfs:comment "Questa proprietà indica i giorni aggiuntivi necessari per il completamento dell'intervento, indicati nel documento di variante."@it ,
                             "The property that represents the additional days needed to complete the intervention, as reported by the variant document."@en ;
                rdfs:label "additional days"@en ,
                           "giorni aggiuntivi"@it .


###  http://w3id.org/italia/onto/agreedAmount
:agreedAmount rdf:type owl:DatatypeProperty ;
              rdfs:domain :ProcedureOutcome ;
              rdfs:range xsd:float ;
              rdfs:comment "La proprietà che determina il prezzo concordato (anche detto importo di aggiudicazione, nel caso di gare con bando) per la realizzazione dell'intervento."@it ,
                           "The property that represents the agreed amount for the actualization of the intervention."@en ;
              rdfs:label "agreed amount"@en ,
                         "prezzo concordato"@it .


###  http://w3id.org/italia/onto/auctionBaseAmount
:auctionBaseAmount rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Procedure ;
                   rdfs:range xsd:float ;
                   rdfs:comment "La proprietà che determina l'importo base dell'asta."@it ,
                                "The property that determines the auction base amount."@en ;
                   rdfs:label "auction base amount"@en ,
                              "importo base asta"@it .


###  http://w3id.org/italia/onto/availableAmount
:availableAmount rdf:type owl:DatatypeProperty ;
                 rdfs:domain :EconomicSummary ;
                 rdfs:range xsd:float ;
                 rdfs:comment "La proprietà che determina la somma a disposizione per il completamento di un progetto, intervento, procedura o contratto, nell'ambito di una versione del prospetto economico."@it ,
                              "The property that represents the amount available to complete a project, intervention, procedure or contract. It is valid within a version of the economic summary."@en ;
                 rdfs:label "available amount"@en ,
                            "somma a disposizione"@it .


###  http://w3id.org/italia/onto/currentAvailableAmount
:currentAvailableAmount rdf:type owl:DatatypeProperty ;
                        rdfs:domain :Procedure ;
                        rdfs:range xsd:float ;
                        rdfs:comment "La proprietà che indica la somma attualmente a disposizione per il completamento della procedura, riportata nell'ultima versione del prospetto economico"@it ,
                                     "The property that represents the amount currently available for the completion of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
                        rdfs:label "attuale somma a disposizione"@it ,
                                   "current available amount"@en .


###  http://w3id.org/italia/onto/currentPlanningAmount
:currentPlanningAmount rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Procedure ;
                       rdfs:range xsd:float ;
                       rdfs:comment "La proprietà che determina l'importo attualmente previsto per la progettazione riportato nell'ultima versione del prospetto economico"@it ,
                                    "The property that represents the currently estimated planning amount needed for the completion of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
                       rdfs:label "attuale importo progettazione"@it ,
                                  "current planning amount"@en .


###  http://w3id.org/italia/onto/currentProcedureEstimatedEndDate
:currentProcedureEstimatedEndDate rdf:type owl:DatatypeProperty ;
                                  rdfs:subPropertyOf ti:endTime ;
                                  rdfs:domain :Procedure ;
                                  rdfs:range xsd:date ;
                                  rdfs:comment "La proprietà che indica la data entro la quale si prevede di portare a termine la procedura di realizzazione di un intervento. Si deduce tale data dall'attuale prospetto economico della procedura, o comunque si intende che essa indica la più recente e valida previsione fatta."@it ,
                                               "The property that represents the estimated end date of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
                                  rdfs:label "attuale previsione di fine procedura"@it ,
                                             "current procedure estimated end date"@en .


###  http://w3id.org/italia/onto/currentSafetyAmount
:currentSafetyAmount rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Procedure ;
                     rdfs:range xsd:float ;
                     rdfs:comment "La proprietà che determina l'importo attualmente previsto per la sicurezza riportato nell'ultima versione del prospetto economico"@it ,
                                  "The property that represents the currently estimated safety amount needed for the completion of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
                     rdfs:label "attuale importo sicurezza"@it ,
                                "current safety amount"@en .


###  http://w3id.org/italia/onto/currentServicesAmount
:currentServicesAmount rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Procedure ;
                       rdfs:range xsd:float ;
                       rdfs:comment "La proprietà che determina l'importo attualmente stimato per i servizi, riportato nell'ultima versione del prospetto economico."@it ,
                                    "The property that represents the currently estimated services amount needed for the completion of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
                       rdfs:label "attuale importo servizi"@it ,
                                  "current services amount"@en .


###  http://w3id.org/italia/onto/currentSuppliesAmount
:currentSuppliesAmount rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Procedure ;
                       rdfs:range xsd:float ;
                       rdfs:comment "La proprietà che determina l'importo attualmente stimato per le forniture, riportato nell'ultima versione del prospetto economico."@it ,
                                    "The property that represents the currently estimated supplies amount needed for the completion of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
                       rdfs:label "attuale importo forniture"@it ,
                                  "current supplies amount"@en .


###  http://w3id.org/italia/onto/currentTotalAmount
:currentTotalAmount rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Procedure ;
