<?xml version="1.0"?>
<rdf:RDF xmlns="http://w3id.org/italia/onto/pcapit#"
     xml:base="http://w3id.org/italia/onto/pcapit"
     xmlns:l0="http://dati.gov.it/onto/l0/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://w3id.org/italia/onto/pcapit">
        <owl:versionIRI rdf:resource="http://w3id.org/italia/onto/"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/covapit/"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/tiapit"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/l0/"/>
        <rdfs:comment xml:lang="en">This is the ontology of the Italian public contracts.</rdfs:comment>
        <rdfs:label xml:lang="it">Ontologia dei contratti pubblici italiani</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-04-27</terms:issued>
        <rdfs:label xml:lang="en">Italian public contracts ontology</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <rdfs:isDefinedBy rdf:resource="http://w3id.org/italia/onto/"/>
        <dc:creator xml:lang="en">CSI Piemonte</dc:creator>
        <dc:creator xml:lang="en">Synapta Srl</dc:creator>
        <rdfs:comment xml:lang="it">Questa è l&apos;ontologia dei contratti pubblici italiani.</rdfs:comment>
        <owl:versionInfo xml:lang="it">Versione 0.1 - 27 Aprile 2018</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.gov.it/onto/l0/controlledVocabulary -->

    <owl:AnnotationProperty rdf:about="http://dati.gov.it/onto/l0/controlledVocabulary"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#boolean -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#boolean"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#float -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#float"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#string -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/italia/onto/actualizes -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/actualizes">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/Intervention"/>
        <rdfs:comment xml:lang="it">La proprietà che lega una procedura di realizzazione a un intervento che realizza.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the procedure to the intervention actualized by the procedure.</rdfs:comment>
        <rdfs:label xml:lang="en">actualizes</rdfs:label>
        <rdfs:label xml:lang="it">realizza</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/approvedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/approvedBy">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/PaymentCertificate"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/WorkProgressReport"/>
        <rdfs:comment xml:lang="it">La proprietà che collega il certificato di pagamento al SAL (Stato Avanzamento Lavori) documento che contiene la delibera al pagamento stesso.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the certificate of payment to the work progress report whereby the payment is approved.</rdfs:comment>
        <rdfs:label xml:lang="en">approved by</rdfs:label>
        <rdfs:label xml:lang="it">deliberato in SAL</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/bidder -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/bidder">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Tender"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/covapit/Organization"/>
        <rdfs:comment xml:lang="it">La proprietà che indica l&apos;organizzazione offerente di un&apos;offerta.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the tender to its bidder.</rdfs:comment>
        <rdfs:label xml:lang="en">bidder</rdfs:label>
        <rdfs:label xml:lang="it">offerente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/changedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/changedBy">
        <owl:inverseOf rdf:resource="http://w3id.org/italia/onto/hasOriginalSummary"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/EconomicSummary"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/ChangeEvent"/>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;inversa di &quot;prospetto economico di origine&quot; e indica un evento che ha contribuito al cambiamento di questo prospetto economico.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is the inverse of &quot;has original summary&quot; and determines the change event which resulted in a change to this economic summary.</rdfs:comment>
        <rdfs:label xml:lang="it">cambiato da</rdfs:label>
        <rdfs:label xml:lang="en">changed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/haContratto -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/haContratto">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/italia/onto/hasDocument"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/Contract"/>
        <rdfs:comment xml:lang="it">La proprietà che determina il legame tra la procedura di realizzazione e il relativo contratto.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that determines the link between a contract and the relative procedure.</rdfs:comment>
        <rdfs:label xml:lang="it">ha contratto</rdfs:label>
        <rdfs:label xml:lang="en">has contract</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasAwardCriteria -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasAwardCriteria">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/AwardCriteria"/>
        <rdfs:comment xml:lang="it">La proprietà che determina il criterio di aggiudicazione della procedura di realizzazione. Tale proprietà si applica solo nel caso di procedure che prevedono gare con bando e conseguente aggiudicazione, e fa riferimento a un vocabolario controllato.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that determines the award criteria of the procedure. This property should be used in the case of procedures that include public calls followed by an awarding.</rdfs:comment>
        <rdfs:label xml:lang="it">ha criterio di aggiudicazione</rdfs:label>
        <rdfs:label xml:lang="en">has award criteria</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasCIG -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasCIG">
        <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/l0/identifier"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasCPV -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasCPV">
        <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/l0/identifier"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Contract"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Intervention"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Notice"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/CPV"/>
        <rdfs:comment xml:lang="it">La proprietà che associa un intervento, procedura di realizzazione, bando o contratto a un identificativo del vocabolario controllato dei CPV (Common Procurement Vocabulary), il sistema di classificazione per gli appalti pubblici.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links an intervention, procedure, notice or contract to the identifier within the controlled vocabulary of CPV (Common Procurement Vocabulary), a code describing the main object of a public procurement contract.</rdfs:comment>
        <rdfs:label xml:lang="it">ha CPV</rdfs:label>
        <rdfs:label xml:lang="en">has CPV</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasChangeDocument -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasChangeDocument">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Contract"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Intervention"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Project"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/ChangeDocument"/>
        <rdfs:comment xml:lang="it">La proprietà che indica il documento che descrive una modifica alle caratteristiche di un Progetto, Intervento, Procedura di realizzazione o Contratto.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that determines the document describing a change to the characteristics of a project, intervention, procedure or contract.</rdfs:comment>
        <rdfs:label xml:lang="it">ha documento di modifica</rdfs:label>
        <rdfs:label xml:lang="en">has change document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasContractingAuthority -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasContractingAuthority">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/covapit/Organization"/>
        <rdfs:comment xml:lang="it">La proprietà che indica l&apos;istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that determines the institution which issues a public contract, receives tenders and chooses a suitable supplier on the base of the conditions given by the contract.</rdfs:comment>
        <rdfs:label xml:lang="it">ha stazione appaltante</rdfs:label>
        <rdfs:label xml:lang="en">has contracting authority</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasDocument -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasDocument">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/Document"/>
        <rdfs:comment xml:lang="it">La proprietà che indica gli atti emessi dalla stazione appaltante nell&apos;espletamento della procedura di realizzazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The properties that links the procedure to the documents issued by the contracting authority during the course of the procedure.</rdfs:comment>
        <rdfs:label xml:lang="it">ha atto</rdfs:label>
        <rdfs:label xml:lang="it">has document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasEconomicSummary -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasEconomicSummary">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Contract"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Intervention"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Project"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/EconomicSummary"/>
        <rdfs:comment xml:lang="it">Questa proprietà indica il prospetto economico di un progetto, intervento, procedura di realizzazione, o contratto.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property represents the economic summary of a project, intervention, procedure or contract.</rdfs:comment>
        <rdfs:label xml:lang="it">ha prospetto economico</rdfs:label>
        <rdfs:label xml:lang="en">has economic summary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasFrameworkAgreement -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasFrameworkAgreement">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/FrameworkAgreement"/>
        <rdfs:comment xml:lang="it">La proprietà che lega un&apos;istanza di procedura di realizzazione a un accordo quadro o convenzione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The properties that links the procedure to its framework agreement.</rdfs:comment>
        <rdfs:label xml:lang="it">ha accordo quadro</rdfs:label>
        <rdfs:label xml:lang="en">has framework agreement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasNotice -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasNotice">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/italia/onto/hasDocument"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/Notice"/>
        <rdfs:comment xml:lang="it">La proprietà che indica il collegamento tra la procedura di realizzazione e un bando di gara o avviso. Diverse procedure di realizzazione possono fare riferimento ad un medesimo bando, allo stesso modo in cui, per quanto riguarda i dati applicativi SIMOG, più lotti (ciascuno dei quali corrisponde a una procedura di realizzazione) fanno riferimento allo stesso bando di gara.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that determines links the procedure to the related announcement or notice. More procedure may be linked to the same announcement or notice.</rdfs:comment>
        <rdfs:label xml:lang="it">ha bando</rdfs:label>
        <rdfs:label xml:lang="en">has notice</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasNoticeType -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasNoticeType">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Notice"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/NoticeType"/>
        <rdfs:comment xml:lang="it">La proprietà che indica il tipo di bando.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links a notice to its type.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di bando</rdfs:label>
        <rdfs:label xml:lang="en">has notice type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasOriginalSummary -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasOriginalSummary">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/ChangeEvent"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/EconomicSummary"/>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;inversa di &quot;cambiata da&quot; e collega un evento di cambiamento al prospetto economico precedente al cambiamento stesso.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is the inverse of &quot;changed by&quot; and links a change event to the economic summary preceding the change event.</rdfs:comment>
        <rdfs:label xml:lang="it">ha prospetto economico di origine</rdfs:label>
        <rdfs:label xml:lang="en">has original summary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasOutcome -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasOutcome">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/ProcedureOutcome"/>
        <rdfs:comment xml:lang="it">La proprietà che collega la procedura di realizzazione all&apos;esito (aggiudicazione, affidamento o accordo-quadro) della procedura stessa.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the procedure to its outcome (open tender award, direct award or framework agreement).</rdfs:comment>
        <rdfs:label xml:lang="it">ha esito</rdfs:label>
        <rdfs:label xml:lang="en">has outcome</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasPaymentCertificate -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasPaymentCertificate">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/PaymentCertificate"/>
        <rdfs:comment xml:lang="it">La proprietà che collega la procedura di realizzazione al certificato di pagamento deliberato in un SAL.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the procedure to the certificate of payment approved by the related progress of work report.</rdfs:comment>
        <rdfs:label xml:lang="it">ha certificato di pagamento</rdfs:label>
        <rdfs:label xml:lang="en">has certificate of payment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasProcedureType -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasProcedureType">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/ProcedureType"/>
        <rdfs:comment xml:lang="it">La proprietà che specifica i criteri di scelta di un contraente validi in una procedura di realizzazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the procedure to its type.</rdfs:comment>
        <rdfs:label xml:lang="it">ha scelta contraente</rdfs:label>
        <rdfs:label xml:lang="en">has type of procedure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasProject -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasProject">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Intervention"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/Project"/>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;inversa di &quot;è progetto di&quot; e lega un intervento a un progetto.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is the inverse of &quot;is project for&quot; and links an intervention to a project.</rdfs:comment>
        <rdfs:label xml:lang="it">ha progetto</rdfs:label>
        <rdfs:label xml:lang="en">has project</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasResolution -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasResolution">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Contract"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/Resolution"/>
        <rdfs:comment xml:lang="it">La proprietà che collega il contratto all&apos;evento che determina la risoluzione del contatto stesso</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the contract to the event that determines its resolution.</rdfs:comment>
        <rdfs:label xml:lang="it">ha risoluzione</rdfs:label>
        <rdfs:label xml:lang="en">has resolution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasResolutionToContract -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasResolutionToContract">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/italia/onto/hasDocument"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/ResolutionToContract"/>
        <rdfs:comment xml:lang="it">La proprietà che determina il legame tra la procedura di realizzazione e una determina a contrarre relativa.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the procedure to the document whereby the contracting authority expresses its resoultion to contract.</rdfs:comment>
        <rdfs:label xml:lang="it">ha determina a contrarre</rdfs:label>
        <rdfs:label xml:lang="en">has resolution to contract</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasResultingSummary -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasResultingSummary">
        <owl:inverseOf rdf:resource="http://w3id.org/italia/onto/resultedFrom"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/ChangeEvent"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/EconomicSummary"/>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;inversa di &quot;risultante da&quot; e indica un prospetto economico creato o ridefinito in seguito a un evento di cambiamento.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is the inverse of &quot;resulted from&quot; and determines the economic summary which was created or changed as a result of the change event.</rdfs:comment>
        <rdfs:label xml:lang="it">ha prospetto risultante</rdfs:label>
        <rdfs:label xml:lang="en">has resulting summary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/hasWorkProgressReport -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/hasWorkProgressReport">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Procedure"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/WorkProgressReport"/>
        <rdfs:comment xml:lang="it">La proprietà che collega la procedura di realizzazione al documento che ne certifica lo Stato di Avanzamento Lavori (SAL).</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links the procedure to its work progress report.</rdfs:comment>
        <rdfs:label xml:lang="it">ha SAL</rdfs:label>
        <rdfs:label xml:lang="en">has work progress report</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/italia/onto/isPrimaryTopicOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/italia/onto/isPrimaryTopicOf">
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/ChangeEvent"/>
        <rdfs:domain rdf:resource="http://w3id.org/italia/onto/Resolution"/>
        <rdfs:range rdf:resource="http://w3id.org/italia/onto/ChangeDocument"/>
        <rdfs:comment xml:lang="it">La proprietà che collega un evento (per esempio un evento di cambiamento del propetto economico, oppure un evento di risoluzione di un contratto) al documento di modifica che ne descrive le caratteristiche.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property that links an event (e.g., an event that modifies the economic summary of a procedure or the event that determines the resolution of a contract) to the document describing the characteristics of the event.</rdfs:comment>
        <rdfs:label xml:lang="en">is primary topic of</rdfs:label>
