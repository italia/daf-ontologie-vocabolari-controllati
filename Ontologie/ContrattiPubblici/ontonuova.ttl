#################################################################
#    Classes
#################################################################

:Contract a owl:Class ;
rdfs:subClassOf l0:Object ;
rdfs:label "Contract"@en;
rdfs:label "Contratto"@it ;
  rdfs:comment "An instance of this class is an abstract information entity about a realized contract between a contracting authority and their supplier."@en ;
  rdfs:comment "Una istanza di questa classe rappresenta un contratto realizzato tra una stazione appaltante e un fornitore."@it .

:ContractNotice a owl:Class ;
  rdfs:subClassOf l0:Object ;
  rdfs:label "Bando/Avviso"@it ;
  rdfs:label "Contract Notice"@en ;
  rdfs:comment "La classe che rappresenta un tipo di annuncio di uno o più lotti, sia esso un bando o un avviso."@it ;
  rdfs:comment "The class that represents a contract notice  of one or more lots. That is either a notice or an contract notice."@en .

:ContractNoticeType a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Tipologia del Bando"@it ;
  rdfs:label "Contract Notice Type "@en ;
  rdfs:comment "La classe che rappresenta il tipo di bando/avviso."@it ;
  rdfs:comment "The class that represents the contract notice type."@en .

:ProcedureType a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Procedura di Scelta del Contraente"@it ;
  rdfs:label "Procedure Type"@en ;
  rdfs:comment "The class that represents the type of procedure."@en ;
  rdfs:comment "La  Classe che rappresenta la procedura di scelta del contraente."@it .

:FrameworkAgreement a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Criterio di Aggiudicazione"@it ;
  rdfs:label "Framework Agreement"@en ;
  rdfs:comment "The class that represents the award type."@en .

:ContractNoticeOutcome a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Esito del Bando"@it ;
  rdfs:label "Contract Notice Outcome"@en ;
  rdfs:comment "La classe che rappresenta l'esito di un bando/avviso."@it ;
  rdfs:comment "The class that represents the type final state of the Contract Notice."@en .





:Tender a owl:Class ;
  rdfs:subClassOf l0:Object ;
  rdfs:label "Tender"@en ;
  rdfs:label "Offerta"@it ;
  rdfs:comment "An instance of this class rapresents a tender of a lot"@en ;
    rdfs:comment "Una istanza di questa classe rappresenta una offerta relativa ad un lotto."@it .


covapit:Organization a owl:Class ;
  rdfs:subClassOf :Agent ;
            rdfs:isDefinedBy covapit: .



:ChangeEvent  rdfs:subClassOf l0:Event ;
  rdfs:label "Change Event"@en ;
  rdfs:label "Evento di cambiamento"@it ;
  rdfs:comment "An instance of this class represents a chenge event"@en ;
  rdfs:comment "Una istance di questa classe un evento di cambiamento."@it .

:EconomicalFramework  rdfs:subClassOf l0:Characteristic;
  rdfs:label "Quadro Economico"@it ;
  rdfs:label "Economical Framework"@en ;
  rdfs:comment "An instance of this class represents the state of the economical framework of a lot"@en ;
  rdfs:comment "Una istance di questa classe rappresenta lo stato di del uqadro economico di un lotto."@it .

:ExtensionDocument a owl:Class ;
 rdfs:subClassOf :DocumentoDiModifica ;
  rdfs:label "Documento di Proroga"@it ;
  rdfs:label "Extension Document"@en ;
  rdfs:comment "The class that represents a lot extension."@en ;
  rdfs:comment "La classe che rappresenta la proroga di un lotto."@it .

:SuspensionDocument a owl:Class ;
 rdfs:subClassOf :DocumentoDiModifica ;
  rdfs:label "Documento di Sospensione"@it ;
  rdfs:label "Suspension Document"@en ;
  rdfs:comment "The class that represents a lot suspension."@en ;
  rdfs:comment "La classe che rappresenta la sospensione di un lotto."@it .

  :DocumentoDiModifica a owl:Class ;
 rdfs:subClassOf l0:Object ;
