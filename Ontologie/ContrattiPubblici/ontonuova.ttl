#################################################################
#    Annotation properties
#################################################################

###  http://dati.gov.it/onto/l0/controlledVocabulary
l0:controlledVocabulary a owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#float
xsd:float a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean a rdfs:Datatype .



#################################################################
#    Object Properties
#################################################################



:frameworkAgreement a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :FrameworkAgreement ;
  rdfs:label "ha criterio di aggiudicazione"@it ;
  rdfs:label "type of Award" @en ;
  rdfs:comment "La proprietà che determina il tipo di aggiudicazione del bando."@it ;
  rdfs:comment "The property that determines the type of award of the contract notice."@en .

:realizza a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Intervento ;
  rdfs:label "realizza"@it ;
  rdfs:label "" @en .

:haAccordoQuadro a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ProceduraDiRealizzazione ;
  rdfs:label "ha Accordo quadro"@it ;
  rdfs:label "" @en .

:haContratto a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Contract ;
  rdfs:label "ha Contratto"@it ;
  rdfs:label "" @en ;
  rdfs:comment "La proprietà che determina il legame tra un contratto e il relativo lotto."@it ;
  rdfs:comment "The property that determines the link between a contract ad the realtive lot."@en .


:hasDeterminaAContrarre a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :DeterminaAContrarre ;
  rdfs:label "ha Determina a contrarre"@it ;
  rdfs:label "has Determina a contrarre" @en ;
  rdfs:comment "La proprietà che determina il legame tra un Lotto e una Determina a contrarre relativo lotto."@it ;
  rdfs:comment "The property that determines the link between a Lot and a Determina a contrarre."@en .


:contractingAuthority a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range covapit:Organization ;
  rdfs:label "stazione appaltante"@it ;
  rdfs:label "contracting Authority"@en ;
  rdfs:comment "La proprietà che indica l'istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto."@it ;
  rdfs:comment "Institution which issues a public contract, receives tenders to the contract and chooses a suitable supplier on the base of the conditions given by the contract."@en .

:hasContractNotice a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ContractNotice ;
  rdfs:label "ha bando" @it ;
  rdfs:label "has contract notice" @en ;
  rdfs:comment "La proprietà che indica se il contratto appartiene a un bando."@it ;
  rdfs:comment "The property that determines if the related contract belongs to a contract notice."@en .

:procedureType a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ProcedureType ;
  rdfs:label "ha scelta contraente"@it ;
  rdfs:label "has type of procedure"@en ;
  rdfs:comment "La proprietà che specifica i criteri applicati per la pubblicazione di un lotto e per la scelta di un contraente."@it ;
  rdfs:comment "Type of procedure of a public contract specifies a procedure which is applied to publish information about the contract and choose a supplier."@en .

:awardedTender a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Tender ;
  rdfs:label "ha offerta aggiudicataria"@it ;
  rdfs:label "has awarded tender"@en ;
  rdfs:comment "La proprietà che indica l'offerta aggiudicataria presentata da un offerente."@it ;
  rdfs:comment "Property for awarded tender submitted by a bidder."@en .


:haDocumentoModifica a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contract ;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "ha documento di modifica"@it ;
  rdfs:label "has Change document"@en ;
  rdfs:comment "La proprietà che indica una modifica alle caratteristiche del Lotto."@it ;
  rdfs:comment "The property that determines the Lot Change."@en .


:contractNoticeOutcome a owl:ObjectProperty ;
  rdfs:domain :ContractNotice ;
  rdfs:range :ContractNoticeOutcome ;
  rdfs:label "ha esito bando"@it ;
  rdfs:label "has Contract Notice outcome "@en ;
  rdfs:comment "La proprietà che indica l'esito del bando."@it ;
  rdfs:comment "The property that determines the Contract Notice outcome."@en .

:bidder a owl:ObjectProperty ;
  rdfs:domain :Tender ;
  rdfs:range covapit:Organization ;
  rdfs:label "bidder"@en ;
  rdfs:label "Offerente"@it ;
  rdfs:comment "This property represents the bidder of a Tender"@en ;
  rdfs:comment "Questa proprietà rappresenta l'offerente di una offerta."@it .

:isPrimaryTopicOf
  rdfs:domain :ChangeEvent ;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "è argomento principale di" @it ;
  rdfs:label "is primary topic of" @en ;
  rdfs:comment "La proprietà collega qualcosa al documento che riguarda principalmente la cosa stessa."@it ;
  rdfs:comment "The isPrimaryTopicOf property relates something to a document that is mainly about it."@en .

:hasEconomicalFramework a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contract ;
  rdfs:range :EconomicalFramework ;
  rdfs:label "has economical framework"@en ;
  rdfs:label "ha quadro economico"@it ;
  rdfs:comment "This property represents the economical framework of a Lot"@en ;
  rdfs:comment "Questa proprietà rappresenta il quadro economico di un Lotto."@it .

:changedBy a owl:ObjectProperty ;
    rdfs:domain :EconomicalFramework ;
    rdfs:range  :ChangeEvent ;
    rdfs:label "changed by"@en;
    rdfs:label "cambiata da"@it;
    rdfs:comment "Indicates a Change event which resulted in a change to this Economical Framework."@en ;
    rdfs:comment "Indica un evento che ha contribuito al cambiamento di questo Quadro economico."@it .

:resultingFramework a owl:ObjectProperty ;
  rdfs:domain :ChangeEvent ;
  rdfs:range :EconomicalFramework ;
  rdfs:label "resulted framework"@en ;
  rdfs:label "quadro risultante"@it ;
  rdfs:comment "Indicates an economical framework which was created or changed as a result of the event."@en ;
  rdfs:comment "Indica un quadro economico creato o ridefinito in seguito a un evento di cambiamento."@it .

:haProgetto a owl:ObjectProperty ;
  rdfs:domain :Progetto ;
  rdfs:range :Intervento ;
  rdfs:label ""@en ;
  rdfs:label "ha progetto"@it .

:haTipoBando a owl:ObjectProperty ;
  rdfs:domain :ContractNotice ;
  rdfs:range :ContractNoticeType ;
  rdfs:label ""@en ;
  rdfs:label "ha tipo bando"@it ;
  rdfs:comment "La proprietà che indica il tipo di bando."@it ;
  rdfs:comment "The property that determines the Contract Notice type."@en .

###  http://dati.gov.it/onto/l0/description
l0:description a owl:DatatypeProperty ;
               rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/identifier
l0:identifier a owl:DatatypeProperty ;
              rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/name
l0:name a owl:DatatypeProperty ;
        rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/tiapit/date
ti:date a owl:DatatypeProperty ;
        rdfs:isDefinedBy <htto://dati.gov.it/onto/tiapit/> .


#################################################################
#    Data properties
#################################################################

:actualEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:date ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui l'intervento è stato realizzato."@it ;
  rdfs:label "Data effettiva realizzazione intervento"@it ;
  rdfs:label ""@en .

:startDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:startTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:label "Data di inizio intervento"@it ;
  rdfs:label "Start Date"@en ;
  rdfs:comment "The property that detemins the beginning date."@en ;
  rdfs:comment "La proprietà che determina la data effettiva di inizio dell'intervento."@en ;
  rdfs:range xsd:date .

  :dataAggiudicazioneAffidamento a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:date ;
    rdfs:domain :ProceduraDiRealizzazione ;
    rdfs:range xsd:date ;
    rdfs:label ""@en ;
    rdfs:label "data di aggiudicazione/affidamento"@it ;
    rdfs:comment "Proprietà che indica la data di aggiudicazione a un offerente o affidamento." @it ;
    rdfs:comment "" @en .

  :publicationDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:issued ;
    rdfs:domain :ContractNotice ;
    rdfs:range xsd:date ;
      rdfs:label "data pubblicazione"@it ;
    rdfs:label "publication date"@en ;
    rdfs:comment "La proprietà che indica la data di pubblicazione del Bando/Avviso."@it ;
    rdfs:comment "The property that determines the publication date of the Contract Notice."@en .

  :expireDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:issued ;
    rdfs:domain :ContractNotice ;
    rdfs:range xsd:date ;
    rdfs:label "expire date"@en ;
    rdfs:label "data di scadenza"@it ;
    rdfs:comment "Proprietà che determina la data di scadenza del Bando/Avviso."@it ;
   rdfs:comment "The property that determines the expire date."@en .

   :CurrentEstimatedEndDate a owl:DatatypeProperty ;
     rdfs:subPropertyOf ti:endTime ;
     rdfs:domain :ProceduraDiRealizzazione ;
     rdfs:label "ultima data stimata realizzazione intervento"@it ;
     rdfs:label ""@en ;
     rdfs:comment "The property that detemins the estimated end date."@en ;
     rdfs:comment "La proprietà che determina l'ultima data stimata per il completamento dell'intervento."@it ;
     rdfs:range xsd:date .

   :estimatedEndDate a owl:DatatypeProperty ;
     rdfs:subPropertyOf ti:endTime ;
     rdfs:domain :EconomicalFramework ;
     rdfs:range xsd:float ;
     rdfs:label "data stimata realizzazione intervento"@it ;
     rdfs:label ""@en ;
     rdfs:comment "The property that determines the estimated end date."@en ;
     rdfs:comment "Questa proprietà determina la data stimata di realizzazione dell'intervento, nell'ambito di una versione del quadro economico."@it .

  :additionalDays a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:duration ;
    rdfs:domain :VariantDocument;
    rdfs:range xsd:float ;
    rdfs:label "giorni aggiuntivi" @it ;
    rdfs:label "additional days" @en ;
    rdfs:comment ""@en ;
    rdfs:comment "Questa proprietà indica i giorni aggiuntivi necessari per il completamento dell'intervento, indicati nella variante."@it.

:agreedPrice a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "prezzo concordato"@it ;
  rdfs:label "agreed price"@en ;
  rdfs:comment "La proprietà che determina il prezzo concordato (anche detto importo di aggiudicazione, nel caso di gare con bando) per la realizzazione dell'intervento."@it ;
  rdfs:comment ""@en .

:nonDiscountableSum a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "somma non ribassabile"@it ;
  rdfs:label "sum of non-discountable money"@en ;
  rdfs:comment "The property that determines the amount of the starting price that  is not lowerable."@en ;
  rdfs:comment "La proprietà che determina la somma di denaro che non è ribassabile."@it .

:reductionPercentage a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "percentuale di ribasso"@it ;
  rdfs:label "reduction percentage"@en ;
  rdfs:comment "La proprietà che determina la percentuale dichiarata di ribasso."@it ;
  rdfs:comment "The property that determines the declared percentage of price cut."@en .

:auctionBaseAmount a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "importo base asta"@it ;
  rdfs:label "auction base amount"@en ;
  rdfs:comment "La proprietà che determina l'importo base dell'asta."@it ;
  rdfs:comment "The property that determines the auction base amount."@en .

:safetyAmount a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "importo sicurezza"@it ;
  rdfs:label "safety amount"@en ;
  rdfs:comment "La proprietà che determina l'importo stabilito destinato alla sicurezza."@it ;
  rdfs:comment "The property that determines the amount of money to be spent for safety reason."@en .

:variantType a owl:DatatypeProperty ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:string ;
  rdfs:label "tipo variante" @it ;
  rdfs:label "variant type" @en ;
  rdfs:comment "La proprietà che determina il tipo di variante del contratto."@it ;
  rdfs:comment "The property that determines contract variant type."@en .

:hasReason a owl:DatatypeProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:string ;
  rdfs:label "ha motivazione"@it ;
  rdfs:label "has motivation"@it ;
  rdfs:comment "La proprietà che collega la variante del contratto all'identificativo nel vocabolario controllato delle motivazioni delle varianti."@it ;
  rdfs:comment "The property that links the contract variant to the identifier within the controlled vocabulary of variant reasons."@en .
#TODO compilare vocabolario controllato delle motivazioni delle varianti

:importoAggiuntivo a owl:DatatypeProperty ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:float ;
  rdfs:label "importo aggiuntivo"@it ;
  rdfs:label ""@en ;
  rdfs:comment "Questa proprietà indica l'importo aggiuntivo, indicato nella variante, necessario per il completamento dell'intervento."@it ;
  rdfs:comment ""@en .


:cig a owl:DatatypeProperty ;
  rdfs:subPropertyOf l0:identifier ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:string ;
  rdfs:label "CIG"@it ;
  rdfs:label "CIG"@en ;
  rdfs:comment "La proprietà che identifica la procedura di realizzazione tramite un CIG (Codice Identificativo di Gara)."@it ;
  rdfs:comment ""@en .

  :cpv a owl:DatatypeProperty ;
    rdfs:subPropertyOf l0:identifier ;
    l0:controlledVocabulary <http://dati.gov.it/onto/controlledvocabulary/CommonProcurementVocabulary> ;
    rdfs:domain :ProceduraDiRealizzazione ;
    rdfs:range xsd:string ;
    rdfs:label "cpv"@it ;
    rdfs:label "cpv"@en ;
    rdfs:comment "La proprietà che classifica una procedura di realizzazione tramite un identificativo del vocabolario controllato dei CPV (Common Procurement Vocabulary)."@it ;
    rdfs:comment "The property that links the Procedura di realizzazione to the identifier within the controlled vocabulary of CPV (Common Procurement Vocabulary), code describing the main object of a public contract."@en .
#TODO completare il vocabolario controllato dei CPV aggiungendo le relazioni gerarchiche

:modalitaSceltaContraente a owl:DatatypeProperty ;
  rdfs:subPropertyOf l0:identifier ;
  #l0:controlledVocabulary <http://dati.gov.it/onto/controlledvocabulary/xxx> ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:string ;
  rdfs:label "modalità di scelta contraente"@it ;
  rdfs:label ""@en .
#TODO compilare il vocabolario controllato delle modalità scelta contraente.

:isSubcontractExpected a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:boolean ;
  rdfs:label "è previsto subappalto"@it ;
  rdfs:label "is subcontract expected"@en ;
  rdfs:comment "La proprietà che determina se è previsto un Subappalto."@it ;
  rdfs:comment "The property that determines if a subcontract is expected."@en .

:ultimoImportoProgetto a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "ultimo importo progetto"@it ;
  rdfs:label ""@en ;
  rdfs:comment "La proprietà che determina l'importo previsto per la realizzazione dell'intervento, secondo l'ultima versione del quadro economico."@it ;
  rdfs:comment ""@en .

:importoProgetto a owl:DatatypeProperty ;
  rdfs:domain :EconomicalFramework ;
  rdfs:range xsd:float ;
  rdfs:label "importo progetto"@it ;
  rdfs:label ""@en ;
  rdfs:comment "La proprietà che determina l'importo previsto per la realizzazione dell'intervento, nell'ambito di una versione del quadro economico."@it ;
  rdfs:comment ""@en .

:importoLiquidato a owl:DatatypeProperty ;
  rdfs:domain :EconomicalFramework ;
  rdfs:range xsd:float ;
  rdfs:label "Importo liquidato"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@en ;
  rdfs:comment "La proprietà che indica l'importo che è stato liquidato, nell'ambito di una versione del quadro economico."@it .

:ultimoImportoLiquidato a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "Ultimo importo liquidato"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@en ;
  rdfs:comment "La proprietà che indica la sommatoria degli importi liquidati per la realizzazione dell'intervento, secondo l'ultimo quadro economico aggiornato."@it .


:specialSector a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:boolean ;
  rdfs:label "settore speciale"@it ;
  rdfs:label "special sector"@en ;
  rdfs:comment ""@en ;
  rdfs:comment "La proprietà che determina se una procedura di realizzazione appartiene a uno dei settori speciali definiti dal DLGS 163/2006 Art3."@it .

#################################################################
#    Classes
#################################################################

:Contract a owl:Class ;
  rdfs:subClassOf l0:Object ;
  rdfs:label "Contract"@en;
  rdfs:label "Contratto"@it ;
  rdfs:comment "An instance of this class is an abstract information entity about a realized contract between a contracting authority and their supplier."@en ;
  rdfs:comment "Una istanza di questa classe rappresenta un contratto realizzato tra una stazione appaltante e un fornitore."@it .

:ContractNotice a owl:Class ;
  rdfs:subClassOf l0:Object ;
  rdfs:label "Bando/Avviso"@it ;
  rdfs:label "Contract Notice"@en ;
  rdfs:comment "La classe che rappresenta qualunque tipo di annuncio o avviso relativo ad una procedura di realizzazione di un Intervento. Tipicamente, un bando pubblico o un'altra tipologia di avviso."@it ;
  rdfs:comment "The class that represents a contract notice of one or more lots. That is either a notice or an contract notice."@en .

:ContractNoticeType a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Tipologia del Bando"@it ;
  rdfs:label "Contract Notice Type "@en ;
  rdfs:comment "La classe che rappresenta il tipo di bando/avviso."@it ;
  rdfs:comment "The class that represents the contract notice type."@en .

:ProcedureType a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Procedura di Scelta del Contraente"@it ;
  rdfs:label "Procedure Type"@en ;
  rdfs:comment "The class that represents the type of procedure."@en ;
  rdfs:comment "La  Classe che rappresenta la procedura di scelta del contraente."@it .

:FrameworkAgreement a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Criterio di Aggiudicazione"@it ;
  rdfs:label "Framework Agreement"@en ;
  rdfs:comment "The class that represents the award type."@en .

:ContractNoticeOutcome a owl:Class ;
  rdfs:subClassOf l0:Characteristic ;
  rdfs:label "Esito del Bando"@it ;
  rdfs:label "Contract Notice Outcome"@en ;
  rdfs:comment "La classe che rappresenta l'esito di un bando/avviso."@it ;
  rdfs:comment "The class that represents the type final state of the Contract Notice."@en .

:Tender a owl:Class ;
  rdfs:subClassOf l0:Object ;
  rdfs:label "Tender"@en ;
  rdfs:label "Offerta"@it ;
  rdfs:comment "An instance of this class rapresents a tender in a call for tenders or other procedure."@en ;
  rdfs:comment "Una istanza di questa classe rappresenta una offerta relativa ad un bando o altra procedura di realizzazione."@it .

covapit:Organization a owl:Class ;
  rdfs:subClassOf :Agent ;
  rdfs:isDefinedBy covapit: .

:ChangeEvent  rdfs:subClassOf l0:Event ;
  rdfs:label "Change Event"@en ;
  rdfs:label "Evento di cambiamento"@it ;
  rdfs:comment "An instance of this class represents a chenge event"@en ;
  rdfs:comment "Una istance di questa classe un evento di cambiamento."@it .

:EconomicalFramework  rdfs:subClassOf l0:Characteristic;
  rdfs:label "Quadro Economico"@it ;
  rdfs:label "Economical Framework"@en ;
  rdfs:comment "An instance of this class represents the state of the economical framework of a lot"@en ;
  rdfs:comment "Una istance di questa classe rappresenta lo stato di del uqadro economico di un lotto."@it .

:ExtensionDocument a owl:Class ;
  rdfs:subClassOf :DocumentoDiModifica ;
  rdfs:label "Documento di Proroga"@it ;
  rdfs:label "Extension Document"@en ;
  rdfs:comment "The class that represents a lot extension."@en ;
  rdfs:comment "La classe che rappresenta la proroga di un lotto."@it .

:SuspensionDocument a owl:Class ;
  rdfs:subClassOf :DocumentoDiModifica ;
  rdfs:label "Documento di Sospensione"@it ;
  rdfs:label "Suspension Document"@en ;
  rdfs:comment "The class that represents a lot suspension."@en ;
  rdfs:comment "La classe che rappresenta la sospensione di un lotto."@it .

  :DocumentoDiModifica a owl:Class ;
 rdfs:subClassOf l0:Object ;
  rdfs:label "Documento di modifica"@it ;
  rdfs:label "Modification document"@en ;
  rdfs:comment "The class that represents a Document that states of the lot characteristic"@en ;
  rdfs:comment "La classe che rappresenta un Documento che stabilisce una modifica nelle caratteristichedel lotto."@it .

:VariantDocument a owl:Class ;
  rdfs:subClassOf :DocumentoDiModifica ;
  rdfs:label "Documento di Variante"@it ;
  rdfs:label "Variant Document"@en ;
