:actualEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:date ;
  rdfs:comment "Property for actual end date of public contract's realization."@en ;
  rdfs:comment "Questa proprietà indica la data effettiva di chiusura lavori"@it ;
  rdfs:label "Data chiusura lavori"@it ;
  rdfs:label "Property for actual end date of public contract's realization."@en .

:agreedPrice a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione, :ContractNotice;
  rdfs:range xsd:float ;
  rdfs:label "Prezzo concordato"@it ;
  rdfs:label "Agreed price"@en ;
  rdfs:comment "La proprietà che determina il prezzo concordato per il Lotto o per il Bando."@it ;
  rdfs:comment "The Property that determines the price agreed for the lot or the Contract Notice."@en .

:nonDiscountableSum a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "Somma non ribassabile"@it ;
  rdfs:label "Sum of non-discountable money"@en ;
  rdfs:comment "The property that determines the amount of the starting price that is not lowerable."@en ;
  rdfs:comment "La proprietà che determina la somma di denaro che non è ribassabile relativa ad un Lotto."@it .




  :startDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:startTime ;
    rdfs:domain :ProceduraDiRealizzazione ;
    rdfs:label "Data di inizio"@it ;
    rdfs:label "Start Date"@en ;
    rdfs:comment "The property that detemins the beginning date."@en ;
    rdfs:comment "La proprietà che determina la data di inizio dei lavori."@en ;
    rdfs:range xsd:date .

:isSubcontractExpected a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:boolean ;
  rdfs:label "è previsto subappalto"@it ;
  rdfs:label "is subcontract expected"@en ;
  rdfs:comment "La proprietà che determina se nel Lotto è previsto un Subappalto."@it ;
  rdfs:comment "The property that determines if a subcontract if expected in the Lot."@en .

:reductionPercentage a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
