:motivazioneInterruzione a owl:DatatypeProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :DocumentoInterruzione;
  rdfs:range xsd:string ;
  rdfs:label "ha motivazione"@it ;
