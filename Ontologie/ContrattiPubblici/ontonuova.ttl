:frameworkAgreement a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :FrameworkAgreement ;
  rdfs:label "ha criterio di aggiudicazione"@it ;
  rdfs:label "type of Award" @en ;
  rdfs:comment "La proprietà che determina il tipo di aggiudicazione del bando."@it ;
  rdfs:comment "The property that determines the type of award of the contract notice."@en .

:realizza a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Intervento ;
  rdfs:label "realizza"@it ;
  rdfs:label "" @en .

:haAccordoQuadro a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ProceduraDiRealizzazione ;
  rdfs:label "ha Accordo quadro"@it ;
  rdfs:label "" @en .

:haContratto a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Contract ;
  rdfs:label "ha Contratto"@it ;
  rdfs:label "" @en ;
  rdfs:comment "La proprietà che determina il legame tra un contratto e il relativo lotto."@it ;
  rdfs:comment "The property that determines the link between a contract ad the realtive lot."@en .


:hasDeterminaAContrarre a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :DeterminaAContrarre ;
  rdfs:label "ha Determina a contrarre"@it ;
  rdfs:label "has Determina a contrarre" @en ;
  rdfs:comment "La proprietà che determina il legame tra un Lotto e una Determina a contrarre relativo lotto."@it ;
  rdfs:comment "The property that determines the link between a Lot and a Determina a contrarre."@en .


:contractingAuthority a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range covapit:Organization ;
  rdfs:label "stazione appaltante"@it ;
  rdfs:label "contracting Authority"@en ;
  rdfs:comment "La proprietà che indica l'istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto."@it ;
  rdfs:comment "Institution which issues a public contract, receives tenders to the contract and chooses a suitable supplier on the base of the conditions given by the contract."@en .

:hasContractNotice a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ContractNotice ;
  rdfs:label "ha bando" @it ;
  rdfs:label "has contract notice" @en ;
  rdfs:comment "La proprietà che indica se il contratto appartiene a un bando."@it ;
  rdfs:comment "The property that determines if the related contract belongs to a contract notice."@en .

:procedureType a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ProcedureType ;
  rdfs:label "ha scelta contraente"@it ;
  rdfs:label "has type of procedure"@en ;
  rdfs:comment "La proprietà che specifica i criteri applicati per la pubblicazione di un lotto e per la scelta di un contraente."@it ;
  rdfs:comment "Type of procedure of a public contract specifies a procedure which is applied to publish information about the contract and choose a supplier."@en .

:awardedTender a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Tender ;
  rdfs:label "ha offerta aggiudicataria"@it ;
  rdfs:label "has awarded tender"@en ;
  rdfs:comment "La proprietà che indica l'offerta aggiudicataria presentata da un offerente."@it ;
  rdfs:comment "Property for awarded tender submitted by a bidder."@en .


:haDocumentoModifica a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contract ;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "ha documento di modifica"@it ;
  rdfs:label "has Change document"@en ;
  rdfs:comment "La proprietà che indica una modifica alle caratteristiche del Lotto."@it ;
  rdfs:comment "The property that determines the Lot Change."@en .

#:contractNoticeType a owl:ObjectProperty ;
#  rdfs:domain :ContractNotice ;
#  rdfs:range :ContractNoticeType ;
#  rdfs:label "ha tipo bando"@it ;
#  rdfs:label "has Contract Notice type "@en ;
#  rdfs:comment "La proprietà che indica il tipo di bando."@it ;
#  rdfs:comment "The property that determines the Contract Notice type."@en .

:contractNoticeOutcome a owl:ObjectProperty ;
  rdfs:domain :ContractNotice ;
  rdfs:range :ContractNoticeOutcome ;
  rdfs:label "ha esito bando"@it ;
  rdfs:label "has Contract Notice outcome "@en ;
  rdfs:comment "La proprietà che indica l'esito del bando."@it ;
  rdfs:comment "The property that determines the Contract Notice outcome."@en .






:bidder a owl:ObjectProperty ;
  rdfs:domain :Tender ;
  rdfs:range covapit:Organization ;
  rdfs:label "bidder"@en ;
  rdfs:label "Offerente"@it ;
  rdfs:comment "This property represents the bidder of a Tender"@en ;
  rdfs:comment "Questa proprietà rappresenta l'offerente di una offerta."@it .

:isPrimaryTopicOf
  rdfs:domain :ChangeEvent ;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "è argomento principale di" @it ;
  rdfs:label "is primary topic of" @en ;
  rdfs:comment "La proprietà collega qualcosa al documento che riguarda principalmente la cosa stessa."@it ;
  rdfs:comment "The isPrimaryTopicOf property relates something to a document that is mainly about it."@en .

:hasEconomicalFramework a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contract ;
  rdfs:range :EconomicalFramework ;
  rdfs:label "has economical framework"@en ;
  rdfs:label "ha quadro economico"@it ;
  rdfs:comment "This property represents the economical framework of a Lot"@en ;
  rdfs:comment "Questa proprietà rappresenta il quadro economico di un Lotto."@it .

:changedBy a owl:ObjectProperty ;
    rdfs:domain :EconomicalFramework ;
    rdfs:range  :ChangeEvent ;
    rdfs:label "changed by"@en;
    rdfs:label "cambiata da"@it;
    rdfs:comment "Indicates a Change event which resulted in a change to this Economical Framework."@en ;
    rdfs:comment "Indica un evento che ha contribuito al cambiamento di questo Quadro economico."@it .

:resultingFramework a owl:ObjectProperty ;
  rdfs:domain :ChangeEvent ;
  rdfs:range :EconomicalFramework ;
  rdfs:label "resulted framework"@en ;
  rdfs:label "quadro risultante"@it ;
  rdfs:comment "Indicates an economical framework which was created or changed as a result of the event."@en ;
  rdfs:comment "Indica un quadro economico creato o ridefinito in seguito a un evento di cambiamento."@it .

:haProgetto a owl:ObjectProperty ;
  rdfs:domain :Progetto ;
  rdfs:range :Intervento ;
  rdfs:label ""@en ;
  rdfs:label "ha progetto"@it .

:haTipologiaDelBando a owl:ObjectProperty ;
  rdfs:domain :ContractNotice ;
  rdfs:range :ContractNoticeType ;
  rdfs:label ""@en ;
  rdfs:label "ha progetto"@it .

