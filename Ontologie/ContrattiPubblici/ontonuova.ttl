#################################################################
#    Annotation properties
#################################################################

###  http://dati.gov.it/onto/l0/controlledVocabulary
l0:controlledVocabulary a owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#float
xsd:float a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean a rdfs:Datatype .



#################################################################
#    Object Properties
#################################################################


:haAggiudicazione a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Aggiudicazione ;
  rdfs:label "ha Aggiudicazione"@it ;
  rdfs:label "" @en .

:haAtto a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Documento ;
  rdfs:label "ha Atto"@it ;
  rdfs:label "" @en .

:haCriterioDiAggiudicazione a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :CriterioDiAggiudicazione ;
  rdfs:label "ha criterio di aggiudicazione"@it ;
  #rdfs:label "type of Award" @en ;
  rdfs:comment "The property that determines the type of award of the contract notice."@en ;
#  rdfs:comment "La proprietà che determina il tipo di aggiudicazione del bando."@it .



:realizza a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Intervento ;
  rdfs:label "realizza"@it ;
  rdfs:label "" @en .

:haAccordoQuadro a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :AccordoQuadro ;
  rdfs:label "ha Accordo quadro"@it ;
  rdfs:label "" @en .

:haContratto a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Contratto;
  rdfs:label "ha Contratto"@it ;
  rdfs:label "" @en ;
#  rdfs:comment "The property that determines the link between a contract ad the realtive Implementation procedure."@en ;
  rdfs:comment "La proprietà che determina il legame tra un contratto e la relativa Procedura di realizzazione."@it .

:haDeterminaAContrarre a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :DeterminaAContrarre ;
  rdfs:label "ha Determina a contrarre"@it ;
  #rdfs:label "has Determina a contrarre" @en ;
  #rdfs:comment "The property that determines the link between an Implementation procedure and a Determina a contrarre."@en ;
  rdfs:comment "La proprietà che determina il legame tra una Procedura di realizzazione e una Determina a contrarre relativa."@it .



:contractingAuthority a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range covapit:Organization ;
  rdfs:label "stazione appaltante"@it ;
  rdfs:label "contracting Authority"@en ;
  rdfs:comment "La proprietà che indica l'istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto."@it ;
  rdfs:comment "Institution which issues a public contract, receives tenders to the contract and chooses a suitable supplier on the base of the conditions given by the contract."@en .

:haBando a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Bando ;
  rdfs:label "ha bando" @it ;
#  rdfs:label "has contract notice" @en ;
#  rdfs:comment "The property that determines if the related contract belongs to a contract notice."@en .
  rdfs:comment ""@it .

:haAvviso a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Avviso ;
  rdfs:label "ha avviso" @it ;
#  rdfs:label "has contract notice" @en ;
#  rdfs:comment "The property that determines if the related contract belongs to a contract notice."@en .
  rdfs:comment ""@it .



:haSceltaContraente a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :modSceltaContraente ;
  rdfs:label "ha scelta contraente"@it ;
  #rdfs:label "has type of procedure"@en ;
# rdfs:comment "Type of procedure of a public contract specifies a procedure which is applied to publish information about the contract and choose a supplier."@en 
  rdfs:comment "La proprietà che specifica i criteri che governano la procedura di scelta di un contraente."@it .


:awardedTender a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Tender ;
  rdfs:label "ha offerta aggiudicataria"@it ;
  rdfs:label "has awarded tender"@en ;
  rdfs:comment "La proprietà che indica l'offerta aggiudicataria presentata da un offerente."@it ;
  rdfs:comment "Property for awarded tender submitted by a bidder."@en .


:haDocumentoModifica a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contratto;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "ha documento di modifica"@it ;
  rdfs:label "has Change document"@en ;
  rdfs:comment "La proprietà che indica una modifica alle caratteristiche di un Progetto, Intervento, Procedura di realizzazione, o Contratto."@it ;
  rdfs:comment "The property that determines the Lot Change."@en .

:haCertificatoDiPagamento a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :CertificatoPagamento ;
  rdfs:label "ha certificato di pagamento"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:deliberatoInSAL a owl:ObjectProperty ;
  rdfs:domain :CertificatoPagamento ;
  rdfs:range :SAL ;
  rdfs:label "deliberato in SAL"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:haSAL a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :SAL ;
  rdfs:label "ha SAL"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:contractNoticeOutcome a owl:ObjectProperty ;
  rdfs:domain :Bando;
  rdfs:range :ContractNoticeOutcome ;
  rdfs:label "ha esito bando"@it ;
  rdfs:label "has Contract Notice outcome "@en ;
  rdfs:comment "La proprietà che indica l'esito del bando."@it ;
  rdfs:comment "The property that determines the Contract Notice outcome."@en .

:bidder a owl:ObjectProperty ;
  rdfs:domain :Tender ;
  rdfs:range covapit:Organization ;
  rdfs:label "bidder"@en ;
  rdfs:label "Offerente"@it ;
  rdfs:comment "This property represents the bidder of a Tender"@en ;
  rdfs:comment "Questa proprietà rappresenta l'offerente di una offerta."@it .

:isPrimaryTopicOf
  rdfs:domain :EventoDiCambiamento ;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "è argomento principale di" @it ;
  rdfs:label "is primary topic of" @en ;
  rdfs:comment "La proprietà collega qualcosa al documento che riguarda principalmente la cosa stessa."@it ;
  rdfs:comment "The isPrimaryTopicOf property relates something to a document that is mainly about it."@en .

:hasEconomicalFramework a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contratto;
  rdfs:range :ProceduraDiRealizzazione ;
  rdfs:label "has economical framework"@en ;
  rdfs:label "ha quadro economico"@it ;
  rdfs:comment "This property represents the economical framework of a Project, Intervention, Realization procedure, or Contract"@en ;
  rdfs:comment "Questa proprietà rappresenta il quadro economico di un Progetto, Intervento, Procedura di realizzazione, o Contratto."@it .

:changedBy a owl:ObjectProperty ;
    owl:inverseOf :originalFramework ;
    rdfs:domain :ProceduraDiRealizzazione ;
    rdfs:range  :EventoDiCambiamento ;
    rdfs:label "changed by"@en;
    rdfs:label "cambiata da"@it;
    rdfs:comment "Indicates a Change event which resulted in a change to this Economical Framework."@en ;
    rdfs:comment "Indica un evento che ha contribuito al cambiamento di questo Quadro economico."@it .

:originalFramework rdf:type owl:ObjectProperty ;
                     rdfs:domain :EventoDiCambiamento ;
                     rdfs:range :ProceduraDiRealizzazione ;
                     rdfs:comment "Questa proprietà collega un evento del cambiamento al quadro economico che esisteva prima del cambiamento.";
                     rdfs:label "quadro economico di origine"@it ,
                                "original economical framework"@en .

:resultingFramework a owl:ObjectProperty ;
  rdfs:domain :EventoDiCambiamento ;
  rdfs:range :ProceduraDiRealizzazione ;
  rdfs:label "resulting framework"@en ;
  rdfs:label "quadro risultante"@it ;
  rdfs:comment "Indicates an economical framework which was created or changed as a result of the event."@en ;
  rdfs:comment "Indica un quadro economico creato o ridefinito in seguito a un evento di cambiamento."@it .

:resultedFrom rdf:type owl:ObjectProperty ;
                owl:inverseOf :resultingFramework ;
                rdfs:domain :ProceduraDiRealizzazione ;
                rdfs:range :EventoDiCambiamento ;
                rdfs:comment "Questa proprietà è l'inversa di quadro risultante e lega un quadro economico all'evento di cambiamento attraverso cui il quadro si è definito."@it ;
                rdfs:label "resulted from"@en ,
                           "risultante da"@it .

:haProgetto a owl:ObjectProperty ;
  rdfs:domain :Intervento ;
  rdfs:range :Progetto ;
  rdfs:label ""@en ;
  rdfs:label "ha progetto"@it .

:isProjectFor a owl:ObjectProperty ;
  rdfs:domain :Progetto ;
  rdfs:range :Intervento ;
  rdfs:label ""@en ;
  rdfs:label "è progetto di"@it .

:haTipoBando a owl:ObjectProperty ;
  rdfs:domain :Bando;
  rdfs:range :ContractNoticeType ;
  rdfs:label ""@en ;
  rdfs:label "ha tipo bando"@it ;
  rdfs:comment "La proprietà che indica il tipo di bando."@it ;
  rdfs:comment "The property that determines the Contract Notice type."@en .

###  http://dati.gov.it/onto/l0/description
l0:description a owl:DatatypeProperty ;
               rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/identifier
l0:identifier a owl:DatatypeProperty ;
              rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/name
l0:name a owl:DatatypeProperty ;
        rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/tiapit/date
ti:date a owl:DatatypeProperty ;
        rdfs:isDefinedBy <htto://dati.gov.it/onto/tiapit/> .


#################################################################
#    Data properties
#################################################################

:actualEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:date ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui l'intervento è stato realizzato."@it ;
  rdfs:label "Data effettiva realizzazione intervento"@it ;
  rdfs:label ""@en .

:startDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:startTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:label "Data di inizio intervento"@it ;
  rdfs:label "Start Date"@en ;
  rdfs:comment "The property that detemins the beginning date."@en ;
  rdfs:comment "La proprietà che determina la data effettiva di inizio dell'intervento."@en ;
  rdfs:range xsd:date .

:dataAggiudicazioneAffidamento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:date ;
  rdfs:label ""@en ;
  rdfs:label "data di aggiudicazione/affidamento"@it ;
  rdfs:comment "Proprietà che indica la data di aggiudicazione a un offerente o affidamento." @it ;
  rdfs:comment "" @en .

  :publicationDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:issued ;
    rdfs:domain :Bando;
    rdfs:range xsd:date ;
      rdfs:label "data pubblicazione"@it ;
    rdfs:label "publication date"@en ;
    rdfs:comment "La proprietà che indica la data di pubblicazione del Bando/Avviso."@it ;
    rdfs:comment "The property that determines the publication date of the Contract Notice."@en .

  :expireDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:issued ;
    rdfs:domain :Bando;
    rdfs:range xsd:date ;
    rdfs:label "expire date"@en ;
    rdfs:label "data di scadenza"@it ;
    rdfs:comment "Proprietà che determina la data di scadenza del Bando/Avviso."@it ;
   rdfs:comment "The property that determines the expire date."@en .

   :ultimaDataStimataTermineIntervento a owl:DatatypeProperty ;
     rdfs:subPropertyOf ti:endTime ;
     rdfs:domain :ProceduraDiRealizzazione ;
     rdfs:label "ultima data stimata realizzazione intervento"@it ;
     rdfs:label ""@en ;
#     rdfs:comment "The property that detemins the estimated end date."@en ;
     rdfs:comment "La proprietà che determina l'ultima data stimata per il completamento dell'intervento."@it ;
     rdfs:range xsd:date .

   :dataStimataRealizzazioneIntervento a owl:DatatypeProperty ;
     rdfs:subPropertyOf ti:endTime ;
     rdfs:domain :ProceduraDiRealizzazione ;
     rdfs:range xsd:float ;
     rdfs:label "data stimata realizzazione intervento"@it ;
     rdfs:label ""@en ;
#     rdfs:comment "The property that determines the estimated end date."@en ;
     rdfs:comment "Questa proprietà determina la data stimata di realizzazione dell'intervento, nell'ambito di una versione del quadro economico."@it .

:dataCertificatoPagamento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :CertificatoPagamento ;
  rdfs:range xsd:date ;
  rdfs:label ""@en ;
  rdfs:label "data del certificato di pagamento"@it ;
  rdfs:comment "" @it ;
  rdfs:comment "" @en .

:importoPagamento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :CertificatoPagamento ;
  rdfs:range xsd:date ;
  rdfs:label ""@en ;
  rdfs:label "importo del pagamento"@it ;
  rdfs:comment "" @it ;
  rdfs:comment "" @en .





:agreedPrice a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "prezzo concordato"@it ;
  rdfs:label "agreed price"@en ;
  rdfs:comment "La proprietà che determina il prezzo concordato (anche detto importo di aggiudicazione, nel caso di gare con bando) per la realizzazione dell'intervento."@it ;
  rdfs:comment ""@en .

:nonDiscountableSum a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "somma non ribassabile"@it ;
  rdfs:label "sum of non-discountable money"@en ;
  rdfs:comment "The property that determines the amount of the starting price that  is not lowerable."@en ;
  rdfs:comment "La proprietà che determina la somma di denaro che non è ribassabile."@it .

:reductionPercentage a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "percentuale di ribasso"@it ;
  rdfs:label "reduction percentage"@en ;
  rdfs:comment "La proprietà che determina la percentuale dichiarata di ribasso."@it ;
  rdfs:comment "The property that determines the declared percentage of price cut."@en .

:auctionBaseAmount a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "importo base asta"@it ;
  rdfs:label "auction base amount"@en ;
  rdfs:comment "La proprietà che determina l'importo base dell'asta."@it ;
  rdfs:comment "The property that determines the auction base amount."@en .

:safetyAmount a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "importo sicurezza"@it ;
  rdfs:label "safety amount"@en ;
  rdfs:comment "La proprietà che determina l'importo stabilito destinato alla sicurezza."@it ;
  rdfs:comment "The property that determines the amount of money to be spent for safety reason."@en .

:giorniAggiuntivi a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:float ;
  rdfs:label "giorni aggiuntivi" @it ;
  #rdfs:label "additional days" @en ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà indica i giorni aggiuntivi necessari per il completamento dell'intervento, indicati nella variante."@it.

:dataVariante a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:date ;
  rdfs:label "data variante" @it ;
  #rdfs:label "" @en ;
  rdfs:comment ""@en ;
  rdfs:comment ""@it.

:tipoVariazione a owl:DatatypeProperty ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:string ;
  rdfs:label "tipo variazione" @it ;
  #rdfs:label "variant type" @en ;
#rdfs:comment "The property that determines contract variant type."@en ;
  rdfs:comment "La proprietà che determina il tipo di variante del contratto."@it .


