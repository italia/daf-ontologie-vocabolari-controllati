  rdfs:range xsd:float ;
  rdfs:label "importo sicurezza"@it ;
  rdfs:label "safety amount"@en ;
  rdfs:comment "La proprietà che determina l'importo dedicato alla sicurezza stabilito nel lotto oppure nel bando"@it ;
  rdfs:comment "The property that determines the amount of money to be spent for safety reason as stated in the lot or in the ContractNotice."@en .

:awardAmount a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "importo di aggiudicazione"@it ;
  rdfs:label "award amount"@en ;
  rdfs:comment "La proprietà che determina l'importo di aggiudicazione stabilito nel lotto"@it ;
  rdfs:comment "The property that determines the award amount of money as stated in the Lot."@en .

:winDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:date ;
  rdfs:label "win date"@en ;
  rdfs:label "data di aggiudicazione"@it ;
  rdfs:comment "Proprietà che indica la data di aggiudicazione di un contratto a un offerente" @it ;
  rdfs:comment "Property for date of contract award to a bidder." @en .

:publicationDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:issued ;
  rdfs:domain :ContractNotice ;
  rdfs:range xsd:date ;
