#################################################################
#    Annotation properties
#################################################################

###  http://dati.gov.it/onto/l0/controlledVocabulary
l0:controlledVocabulary a owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#float
xsd:float a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean a rdfs:Datatype .



#################################################################
#    Object Properties
#################################################################


:haEsito a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :EsitoProcedura ;
  rdfs:label "ha esito"@it ;
  rdfs:label "" @it .

:haAtto a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Documento ;
  rdfs:label "ha Atto"@it ;
  rdfs:label "" @en .

:haCriterioDiAggiudicazione a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :CriterioDiAggiudicazione ;
  rdfs:label "ha criterio di aggiudicazione"@it ;
  #rdfs:label "type of Award" @en ;
#  rdfs:comment "The property that determines the type of award of the contract notice."@en ;
  rdfs:comment "La proprietà che determina il tipo di aggiudicazione del bando."@it .

:realizza a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Intervento ;
  rdfs:label "realizza"@it ;
  rdfs:label "" @en .
  rdfs:comment ""@en ;
  rdfs:comment "La proprietà che lega una procedura di realizzazione a un intervento che realizza."@it .



:haAccordoQuadro a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :AccordoQuadro ;
  rdfs:label "ha Accordo quadro"@it ;
  rdfs:label "" @en .

:haContratto a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Contratto;
  rdfs:label "ha contratto"@it ;
  rdfs:label "" @en ;
#  rdfs:comment "The property that determines the link between a contract ad the realtive Implementation procedure."@en ;
  rdfs:comment "La proprietà che determina il legame tra la procedura di realizzazione e il relativo contratto."@it .

:haDeterminaAContrarre a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :DeterminaAContrarre ;
  rdfs:label "ha determina a contrarre"@it ;
  #rdfs:label "has Determina a contrarre" @en ;
  #rdfs:comment "The property that determines the link between an Implementation procedure and a Determina a contrarre."@en ;
  rdfs:comment "La proprietà che determina il legame tra la procedura di realizzazione e una determina a contrarre relativa."@it .

:stazioneAppaltante a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range covapit:Organization ;
  rdfs:label "ha stazione appaltante"@it ;
  #rdfs:label "contracting Authority"@en ;
#  rdfs:comment "Institution which issues a public contract, receives tenders to the contract and chooses a suitable supplier on the base of the conditions given by the contract."@en ;
  rdfs:comment "La proprietà che indica l'istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto."@it .

:haBando a owl:ObjectProperty ;
  rdfs:subPropertyOf :haAtto ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :Bando ;
  rdfs:label "ha bando" @it ;
#  rdfs:label "has contract notice" @en ;
#  rdfs:comment "The property that determines if the related contract belongs to a contract notice."@en .
  rdfs:comment "La proprietà che indica se la procedura di realizzazione appartiene a un bando o avviso."@it .

:haSceltaContraente a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :ModalitaSceltaContraente ;
  rdfs:label "ha scelta contraente"@it ;
  #rdfs:label "has type of procedure"@en ;
# rdfs:comment "Type of procedure of a public contract specifies a procedure which is applied to publish information about the contract and choose a supplier."@en
  rdfs:comment "La proprietà che specifica criteri di scelta di un contraente validi in una procedura di realizzazione."@it .

:offertaAggiudicataria a owl:ObjectProperty ;
  rdfs:domain :EsitoProcedura ;
  rdfs:range :Tender ;
  rdfs:label "ha offerta aggiudicataria"@it ;
#  rdfs:label "has awarded tender"@en ;
  rdfs:comment "Property for awarded tender submitted by a bidder."@en ;
  rdfs:comment "La proprietà che indica l'offerta aggiudicataria presentata da un offerente."@it .

:haDocumentoModifica a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contratto;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "ha documento di modifica"@it ;
#  rdfs:label "has Change document"@en ;
  rdfs:comment "La proprietà che indica una modifica alle caratteristiche di un Progetto, Intervento, Procedura di realizzazione o Contratto."@it ;
  rdfs:comment ""@en .

:haCertificatoDiPagamento a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :CertificatoPagamento ;
  rdfs:label "ha certificato di pagamento"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:deliberatoInSAL a owl:ObjectProperty ;
  rdfs:domain :CertificatoPagamento ;
  rdfs:range :SAL ;
  rdfs:label "deliberato in SAL"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:haSAL a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range :SAL ;
  rdfs:label "ha SAL"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:haEsito a owl:ObjectProperty ;
  rdfs:domain :EsitoProcedura ;
  rdfs:range :EsitoAggiudicazione ;
  rdfs:label "ha esito"@it ;
  #rdfs:label "has Contract Notice outcome "@en ;
#rdfs:comment "The property that determines the Contract Notice outcome."@en ;
  rdfs:comment "La proprietà che indica l'esito del processo di aggiudicazione."@it .

:haRisoluzione a owl:ObjectProperty ;
  rdfs:domain :Contratto ;
  rdfs:range :Risoluzione ;
  rdfs:label "ha risoluzione"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:offerente a owl:ObjectProperty ;
  rdfs:domain :Tender ;
  rdfs:range covapit:Organization ;
  #rdfs:label "bidder"@en ;
  rdfs:label "Offerente"@it ;
  #rdfs:comment "This property represents the bidder of a Tender"@en ;
  rdfs:comment "Questa proprietà rappresenta l'offerente di una offerta."@it .

:isPrimaryTopicOf
  rdfs:domain :EventoCambiamento , :Risoluzione ;
  rdfs:range :DocumentoDiModifica ;
  rdfs:label "è argomento principale di" @it ;
  #rdfs:label "is primary topic of" @en ;
  rdfs:comment "La proprietà collega un evento (per esempio un evento di cambiamento del propetto economico, oppure un evento di risoluzione di un contratto) al documento di modifica che ne descrive le caratteristiche."@it ;
  rdfs:comment ""@en .

:haProspettoEconomico a owl:ObjectProperty ;
  rdfs:domain :ProceduraDiRealizzazione , :Progetto , :Intervento , :Contratto;
  rdfs:range :ProspettoEconomico ;
  #rdfs:label "has economical framework"@en ;
  rdfs:label "ha prospetto economico"@it ;
  #rdfs:comment "This property represents the economical framework of a Project, Intervention, Realization procedure, or Contract"@en ;
  rdfs:comment "Questa proprietà indica il prospetto economico di un progetto, intervento, procedura di realizzazione, o contratto."@it .

:originalFramework rdf:type owl:ObjectProperty ;
                     rdfs:domain :EventoCambiamento ;
                     rdfs:range :ProspettoEconomico ;
                     rdfs:comment "Questa proprietà è l'inversa di \"cambiata da\" e collega un evento di cambiamento al prospetto economico precedente al cambiamento stesso.";
                     rdfs:label "Prospetto economico di origine"@it ,
                                "original economical framework"@en .

:changedBy a owl:ObjectProperty ;
    owl:inverseOf :originalFramework ;
    rdfs:domain :ProspettoEconomico ;
    rdfs:range  :EventoCambiamento ;
    rdfs:label "changed by"@en;
    rdfs:label "cambiata da"@it;
    rdfs:comment "Indicates a Change event which resulted in a change to this economical framework."@en ;
    rdfs:comment "Questa proprietà è l'inversa di \"prospetto economico di origine\" e indica un evento che ha contribuito al cambiamento di questo prospetto economico."@it .

:resultingFramework a owl:ObjectProperty ;
  rdfs:domain :EventoCambiamento ;
  rdfs:range :ProspettoEconomico ;
  rdfs:label "resulting framework"@en ;
  rdfs:label "prospetto risultante"@it ;
  rdfs:comment "Indicates an economical framework which was created or changed as a result of the event."@en ;
  rdfs:comment "Questa proprietà è l'inversa di \"risultante da\" e indica un prospetto economico creato o ridefinito in seguito a un evento di cambiamento."@it .

:resultedFrom rdf:type owl:ObjectProperty ;
                owl:inverseOf :resultingFramework ;
                rdfs:domain :ProspettoEconomico ;
                rdfs:range :EventoCambiamento ;
                rdfs:comment "Questa proprietà è l'inversa di \"prospetto risultante\" e lega un prospetto economico all'evento di cambiamento che lo ha generato."@it ;
                rdfs:label "resulted from"@en ,
                           "risultante da"@it .

:haProgetto a owl:ObjectProperty ;
  rdfs:domain :Intervento ;
  rdfs:range :Progetto ;
  rdfs:label ""@en ;
  rdfs:label "ha progetto"@it ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà è l'inversa di \"è progetto di\" e lega un intervento a un progetto."@it .

:isProjectFor a owl:ObjectProperty ;
  rdfs:domain :Progetto ;
  rdfs:range :Intervento ;
  rdfs:label ""@en ;
  rdfs:label "è progetto per"@it ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà è l'inversa di \"ha progetto\" e lega un progetto a un intervento."@it .

:haTipoBando a owl:ObjectProperty ;
  rdfs:domain :Bando;
  rdfs:range :TipoBando ;
  rdfs:label ""@en ;
  rdfs:label "ha tipo bando"@it ;
  #  rdfs:comment "The property that determines the Contract Notice type."@en .
  rdfs:comment "La proprietà che indica il tipo di bando."@it .

:motivazioneVariante a owl:ObjectProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :DocumentoVariante;
  rdfs:range :MotivazioneVariante ;
  rdfs:label "ha motivazione"@it ;
  #rdfs:label "has motivation"@it ;
  rdfs:comment "The property that links the contract variant to the identifier within the controlled vocabulary of variant reasons."@en ;
  rdfs:comment "La proprietà che collega la variante della procedura di realizzazione all'identificativo nel vocabolario controllato delle motivazioni delle varianti."@it .

:motivazioneSospensione a owl:ObjectProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :DocumentoSospensione;
  rdfs:range :MotivazioneSospensione ;
  rdfs:label "ha motivazione"@it ;
  #rdfs:label "has motivation"@it ;
  #rdfs:comment "The property that links the contract variant to the identifier within the controlled vocabulary of variant reasons."@en ;
  rdfs:comment "La proprietà che collega la sospensione del contratto all'identificativo nel vocabolario controllato delle motivazioni delle sospensioni."@it .
  #TODO compilare vocabolario controllato delle motivazioni delle sospensioni

:motivazioneRisoluzione a owl:ObjectProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :DocumentoRisoluzione;
  rdfs:range :MotivazioneRisoluzione ;
  rdfs:label "ha motivazione"@it ;
  #rdfs:label "has motivation"@it ;
  #rdfs:comment ""@en ;
  rdfs:comment "La proprietà che collega la risoluzione del contratto all'identificativo nel vocabolario controllato delle motivazioni delle risoluzioni."@it .
  #TODO compilare vocabolario controllato delle motivazioni delle risoluzioni

:cpv a owl:ObjectProperty ;
  rdfs:subPropertyOf l0:identifier ;
  rdfs:domain :Intervento, :ProceduraDiRealizzazione, :Bando, :Contratto ;
  rdfs:range :CPV ;
  rdfs:label "cpv"@it ;
  rdfs:label "cpv"@en ;
  rdfs:comment "La proprietà che associa un intervento, procedura di realizzazione, bando o contratto a un identificativo del vocabolario controllato dei CPV (Common Procurement Vocabulary), il sistema di classificazione per gli appalti pubblici."@it ;
  rdfs:comment "The property that links the Procedura di realizzazione to the identifier within the controlled vocabulary of CPV (Common Procurement Vocabulary), code describing the main object of a public contract."@en .

###  http://dati.gov.it/onto/l0/description
l0:description a owl:DatatypeProperty ;
               rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/identifier
l0:identifier a owl:DatatypeProperty ;
              rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/name
l0:name a owl:DatatypeProperty ;
        rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/tiapit/date
ti:date a owl:DatatypeProperty ;
        rdfs:isDefinedBy <htto://dati.gov.it/onto/tiapit/> .


#################################################################
#    Data properties
#################################################################

:dataFineIntervento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :Intervento ;
  rdfs:range xsd:date ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui l'intervento è terminato."@it ;
  rdfs:label "Data fine intervento"@it ;
  rdfs:label ""@en .

:dataFineProcedura a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:date ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui la procedura è terminata. Può essere dedotta dalla fase di collaudo."@it ;
  rdfs:label ""@en ;
  rdfs:label "Data fine procedura"@it .

:dataInizioIntervento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:startTime ;
  rdfs:domain :Intervento ;
  rdfs:label "Data di inizio intervento"@it ;
  #rdfs:label "Start Date"@en ;
  #rdfs:comment "The property that detemins the beginning date."@en ;
  rdfs:comment "La proprietà che determina la data effettiva di inizio dell'intervento."@en ;
  rdfs:range xsd:date .

:dataInizioProcedura a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:startTime ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:label "Data di inizio procedura"@it ;
  #rdfs:label "Start Date"@en ;
  #rdfs:comment "The property that detemins the beginning date."@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui la procedura è iniziata. Può essere dedotta dalla fase di collaudo."@en ;
  rdfs:range xsd:date .


:dataAggiudicazione a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :EsitoProcedura ;
  rdfs:range xsd:date ;
  rdfs:label ""@en ;
  rdfs:label "data di aggiudicazione"@it ;
  rdfs:comment "Proprietà che indica la data di aggiudicazione o affidamento a un offerente." @it ;
  rdfs:comment "" @en .

  :publicationDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:issued ;
    rdfs:domain :Bando ;
    rdfs:range xsd:date ;
      rdfs:label "data pubblicazione"@it ;
#    rdfs:label "publication date"@en ;
    rdfs:comment "La proprietà che indica la data di pubblicazione del bando o dell'avviso."@it ;
    rdfs:comment "The property that determines the publication date of the Contract Notice."@en .

  :expireDate a owl:DatatypeProperty ;
    rdfs:subPropertyOf ti:issued ;
    rdfs:domain :Bando ;
    rdfs:range xsd:date ;
#    rdfs:label "expire date"@en ;
    rdfs:label "data di scadenza"@it ;
    rdfs:comment "Proprietà che determina la data di scadenza del Bando."@it ;
   rdfs:comment "The property that determines the expire date."@en .

   :attualePrevisioneFineProcedura a owl:DatatypeProperty ;
     rdfs:subPropertyOf ti:endTime ;
     rdfs:domain :ProceduraDiRealizzazione ;
     rdfs:label "attuale previsione fine procedura"@it ;
     rdfs:label ""@en ;
#     rdfs:comment "The property that detemins the estimated end date."@en ;
     rdfs:comment "La proprietà che indica la data entro la quale si prevede di portare a termine la procedura di realizzazione di un intervento. Si deduce tale data dall'attuale prospetto economico della procedura di realizzazione, o comunque si intende che essa indica la più recente e valida previsione fatta."@it ;
     rdfs:range xsd:date .

   :PrevisioneFineProcedura a owl:DatatypeProperty ;
     rdfs:subPropertyOf ti:endTime ;
     rdfs:domain :ProspettoEconomico ;
     rdfs:range xsd:float ;
     rdfs:label "previsione fine procedura"@it ;
     rdfs:label ""@en ;
#     rdfs:comment "The property that determines the estimated end date."@en ;
     rdfs:comment "La proprietà che indica la data entro la quale si prevede di portare a termine un progetto, un intervento, una procedura di realizzazione o un contratto. La data è indicata nell'ambito di una versione del prospetto economico, e può aggiornare (o essere aggiornata da) quella indicata nel prospetto economico precedente (o successivo)."@it .

:dataCertificatoPagamento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :CertificatoPagamento ;
  rdfs:range xsd:date ;
  rdfs:label ""@en ;
  rdfs:label "data del certificato di pagamento"@it ;
  rdfs:comment "" @it ;
  rdfs:comment "" @en .

:importoPagamento a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :CertificatoPagamento ;
  rdfs:range xsd:date ;
  rdfs:label ""@en ;
  rdfs:label "importo del pagamento"@it ;
  rdfs:comment "" @it ;
  rdfs:comment "" @en .

:prezzoConcordato a owl:DatatypeProperty ;
  rdfs:domain :EsitoProcedura ;
  rdfs:range xsd:float ;
  rdfs:label "prezzo concordato"@it ;
  #rdfs:label "agreed price"@en ;
  rdfs:comment "La proprietà che determina il prezzo concordato (anche detto importo di aggiudicazione, nel caso di gare con bando) per la realizzazione dell'intervento."@it ;
  rdfs:comment ""@en .

:sommaNonRibassabile a owl:DatatypeProperty ;
  rdfs:domain :ProspettoEconomico ;
  rdfs:range xsd:float ;
  rdfs:label "somma non ribassabile"@it ;
  #rdfs:label "sum of non-discountable money"@en ;
  rdfs:comment "The property that determines the amount of the starting price that  is not lowerable."@en ;
  rdfs:comment "La proprietà che determina la somma di denaro che non è ribassabile."@it .

:percentualeRibasso a owl:DatatypeProperty ;
  rdfs:domain :EsitoProcedura ;
  rdfs:range xsd:float ;
  rdfs:label "percentuale di ribasso"@it ;
  #rdfs:label "reduction percentage"@en ;
  rdfs:comment "La proprietà che determina la percentuale dichiarata di ribasso."@it ;
  rdfs:comment "The property that determines the declared percentage of price cut."@en .

:importoBaseAsta a owl:DatatypeProperty ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:float ;
  rdfs:label "importo base asta"@it ;
  #rdfs:label "auction base amount"@en ;
  rdfs:comment "La proprietà che determina l'importo base dell'asta."@it ;
  rdfs:comment "The property that determines the auction base amount."@en .

:giorniAggiuntivi a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:float ;
  rdfs:label "giorni aggiuntivi" @it ;
  #rdfs:label "additional days" @en ;
  rdfs:comment ""@en ;
  rdfs:comment "Questa proprietà indica i giorni aggiuntivi necessari per il completamento dell'intervento, indicati nella variante."@it.

:dataVariante a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:date ;
  rdfs:label "data variante" @it ;
  #rdfs:label "" @en ;
  rdfs:comment ""@en ;
  rdfs:comment ""@it.

:tipoVariazione a owl:DatatypeProperty ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:string ;
  rdfs:label "tipo variazione" @it ;
  #rdfs:label "variant type" @en ;
#rdfs:comment "The property that determines contract variant type."@en ;
  rdfs:comment "La proprietà che determina il tipo di variante del contratto."@it .

:aumentoImporto a owl:DatatypeProperty ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:float ;
  rdfs:label "aumento importo"@it ;
  rdfs:label ""@en ;
  rdfs:comment "Questa proprietà indica l'importo aggiuntivo, indicato nella variante, necessario per il completamento dell'intervento."@it ;
  rdfs:comment ""@en .

:diminuzioneImporto a owl:DatatypeProperty ;
  rdfs:domain :DocumentoVariante;
  rdfs:range xsd:float ;
  rdfs:label "diminuzione importo"@it ;
  rdfs:label ""@en ;
  rdfs:comment ""@it ;
  rdfs:comment ""@en .

:dataSospensione a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :DocumentoSospensione;
  rdfs:range xsd:date ;
  rdfs:label "data sospensione" @it ;
  #rdfs:label "" @en ;
  rdfs:comment ""@en ;
  rdfs:comment ""@it.

:dataRipresa a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :DocumentoSospensione;
  rdfs:range xsd:date ;
  rdfs:label "data ripresa" @it ;
  #rdfs:label "" @en ;
  rdfs:comment ""@en ;
  rdfs:comment ""@it .

:cig a owl:DatatypeProperty ;
  rdfs:subPropertyOf l0:identifier ;
  rdfs:domain :ProceduraDiRealizzazione ;
  rdfs:range xsd:string ;
