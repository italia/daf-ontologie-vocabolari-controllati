  rdfs:comment "The property that determines the publication date of the Contract Notice."@en .

:expireDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:issued ;
  rdfs:domain :ContractNotice ;
  rdfs:range xsd:date ;
  rdfs:label "expire date"@en ;
  rdfs:label "expire date"@it ;
  rdfs:comment "Proprietà che determina la data di scadenza del Bando/Avviso."@it ;
 rdfs:comment "The property that determines the expire date."@en .

:additionalDays a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :Variant ;
  rdfs:range xsd:float ;
  rdfs:label "giorni aggiuntivi" @it ;
  rdfs:label "additional days" @en ;
  rdfs:comment "The property that determines additional days needed in the variant."@en ;
  rdfs:comment "Questa proprietà indica i giorni aggiuntivi necessari per la variante."@it.

:variantType a owl:DatatypeProperty ;
  rdfs:domain :Variant ;
  rdfs:range xsd:string ;
  rdfs:label "tipo variante" @it ;
  rdfs:label "variant type" @en ;
  rdfs:comment "La proprietà che determina il tipo di variante del contratto."@it ;
  rdfs:comment "The property that determines contract variant type."@en .

:hasReason a owl:DatatypeProperty ;
#  l0:controlledVocabulary <http://> ;
