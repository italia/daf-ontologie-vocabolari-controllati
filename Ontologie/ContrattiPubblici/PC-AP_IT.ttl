@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix l0: <http://dati.gov.it/onto/l0/> .
@prefix ti: <http://dati.gov.it/onto/tiapit> .
@prefix covapit: <http://dati.gov.it/onto/covapit/> .
@prefix : <http://w3id.org/italia/onto> .
@prefix schema: <http://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@base <http://contrattipubblici.org/csi/> .

<http://w3id.org/italia/onto/pcapit> a owl:Ontology ;
                                  owl:versionIRI : ;
                                  owl:imports ti: ,
                                              l0: ,
                                              covapit: ;
                                  dc:creator "Synapta Srl"@en ;
                                  dc:creator "CSI Piemonte"@en ;
                                  rdfs:comment "This is the ontology of the Italian public contracts."@en ;
                                  rdfs:comment "Questa è l'ontologia dei contratti pubblici italiani."@it ;
                                  dct:issued "2018-04-27"^^xsd:date ;
                                  rdfs:label "Ontologia dei contratti pubblici italiani"@it ;
                                  rdfs:label "Italian public contracts ontology"@en ;
                                  owl:versionInfo "Versione 0.1 - 27 Aprile 2018"@it ;
                                  owl:versionInfo   "instabile"@it , "unstable"@en ;
                                  rdfs:isDefinedBy : ;
                                  dct:license <https://creativecommons.org/licenses/by/4.0/> . # Da definirsi


#################################################################
#    Annotation properties
#################################################################

###  http://dati.gov.it/onto/l0/controlledVocabulary
l0:controlledVocabulary a owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#float
xsd:float a rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean a rdfs:Datatype .



#################################################################
#    Object Properties
#################################################################


:hasDocument a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :Document ;
  rdfs:label "ha atto"@it ;
  rdfs:label "has document"@it ;
  rdfs:comment "The properties that links the procedure to the documents issued by the contracting authority during the course of the procedure." @en ;
  rdfs:comment "La proprietà che indica gli atti emessi dalla stazione appaltante nell'espletamento della procedura di realizzazione." @it .

:hasAwardCriteria a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :AwardCriteria ;
  rdfs:label "ha criterio di aggiudicazione"@it ;
  rdfs:label "has award criteria" @en ;
  rdfs:comment "The property that determines the award criteria of the procedure. This property should be used in the case of procedures that include public calls followed by an awarding."@en ;
  rdfs:comment "La proprietà che determina il criterio di aggiudicazione della procedura di realizzazione. Tale proprietà si applica solo nel caso di procedure che prevedono gare con bando e conseguente aggiudicazione, e fa riferimento a un vocabolario controllato."@it .

:actualizes a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :Intervention ;
  rdfs:label "realizza"@it ;
  rdfs:label "actualizes" @en ;
  rdfs:comment "The property that links the procedure to the intervention actualized by the procedure."@en ;
  rdfs:comment "La proprietà che lega una procedura di realizzazione a un intervento che realizza."@it .

:hasFrameworkAgreement a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :FrameworkAgreement ;
  rdfs:label "ha accordo quadro"@it ;
  rdfs:label "has framework agreement"@en ;
  rdfs:comment "The properties that links the procedure to its framework agreement." @en ;
  rdfs:comment "La proprietà che lega un'istanza di procedura di realizzazione a un accordo quadro o convenzione." @it .

:haContratto a owl:ObjectProperty ;
  rdfs:subPropertyOf :hasDocument ;
  rdfs:domain :Procedure ;
  rdfs:range :Contract;
  rdfs:label "ha contratto"@it ;
  rdfs:label "has contract" @en ;
  rdfs:comment "The property that determines the link between a contract and the relative procedure."@en ;
  rdfs:comment "La proprietà che determina il legame tra la procedura di realizzazione e il relativo contratto."@it .

:hasResolutionToContract a owl:ObjectProperty ;
  rdfs:subPropertyOf :hasDocument ;
  rdfs:domain :Procedure ;
  rdfs:range :ResolutionToContract ;
  rdfs:label "ha determina a contrarre"@it ;
  rdfs:label "has resolution to contract" @en ;
  rdfs:comment "The property that links the procedure to the document whereby the contracting authority expresses its resoultion to contract."@en ;
  rdfs:comment "La proprietà che determina il legame tra la procedura di realizzazione e una determina a contrarre relativa."@it .

:hasContractingAuthority a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range covapit:Organization ;
  rdfs:label "ha stazione appaltante"@it ;
  rdfs:label "has contracting authority"@en ;
  rdfs:comment "The property that determines the institution which issues a public contract, receives tenders and chooses a suitable supplier on the base of the conditions given by the contract."@en ;
  rdfs:comment "La proprietà che indica l'istituzione che emette il contratto pubblico, riceve le offerte e sceglie il fornitore idoneo sulla base delle condizioni definite dal contratto."@it .

:hasNotice a owl:ObjectProperty ;
  rdfs:subPropertyOf :hasDocument ;
  rdfs:domain :Procedure ;
  rdfs:range :Notice ;
  rdfs:label "ha bando" @it ;
  rdfs:label "has notice" @en ;
  rdfs:comment "The property that determines links the procedure to the related announcement or notice. More procedure may be linked to the same announcement or notice."@en ;
  rdfs:comment "La proprietà che indica il collegamento tra la procedura di realizzazione e un bando di gara o avviso. Diverse procedure di realizzazione possono fare riferimento ad un medesimo bando, allo stesso modo in cui, per quanto riguarda i dati applicativi SIMOG, più lotti (ciascuno dei quali corrisponde a una procedura di realizzazione) fanno riferimento allo stesso bando di gara."@it .

:hasProcedureType a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :ProcedureType ;
  rdfs:label "ha scelta contraente"@it ;
  rdfs:label "has type of procedure"@en ;
  rdfs:comment "The property that links the procedure to its type."@en ;
  rdfs:comment "La proprietà che specifica i criteri di scelta di un contraente validi in una procedura di realizzazione."@it .

:selectedTender a owl:ObjectProperty ;
  rdfs:domain :ProcedureOutcome ;
  rdfs:range :Tender ;
  rdfs:label "ha offerta selezionata"@it ;
  rdfs:label "has selected tender"@en ;
  rdfs:comment "The property that determines the selected tender submitted by a bidder."@en ;
  rdfs:comment "La proprietà che indica l'offerta selezionata per la realizzazione dell'intervento, in seguito a un processo di aggiudicazione, affidamento o adesione ad un accordo quadro."@it .

:hasChangeDocument a owl:ObjectProperty ;
  rdfs:domain :Procedure , :Project , :Intervention , :Contract ;
  rdfs:range :ChangeDocument ;
  rdfs:label "ha documento di modifica"@it ;
  rdfs:label "has change document"@en ;
  rdfs:comment "La proprietà che indica il documento che descrive una modifica alle caratteristiche di un Progetto, Intervento, Procedura di realizzazione o Contratto."@it ;
  rdfs:comment "The property that determines the document describing a change to the characteristics of a project, intervention, procedure or contract."@en .

:hasPaymentCertificate a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :PaymentCertificate ;
  rdfs:label "ha certificato di pagamento"@it ;
  rdfs:label "has certificate of payment"@en ;
  rdfs:comment "La proprietà che collega la procedura di realizzazione al certificato di pagamento deliberato in un SAL."@it ;
  rdfs:comment "The property that links the procedure to the certificate of payment approved by the related progress of work report."@en .

:approvedBy a owl:ObjectProperty ;
  rdfs:domain :PaymentCertificate ;
  rdfs:range :WorkProgressReport ;
  rdfs:label "deliberato in SAL"@it ;
  rdfs:label "approved by"@en ;
  rdfs:comment "La proprietà che collega il certificato di pagamento al SAL (Stato Avanzamento Lavori) documento che contiene la delibera al pagamento stesso."@it ;
  rdfs:comment "The property that links the certificate of payment to the work progress report whereby the payment is approved."@en .

:hasWorkProgressReport a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :WorkProgressReport ;
  rdfs:label "ha SAL"@it ;
  rdfs:label "has work progress report"@en ;
  rdfs:comment "La proprietà che collega la procedura di realizzazione al documento che ne certifica lo Stato di Avanzamento Lavori (SAL)."@it ;
  rdfs:comment "The property that links the procedure to its work progress report."@en .

:hasOutcome a owl:ObjectProperty ;
  rdfs:domain :Procedure ;
  rdfs:range :ProcedureOutcome ;
  rdfs:label "ha esito"@it ;
  rdfs:label "has outcome"@en ;
  rdfs:comment "The property that links the procedure to its outcome (open tender award, direct award or framework agreement)." @en ;
  rdfs:comment "La proprietà che collega la procedura di realizzazione all'esito (aggiudicazione, affidamento o accordo-quadro) della procedura stessa." @it .

:hasResolution a owl:ObjectProperty ;
  rdfs:domain :Contract ;
  rdfs:range :Resolution ;
  rdfs:label "ha risoluzione"@it ;
  rdfs:label "has resolution"@en ;
  rdfs:comment "La proprietà che collega il contratto all'evento che determina la risoluzione del contatto stesso"@it ;
  rdfs:comment "The property that links the contract to the event that determines its resolution."@en .

:bidder a owl:ObjectProperty ;
  rdfs:domain :Tender ;
  rdfs:range covapit:Organization ;
  rdfs:label "bidder"@en ;
  rdfs:label "offerente"@it ;
  rdfs:comment "The property that links the tender to its bidder."@en ;
  rdfs:comment "La proprietà che indica l'organizzazione offerente di un'offerta."@it .

:isPrimaryTopicOf
  rdfs:domain :ChangeEvent , :Resolution ;
  rdfs:range :ChangeDocument ;
  rdfs:label "è argomento principale di" @it ;
  rdfs:label "is primary topic of" @en ;
  rdfs:comment "La proprietà che collega un evento (per esempio un evento di cambiamento del propetto economico, oppure un evento di risoluzione di un contratto) al documento di modifica che ne descrive le caratteristiche."@it ;
  rdfs:comment "The property that links an event (e.g., an event that modifies the economic summary of a procedure or the event that determines the resolution of a contract) to the document describing the characteristics of the event."@en .

:hasEconomicSummary a owl:ObjectProperty ;
  rdfs:domain :Procedure , :Project , :Intervention , :Contract;
  rdfs:range :EconomicSummary ;
  rdfs:label "has economic summary"@en ;
  rdfs:label "ha prospetto economico"@it ;
  rdfs:comment "This property represents the economic summary of a project, intervention, procedure or contract."@en ;
  rdfs:comment "Questa proprietà indica il prospetto economico di un progetto, intervento, procedura di realizzazione, o contratto."@it .

:hasOriginalSummary rdf:type owl:ObjectProperty ;
  rdfs:domain :ChangeEvent ;
  rdfs:range :EconomicSummary ;
  rdfs:comment "Questa proprietà è l'inversa di \"cambiata da\" e collega un evento di cambiamento al prospetto economico precedente al cambiamento stesso." @it ;
  rdfs:comment "This property is the inverse of \"changed by\" and links a change event to the economic summary preceding the change event." @en ;
  rdfs:label "ha prospetto economico di origine" @it ;
  rdfs:label "has original summary" @en .

:changedBy a owl:ObjectProperty ;
 owl:inverseOf :hasOriginalSummary ;
 rdfs:domain :EconomicSummary ;
 rdfs:range  :ChangeEvent ;
 rdfs:label "changed by"@en;
 rdfs:label "cambiato da"@it;
 rdfs:comment "This property is the inverse of \"has original summary\" and determines the change event which resulted in a change to this economic summary."@en ;
 rdfs:comment "Questa proprietà è l'inversa di \"prospetto economico di origine\" e indica un evento che ha contribuito al cambiamento di questo prospetto economico."@it .

:hasResultingSummary a owl:ObjectProperty ;
  rdfs:domain :ChangeEvent ;
  rdfs:range :EconomicSummary ;
  rdfs:label "has resulting summary"@en ;
  rdfs:label "ha prospetto risultante"@it ;
  rdfs:comment "This property is the inverse of \"resulted from\" and determines the economic summary which was created or changed as a result of the change event."@en ;
  rdfs:comment "Questa proprietà è l'inversa di \"risultante da\" e indica un prospetto economico creato o ridefinito in seguito a un evento di cambiamento."@it .

:resultedFrom rdf:type owl:ObjectProperty ;
  owl:inverseOf :hasResultingSummary ;
  rdfs:domain :EconomicSummary ;
  rdfs:range :ChangeEvent ;
  rdfs:comment "Questa proprietà è l'inversa di \"prospetto risultante\" e lega un prospetto economico all'evento di cambiamento che lo ha ridefinito."@it ;
  rdfs:comment "This property is the inverse of \"has resulting summary\" and links an economic summary to the change event that ridefined it."@en ;
  rdfs:label "resulted from"@en ;
  rdfs:label "risultante da"@it .

:hasProject a owl:ObjectProperty ;
  rdfs:domain :Intervention ;
  rdfs:range :Project ;
  rdfs:label "has project"@en ;
  rdfs:label "ha progetto"@it ;
  rdfs:comment "This property is the inverse of \"is project for\" and links an intervention to a project."@en ;
  rdfs:comment "Questa proprietà è l'inversa di \"è progetto di\" e lega un intervento a un progetto."@it .

:isProjectFor a owl:ObjectProperty ;
  rdfs:domain :Project ;
  rdfs:range :Intervention ;
  rdfs:label "is project for"@en ;
  rdfs:label "è progetto per"@it ;
  rdfs:comment "This property is the inverse of \"has project\" and links a project to an intervention. "@en ;
  rdfs:comment "Questa proprietà è l'inversa di \"ha progetto\" e lega un progetto a un intervento."@it .

:hasNoticeType a owl:ObjectProperty ;
  rdfs:domain :Notice;
  rdfs:range :NoticeType ;
  rdfs:label "has notice type"@en ;
  rdfs:label "ha tipo di bando"@it ;
  rdfs:comment "The property that links a notice to its type."@en ;
  rdfs:comment "La proprietà che indica il tipo di bando."@it .

:variantReason a owl:ObjectProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :VariantDocument;
  rdfs:range :VariantReason ;
  rdfs:label "ha motivazione"@it ;
  rdfs:label "has variant reason"@en ;
  rdfs:comment "The property that links the variant to the identifier within the controlled vocabulary of variant reasons."@en ;
  rdfs:comment "La proprietà che collega la variante all'identificativo nel vocabolario controllato delle motivazioni delle varianti."@it .

:suspensionReason a owl:ObjectProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :SuspensionDocument ;
  rdfs:range :SuspensionReason ;
  rdfs:label "ha motivazione"@it ;
  rdfs:label "has suspension reason"@en ;
  rdfs:comment "The property that links the suspension to the identifier within the controlled vocabulary of suspension reasons."@en ;
  rdfs:comment "La proprietà che collega la sospensione all'dentificativo nel vocabolario controllato delle motivazioni delle sospensioni."@it .
  #TODO compilare vocabolario controllato delle motivazioni delle sospensioni

:resolutionReason a owl:ObjectProperty ;
#  l0:controlledVocabulary <http://> ;
  rdfs:domain :ResolutionDocument ;
  rdfs:range :ResolutionReason ;
  rdfs:label "ha motivazione"@it ;
  rdfs:label "has resolution motivation"@en ;
  rdfs:comment "The property that links the resolution to the identifier within the controlled vocabulary of resolution reasons."@en ;
  rdfs:comment "La proprietà che collega la risoluzione del contratto all'identificativo nel vocabolario controllato delle motivazioni delle risoluzioni."@it .
  #TODO compilare vocabolario controllato delle motivazioni delle risoluzioni

:hasCPV a owl:ObjectProperty ;
  rdfs:subPropertyOf l0:identifier ;
  rdfs:domain :Intervention, :Procedure, :Notice, :Contract ;
  rdfs:range :CPV ;
  rdfs:label "ha CPV"@it ;
  rdfs:label "has CPV"@en ;
  rdfs:comment "La proprietà che associa un intervento, procedura di realizzazione, bando o contratto a un identificativo del vocabolario controllato dei CPV (Common Procurement Vocabulary), il sistema di classificazione per gli appalti pubblici."@it ;
  rdfs:comment "The property that links an intervention, procedure, notice or contract to the identifier within the controlled vocabulary of CPV (Common Procurement Vocabulary), a code describing the main object of a public procurement contract."@en .

###  http://dati.gov.it/onto/l0/description
l0:description a owl:DatatypeProperty ;
               rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/identifier
l0:identifier a owl:DatatypeProperty ;
              rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/l0/name
l0:name a owl:DatatypeProperty ;
        rdfs:isDefinedBy l0: .


###  http://dati.gov.it/onto/tiapit/date
ti:date a owl:DatatypeProperty ;
        rdfs:isDefinedBy <htto://dati.gov.it/onto/tiapit/> .


#################################################################
#    Data properties
#################################################################

:interventionEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :Intervention ;
  rdfs:range xsd:date ;
  rdfs:comment "This property represents the actual end date of the intervention."@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui l'intervento è terminato."@it ;
  rdfs:label "data di fine intervento"@it ;
  rdfs:label "intervention end date"@en .

:procedureEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :Procedure ;
  rdfs:range xsd:date ;
  rdfs:comment "This property represents the actual end date of the procedure, and it may be imported from the results of the testing stage."@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui la procedura è terminata. Può essere dedotta dalle verifiche effettuate nel collaudo finale."@it ;
  rdfs:label "procedure end date"@en ;
  rdfs:label "data di fine procedura"@it .

:interventionStartDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:startTime ;
  rdfs:domain :Intervention ;
  rdfs:label "data di inizio intervento"@it ;
  rdfs:label "intervention start date"@en ;
  rdfs:comment "This property represents the actual start date of the intervention."@en ;
  rdfs:comment "La proprietà che determina la data effettiva di inizio dell'intervento."@it ;
  rdfs:range xsd:date .

:procedureStartDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:startTime ;
  rdfs:domain :Procedure ;
  rdfs:label "data di inizio procedura"@it ;
  rdfs:label "procedure start date"@en ;
  rdfs:comment "This property represents the actual start date of the procedure, and it may be imported from the results of the testing stage."@en ;
  rdfs:comment "Questa proprietà indica la data effettiva in cui la procedura è iniziata. Può essere dedotta dalle verifiche effettuate nel collaudo finale."@it ;
  rdfs:range xsd:date .

:outcomeDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :ProcedureOutcome ;
  rdfs:range xsd:date ;
  rdfs:label "outcome date"@en ;
  rdfs:label "data esito procedura"@it ;
  rdfs:comment "Proprietà che indica la data di aggiudicazione o affidamento a un offerente, o di adesione a un accordo quadro da parte di un offerente." @it ;
  rdfs:comment "This property represents the date of the outcome of the procedure (e.g. the date of the award to a bidder, or the date of the framework agreement)" @en .

:publicationDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:issued ;
  rdfs:domain :Notice ;
  rdfs:range xsd:date ;
  rdfs:label "data di pubblicazione"@it ;
  rdfs:label "publication date"@en ;
  rdfs:comment "La proprietà che indica la data di pubblicazione del bando o dell'avviso."@it ;
  rdfs:comment "The property that represents the publication date of the notice."@en .

:expireDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:issued ;
  rdfs:domain :Notice ;
  rdfs:range xsd:date ;
  rdfs:label "expire date"@en ;
  rdfs:label "data di scadenza"@it ;
  rdfs:comment "Proprietà che determina la data di scadenza del bando."@it ;
  rdfs:comment "The property that represents the expire date of the notice."@en .

:currentProcedureEstimatedEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :Procedure ;
  rdfs:label "attuale previsione di fine procedura"@it ;
  rdfs:label "current procedure estimated end date"@en ;
  rdfs:comment "The property that represents the estimated end date of the procedure. The date is imported from the last economic summary related to the procedure."@en ;
  rdfs:comment "La proprietà che indica la data entro la quale si prevede di portare a termine la procedura di realizzazione di un intervento. Si deduce tale data dall'attuale prospetto economico della procedura, o comunque si intende che essa indica la più recente e valida previsione fatta."@it ;
  rdfs:range xsd:date .

:estimatedEndDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:endTime ;
  rdfs:domain :EconomicSummary ;
  rdfs:range xsd:float ;
  rdfs:label "previsione di fine procedura"@it ;
  rdfs:label "procedure estimated end date"@en ;
  rdfs:comment "The property that represents the estimated end date of a project, intervention, procedure or contract. The date is valid within a version of the economic summary, and may update (or be updated by) the date reported by the former (or next) economic summary"@en ;
  rdfs:comment "La proprietà che indica la data entro la quale si prevede di portare a termine un progetto, un intervento, una procedura di realizzazione o un contratto. La data è indicata nell'ambito di una versione del prospetto economico, e può aggiornare (o essere aggiornata da) quella indicata nel prospetto economico precedente (o successivo)."@it .

:paymentCetificateDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:date ;
  rdfs:domain :PaymentCertificate ;
  rdfs:range xsd:date ;
  rdfs:label "payment certificate date"@en ;
  rdfs:label "data del certificato di pagamento"@it ;
  rdfs:comment "La proprietà che indica la data di emissione del certificato di pagamento da parte della stazione appaltante." @it ;
  rdfs:comment "The property that represents the date of the certificate of the payment." @en .

:paymentAmount a owl:DatatypeProperty ;
  rdfs:domain :PaymentCertificate ;
  rdfs:range xsd:date ;
  rdfs:label "amount of the payment"@en ;
  rdfs:label "importo del pagamento"@it ;
  rdfs:comment "La proprietà che determina l'importo del pagamento." @it ;
  rdfs:comment "The property that represents the amount of the payment." @en .

:agreedAmount a owl:DatatypeProperty ;
  rdfs:domain :ProcedureOutcome ;
  rdfs:range xsd:float ;
  rdfs:label "prezzo concordato"@it ;
  rdfs:label "agreed amount"@en ;
  rdfs:comment "La proprietà che determina il prezzo concordato (anche detto importo di aggiudicazione, nel caso di gare con bando) per la realizzazione dell'intervento."@it ;
  rdfs:comment "The property that represents the agreed amount for the actualization of the intervention."@en .

:nonDiscountableSum a owl:DatatypeProperty ;
  rdfs:domain :EconomicSummary ;
  rdfs:range xsd:float ;
  rdfs:label "somma non ribassabile"@it ;
  rdfs:label "sum of non-discountable money"@en ;
  rdfs:comment "The property that determines the amount of the starting price that is not lowerable."@en ;
  rdfs:comment "La proprietà che determina la somma di denaro che non è ribassabile."@it .

:reductionPercentage a owl:DatatypeProperty ;
  rdfs:domain :ProcedureOutcome ;
  rdfs:range xsd:float ;
  rdfs:label "percentuale di ribasso"@it ;
  rdfs:label "reduction percentage"@en ;
  rdfs:comment "La proprietà che determina la percentuale dichiarata di ribasso."@it ;
  rdfs:comment "The property that determines the declared percentage of price cut."@en .

:auctionBaseAmount a owl:DatatypeProperty ;
  rdfs:domain :Procedure ;
  rdfs:range xsd:float ;
  rdfs:label "importo base asta"@it ;
  rdfs:label "auction base amount"@en ;
  rdfs:comment "La proprietà che determina l'importo base dell'asta."@it ;
  rdfs:comment "The property that determines the auction base amount."@en .

:additionalDays a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:float ;
  rdfs:label "giorni aggiuntivi" @it ;
  rdfs:label "additional days" @en ;
  rdfs:comment "The property that represents the additional days needed to complete the intervention, as reported by the variant document."@en ;
  rdfs:comment "Questa proprietà indica i giorni aggiuntivi necessari per il completamento dell'intervento, indicati nel documento di variante."@it.

:variantDate a owl:DatatypeProperty ;
  rdfs:subPropertyOf ti:duration ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:date ;
  rdfs:label "data variante" @it ;
  rdfs:label "variant date" @en ;
  rdfs:comment "The property that represents the starting date of the variant."@en ;
  rdfs:comment "La proprietà che determina la data di inizio della variante."@it.

:variantType a owl:DatatypeProperty ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:string ;
  rdfs:label "tipo variazione" @it ;
  rdfs:label "variant type" @en ;
  rdfs:comment "The property that represents the type of the variant."@en ;
  rdfs:comment "La proprietà che determina il tipo di variante."@it .

:increaseAmount a owl:DatatypeProperty ;
  rdfs:domain :VariantDocument;
  rdfs:range xsd:float ;
  rdfs:label "aumento importo"@it ;
  rdfs:label "increase amount"@en ;
  rdfs:comment "Questa proprietà indica un aumento dell'importo necessario per il completamento dell'intervento, indicato nel documento di variante."@it ;
  rdfs:comment "The property thath represents the increase amount needed to complete the intervention, as reported by the variant document."@en .

:reductionAmount a owl:DatatypeProperty ;
  rdfs:domain :VariantDocument;
