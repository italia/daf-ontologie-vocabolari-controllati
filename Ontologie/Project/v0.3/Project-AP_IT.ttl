###  https://w3id.org/italia/onto/Project/projectAcronym
:projectAcronym rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf l0:name ;
                rdfs:domain :PublicInvestmentProject ;
                rdfs:range rdfs:Literal ;
                rdfs:comment "E' l'acronimo con cui identificare il progetto"@it ,
                             "It is the acronym of the project"@en ;
                rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
                rdfs:label "acronimo progetto"@it ,
                           "project acronym"@en ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


###  https://w3id.org/italia/onto/Project/projectKeyword
:projectKeyword rdf:type owl:DatatypeProperty ;
                rdfs:domain :PublicInvestmentProject ;
                rdfs:range rdfs:Literal ;
                rdfs:comment "Property that allows one to specify more than one keyword for a project."@en ,
                             "Proprietà che consente di specificare più di una parola chiave a un progetto."@it ;
                rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
                rdfs:label "parola chiave progetto"@it ,
                           "project keyword"@en ;
                owl:versionInfo "stabile - inclusa nella versione 0.2 dell'ontologia"@it ,
                                "stable - included in version 0.2 of the ontology"@en .


###  https://w3id.org/italia/onto/Project/projectTitle
:projectTitle rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf l0:name ;
              rdfs:domain :PublicInvestmentProject ;
              rdfs:range rdfs:Literal ;
              rdfs:comment "Il titolo, per esteso, del progetto"@it ,
                           "The full title of the project"@en ;
              rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
              rdfs:label "project title"@en ,
                         "titolo del progetto"@it ;
              owl:versionInfo "stabile"@it ,
                              "stable"@en .


###  https://w3id.org/italia/onto/Project/projectTotalCost
:projectTotalCost rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf <https://w3id.org/italia/onto/MU/value> ;
                  rdfs:domain :PublicInvestmentProject ;
                  rdfs:range xsd:float ;
                  rdfs:comment "Property that represents the total cost of the project"@en ,
                               "Proprietà che consente di rappresentare il costo totale del progetto"@it ;
                  rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
                  rdfs:label "costo totale progetto"@it ,
                             "project total cost"@en ;
                  owl:versionInfo "provvisoria"@it ,
                                  "unstable"@en .


###  https://w3id.org/italia/onto/Project/uniqueProjectCodeType
:uniqueProjectCodeType rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain :UniqueProjectCode ;
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:oneOf [ rdf:type rdf:List ;
                                                rdf:first "linked" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "master" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "normal" ;
                                                                      rdf:rest rdf:nil
                                                                    ]
                                                         ]
                                              ]
                                  ] ;
                       rdfs:comment "Il tipo associato al CUP del progetto. Esso può assumere uno dei seguenti tre valori: \"normale\" (normal), \"master\" (master), \"collegato\" (linked)"@it ,
                                    "The type of the unique code of the project (CUP acronym in Italian). It can assume one of the following three values: normal, master, linked"@en ;
                       rdfs:label "Tipo CUP"@it ,
                                  "Type of Unique Project Code"@en ;
                       owl:versionInfo "provvisoria"@it ,
                                       "unstable"@en .


###  https://w3id.org/italia/onto/Project/uniqueProjectCodeValue
:uniqueProjectCodeValue rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf l0:identifier ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :PublicInvestmentProject
                                                    :UniqueProjectCode
                                                  )
                                    ] ;
                        rdfs:range rdfs:Literal ;
                        rdfs:comment "It is the alfanumeric string representing the value of the unique code for the project (in Italian CUP acronym)"@en ,
                                     "La stringa alfanumerica che rappresenta il valore del Codice Unico di Progetto"@it ;
                        rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
                        rdfs:label "unique project code value"@en ,
                                   "valore del Codice Unico di Progetto"@it ;
                        owl:versionInfo "stabile"@it ,
                                        "stable"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/italia/onto/Project/Call
:Call rdf:type owl:Class ;
      rdfs:subClassOf l0:Object ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty l0:description ;
                        owl:someValuesFrom rdfs:Literal
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty l0:name ;
                        owl:someValuesFrom rdfs:Literal
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty l0:identifier ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ;
      rdfs:comment "L'insieme delle informazioni necessarie per invitare candidati a presentare proposte di progetti, con l'obiettivo di ricevere un finanziamento."@it ,
                   "The set of information required to invite applicants to submit project proposals, with the aim of receiving funding."@en ;
      rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
      rdfs:label "Bando"@it ,
                 "Call"@en ;
      owl:versionInfo "stabile - introdotta nella versione 0.3"@it ,
                      "stable - introduced in version 0.3"@en .


###  https://w3id.org/italia/onto/Project/ProcurementProject
:ProcurementProject rdf:type owl:Class ;
                    rdfs:subClassOf :PublicInvestmentProject ;
                    owl:disjointWith :PublicResearchProject ;
                    rdfs:comment "La classe che comprende i progetti atti a soddisfare un fabbisogno individuato dall'amministrazione. Il Project Procurement può essere spiegato come il raggruppamento di più appalti. Ad esempio, quando si progetta la costruzione di un aeroporto si possono prevedere più appalti, che possono finire in lotti diversi in una o più procedure: uno finalizzato all'acquisto dell'asfalto per le piste, un altro per l'appalto dei servizi di ristorazione, un altro per la costruzione degli edifici, ecc. Questi appalti possono essere avviati in tempi diversi (ad esempio, anni diversi)."@it ,
                                 "The class that includes projects aiming at meet the needs detected by a public administration. Project Procurement can be explained as the grouping of several procurements. For example, when planning the construction of an airport several procurements can be foreseen, which may end up in different lots in one or more procedures: one aimed at the purchasing of tarmac for the runways, another for the contracting of catering services, another for the construction of the buildings, etc.. These procurements may be launched in different times (different years, for example)."@en ;
                    rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
                    rdfs:label "Procurement Project"@en ,
                               "Progetto di approvigionamento"@it ;
                    owl:versionInfo "provvisoria"@it ,
                                    "unstable"@en .


###  https://w3id.org/italia/onto/Project/Programme
:Programme rdf:type owl:Class ;
           rdfs:subClassOf l0:Concept ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty l0:description ;
                             owl:someValuesFrom rdfs:Literal
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty l0:name ;
                             owl:someValuesFrom rdfs:Literal
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty l0:identifier ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:comment "A set of related measures or activities with a particular long-term aim, typically established by a funding agency. Often associated with publication of a call."@en ,
                        "Un insieme di misure o attività correlate con un particolare obiettivo a lungo termine, tipicamente stabilite da un'agenzia di finanziamento. Spesso associato alla pubblicazione di un invito a presentare proposte (bando)."@it ;
           rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
           rdfs:label "Programma"@it ,
                      "Programme"@en ;
           owl:versionInfo "stabile - introdotto nella versione 0.3"@it ,
                           "stable - introduced in version 0.3"@en .


###  https://w3id.org/italia/onto/Project/PublicInvestmentProject
:PublicInvestmentProject rdf:type owl:Class ;
                         rdfs:subClassOf l0:Description ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasProgramme ;
                                           owl:someValuesFrom :Programme
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :projectFunder ;
                                           owl:someValuesFrom <https://w3id.org/italia/onto/COV/Organization>
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty CLV:hasSpatialCoverage ;
                                           owl:allValuesFrom CLV:Feature
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasParticipantingAgent ;
                                           owl:allValuesFrom l0:Agent
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty <https://w3id.org/italia/onto/RO/hasRiT> ;
                                           owl:allValuesFrom <https://w3id.org/italia/onto/RO/TimeIndexedRole>
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty <https://w3id.org/italia/onto/TI/hasDuration> ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass <https://w3id.org/italia/onto/TI/Duration>
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasCall ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Call
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasSummary ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Summary
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :hasUniqueProjectCode ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :UniqueProjectCode
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty <https://w3id.org/italia/onto/SM/hasOnlineContactPoint> ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass <https://w3id.org/italia/onto/SM/OnlineContactPoint>
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :projectTitle ;
                                           owl:someValuesFrom rdfs:Literal
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :projectKeyword ;
                                           owl:allValuesFrom rdfs:Literal
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :uniqueProjectCodeValue ;
                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :projectAcronym ;
                                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                         ] ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :projectTotalCost ;
                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onDataRange xsd:float
                                         ] ;
                         rdfs:comment "La classe consente di rappresentare progetti di investimento pubblico. Un 'progetto d’investimento pubblico'  è un complesso di azioni o di strumenti di sostegno, afferenti a un medesimo quadro economico di spesa e collegati fra loro da quattro elementi: la presenza di un decisore pubblico; la previsione di un finanziamento (diretto o indiretto, totale o parziale) con risorse pubbliche; l’indicazione di un comune obiettivo di sviluppo economico e sociale; la definizione di un orizzonte temporale certo."@it ,
                                      "The class allows one to model a public investmnet project."@en ;
                         rdfs:isDefinedBy <https://w3id.org/italia/onto/Project> ;
                         rdfs:label "Progetto d'Investimento Pubblico"@it ,
                                    "Public Investment Project"@en ;
                         owl:versionInfo "stabile"@it ,
                                         "stable"@en .


###  https://w3id.org/italia/onto/Project/PublicResearchProject
:PublicResearchProject rdf:type owl:Class ;
                       rdfs:subClassOf :PublicInvestmentProject ,
