                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/AccessCondition/reasonClosure"></owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"></owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
