	rdfs:label "has allowed format"@en, "da pubblicare nel formato"@it;
	rdfs:comment "This property links a transparency resource to the allowed format defined for publication."@en,
		"Questa proprità lega una risorsa della trasparenza al formato definito per la pubblicazione"@it;
	rdfs:domain :TransparencyResource ;
	rdfs:range :Format ;
	rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency> ; 
