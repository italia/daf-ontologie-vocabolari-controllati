                          rdfs:subPropertyOf :hasActivityType .



###  https://w3id.org/italia/onto/COV/hasPublicOrganization

:hasPublicOrganization rdf:type owl:ObjectProperty ;

                       rdfs:label "has public organization"@en ,
                                  "ha organizzazione pubblica"@it ,
                                  "ha pubblica amministrazione"@it ;

                       rdfs:comment "Proprerty meant for linking any entity to a public organization."@en ,
                                    "Proprietà che collega una qualsiasi entità ad un organizzazione pubblica."@it ;

                       rdfs:domain owl:Thing ;

                       rdfs:isDefinedBy <https://w3id.org/italia/onto/COV> ;

                       rdfs:range :PublicOrganization ;

                       rdfs:subPropertyOf :hasOrganization .



###  https://w3id.org/italia/onto/COV/hasSubOrganization

:hasSubOrganization rdf:type owl:ObjectProperty ;

                    rdfs:label "has sub company"@en ,
                               "has sub-organization"@en ,
                               "ha sotto azienda"@it ,
                               "ha sotto compagnia"@it ,
                               "ha sotto-organizzazione"@it ;

                    rdfs:comment "Organizations are often large and complex and may be a collection of smaller organizations, each of which has a specific identity that may be legally defined.  This property, together with its inverse subOrganizationOf allows one to model a hierarchical structure between organizations."@en ;

                    owl:versionInfo "stable"@en ;

                    rdfs:comment "Organizzazioni sono spesso grandi e complesse e possono essere viste come una collezione di organizzazioni più piccole, ognuna delle quali con una specifica identità legamente definita. Questa proprietà, insieme alla sua inversa sotto-organizzazione di, consente di modellare una struttura gerarchica tra organizzazioni. Si pensi per esempio ai concetti di holding per organizazioni private."@it ;

                    owl:versionInfo "stabile"@it ;

                    rdfs:isDefinedBy <https://w3id.org/italia/onto/COV> ;

                    rdfs:range :Organization ;

                    rdfs:domain :Organization ;

                    owl:inverseOf :subOrganizationOf .



###  https://w3id.org/italia/onto/COV/hasSupportUnit

:hasSupportUnit rdf:type owl:ObjectProperty ;

                rdfs:label "has department"@en ,
                           "has support unit"@en ,
                           "ha dipartimento"@it ,
                           "ha divisione"@it ;

                rdfs:comment "This property links an organization to its support units (or dividision or departments) that can be homogeneous organizational areas or offices."@en ;

                owl:versionInfo "stable"@en ;

                rdfs:comment "Questa proprietà lega un'organizzazione alle sue unità di supporto (o divisioni o dipartimenti) che possono essere areee organizzative omogenee o uffici"@it ;

                owl:versionInfo "stabile"@it ;

                rdfs:isDefinedBy <https://w3id.org/italia/onto/COV> ;

                rdfs:domain :Organization ;

                rdfs:range :SupportUnit ;

                owl:inverseOf :isSupportUnitOf .



