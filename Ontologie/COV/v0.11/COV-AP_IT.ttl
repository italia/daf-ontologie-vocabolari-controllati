                 rdfs:comment "This property links an organization with the type of function/activities it carries out."@en ;

                 owl:versionInfo "unstable"@en ;

                 rdfs:comment "Questa proprietà lega un'organizzazione con il tipo di funzioni/attività svolte."@it ;

                 owl:versionInfo "provvisoria"@it ;

                 rdfs:isDefinedBy <https://w3id.org/italia/onto/COV> ;

                 rdfs:range :ActivityType ;

                 rdfs:domain :Organization .



