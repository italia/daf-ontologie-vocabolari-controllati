:hasParticipationType rdf:type owl:ObjectProperty ;

                      rdfs:label "has participation type"@en ,
                                 "ha tipo di partecipazione nell'organizzazione"@it ;

                      rdfs:comment "This property links the Participation to its type (i.e., direct or indirect)."@en ;

                      owl:versionInfo "stable"@en ;

                      rdfs:comment "Questa proprietà collega la partecipazione al suo tipo (diretta o indiretta)."@it ;

