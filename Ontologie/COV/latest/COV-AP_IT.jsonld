    {
      "@id": "https://w3id.org/italia/onto/COV/ChangeEvent",
      "@type": "owl:Class",
      "owl:versionInfo": [
        {
          "@language": "it",
          "@value": "stabile"
        },
        {
          "@language": "en",
          "@value": "stable"
        }
      ],
      "prov:wasDerivedFrom": {
