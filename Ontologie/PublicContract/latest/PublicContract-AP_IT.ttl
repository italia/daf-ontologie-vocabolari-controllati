        rdfs:subClassOf :ProcurementDocument ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCPV ;
                          owl:someValuesFrom :CPV
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasNoticeType ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :NoticeType
                        ] ;
        rdfs:comment "La classe che rappresenta qualunque tipo di annuncio o avviso relativo ad una procedura di affidamento. Tipicamente, un bando pubblico o un'altra tipologia di avviso o invito."@it ,
                     "This class represents any kind of notice or announcement related to a procedure of actualization of an intervention. Usually, it refers to a public call or any kind of notice or invitation."@en ;
        rdfs:isDefinedBy <https://w3id.org/italia/onto/PublicContract> ;
        rdfs:label "Avviso"@it ,
                   "Notice"@en ;
        owl:versionInfo "stabile"@it ,
                        "stable"@en .


