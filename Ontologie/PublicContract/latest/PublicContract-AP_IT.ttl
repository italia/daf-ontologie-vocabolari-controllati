###  https://w3id.org/italia/onto/PublicContract/Tender
:Tender rdf:type owl:Class ;
        rdfs:subClassOf l0:Entity ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :appliesToLot ;
                          owl:someValuesFrom :Lot
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :attachesTenderDocument ;
                          owl:someValuesFrom :TenderDocument
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :tenderer ;
                          owl:allValuesFrom l0:Agent
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :submittedDuring ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :TenderSubmission
                        ] ;
        rdfs:comment "Questa classe rappresenta un'entità di offerta che si applica al lotto, a cui è attaccato un documento di offerta e che è presentata da un offerente durante la fase di presentazione delle offerte (rappresenta una relazione n-aria)"@it ,
                     "This class represents and entity related to the offer that applies to the lot, to which is attached a document and that it is presented during the tender submission phase (n-ary relationship)"@en ;
        rdfs:isDefinedBy <https://w3id.org/italia/onto/PublicContract> ;
        rdfs:label "Offerta"@it ,
                   "Tender"@en ;
        owl:versionInfo "stabile"@it ,
                        "stable"@en ;
        prov:wasInfluencedBy <http://w3id.org/ocds/ns#Tender> .


