                                              )
                                ] ;
                    rdfs:range :EconomicSummary ;
                    rdfs:comment "Questa proprietà indica il prospetto economico di un lotto o contratto."@it ,
                                 "This property represents the economic summary of a lot or contract."@en ;
                    rdfs:isDefinedBy <https://w3id.org/italia/onto/PublicContract> ;
                    rdfs:label "ha prospetto economico"@it ,
                               "has economic summary"@en ;
                    owl:versionInfo "stabile"@it ,
                                    "stable"@en .


