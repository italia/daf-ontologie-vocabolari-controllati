                                  owl:onClass :Tender
                                ] ;
                rdfs:comment "Classe che rappresenta il documento con l'offerta di un offerente in risposta a un bando di gara"@it ,
                             "It represents the document which includes the offer submitted as response to a call for tender"@en ;
                rdfs:isDefinedBy <https://w3id.org/italia/onto/PublicContract> ;
                rdfs:label "Documento di Offerta"@it ,
                           "Tender Document"@it ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en ;
                prov:wasInfluencedBy <http://data.europa.eu/eproc/ontology#Tender> .


