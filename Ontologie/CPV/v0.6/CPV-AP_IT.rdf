    <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
    <rdfs:label xml:lang="it">residente in</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/knows">
    <rdfs:comment xml:lang="it">La relazione di conoscenza che intercorre tra due persone. Ex. 'Mario Rossi' conosce 'Paolo Neri'.</rdfs:comment>
    <rdfs:label xml:lang="it">conosce</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasRelationshipWith"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">knows</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
    <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:comment xml:lang="en">The relationship of knowledge between two people. E.g. 'John White' knows 'Bob Black'.</rdfs:comment>
  </rdf:Description>
