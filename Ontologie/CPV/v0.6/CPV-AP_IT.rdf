  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/hasCurrentResidence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="it">ha residenza attuale</rdfs:label>
    <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
    <rdfs:label xml:lang="en">has current residence</rdfs:label>
    <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isCurrentResidenceOf"/>
    <rdfs:comment xml:lang="en">The relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person.</rdfs:comment>
