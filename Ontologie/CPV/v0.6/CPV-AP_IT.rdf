    <rdfs:label xml:lang="it">luogo di morte</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
    <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Location"/>
    <rdfs:label xml:lang="en">death place</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/FamilySheetHolder">
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdfs:label xml:lang="it">Intestatario Foglio di Famiglia</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/CPV/ParentalRelationship"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="it">Questa classe rappresenta l'intestatario del foglio di famiglia. Per intestatario del foglio di famiglia si intende la persona cui è intestata la scheda di famiglia in anagrafe.</rdfs:comment>
    <rdfs:comment xml:lang="en">This class represents the holder of the family sheet. The holder of the family sheet is defined as the person to whom the family in the civil register is headed.</rdfs:comment>
    <rdfs:label xml:lang="en">Family Sheet Holder</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/isChildOf">
    <rdfs:comment xml:lang="en">The relation child-parent (e.g. John is the son of Peter).</rdfs:comment>
    <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isParentOf"/>
    <rdfs:label xml:lang="it">figlio di</rdfs:label>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith"/>
    <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    <rdfs:label xml:lang="en">child of</rdfs:label>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:comment xml:lang="it">La relazione figlio-padre (ex. Mario è figlio di Carlo).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
  </rdf:Description>
