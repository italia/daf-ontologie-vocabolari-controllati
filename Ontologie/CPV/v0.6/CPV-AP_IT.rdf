  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/residentIn">
    <owl:propertyChainAxiom rdf:nodeID="fe019ab14e3c146a7a0cf99c681b0374cb1"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">This property represents the residence expressed as binary relation between a person and an address.</rdfs:comment>
