  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Stateless">
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <owl:disjointWith rdf:nodeID="ub24bL1169C29"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
    <rdfs:comment xml:lang="it">Una persona naturale emigrata all'estero, che non ha alcuna cittadinanza, perché priva di quella di origine e non in possesso di un'altra</rdfs:comment>
    <rdfs:label xml:lang="it">Apolide</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/CPV/Alive"/>
    <rdfs:label xml:lang="en">Stateless Person</rdfs:label>
    <rdfs:comment xml:lang="en">A natural person who emigrated abroad, who has no nationality, because he or she does not have the nationality of origin and does not possess another one.</rdfs:comment>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/ForeignCitizen">
