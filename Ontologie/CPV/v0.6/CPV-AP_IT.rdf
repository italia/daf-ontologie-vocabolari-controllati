    <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/identifier"/>
    <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/EducationLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="it">codice univoco che identifica il grado di istruzione nel vocabolario controllato EducationLevel https://w3id.org/italia/controlled-vocabulary/classifications-for-people/education-level</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
