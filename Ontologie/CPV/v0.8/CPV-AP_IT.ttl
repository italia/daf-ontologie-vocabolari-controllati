     rdfs:isDefinedBy <https://w3id.org/italia/onto/CPV> ;
     rdfs:label "Sesso"@it ,
                "Sex"@en ;
     owl:versionInfo "stabile"@it ,
                     "stable"@en ;
     l0:controlledVocabulary <https://w3id.org/italia/controlled-vocabulary/classifications-for-people/sex> .


