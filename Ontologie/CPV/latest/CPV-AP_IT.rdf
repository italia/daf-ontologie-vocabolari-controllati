    <!-- https://w3id.org/italia/onto/CPV/ResidenceInTime -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/CPV/ResidenceInTime">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/TI/TimeIndexedEvent"></rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/CPV/residenceInLocation"></owl:onProperty>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/italia/onto/CLV/Address"></owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
