                                                                                                                                                                    rdf:first "Other" ;
                                                                                                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                               rdf:first "Prospective" ;
                                                                                                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                                          rdf:first "Retrospective" ;
                                                                                                                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                                                     rdf:first "Retrospective/Prospective" ;
                                                                                                                                                                                                     rdf:rest rdf:nil
