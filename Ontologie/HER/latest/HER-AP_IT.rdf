    <!-- https://w3id.org/italia/onto/HER/patentPriorityNo -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/onto/HER/patentPriorityNo">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/identifier"></rdfs:subPropertyOf>
