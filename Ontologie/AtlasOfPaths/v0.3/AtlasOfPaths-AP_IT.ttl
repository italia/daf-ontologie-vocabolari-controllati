:Pathway rdf:type owl:Class ;
         rdfs:subClassOf route:Route ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasMaintainer ;
                           owl:someValuesFrom l0:Agent
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasSurveillance ;
                           owl:someValuesFrom l0:Agent
                         ] ,
