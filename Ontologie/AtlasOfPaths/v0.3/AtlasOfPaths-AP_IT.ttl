                            owl:onProperty :hasTravellingMethod ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :TravellingMethod
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty route:isTripPlanOf ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Path
                          ] ;
          rdfs:comment "Questa classe rappresenta le informazioni relative alla pianificazione del percorso del cammino da intraprendere quali, ad esempio, il possibile metodo di viaggio o il periodo dell'anno preferibile."@it ,
                       "This class represents the information related to the path planning such as, for example, the possible travel method or the preferable period of the year."@en ;
          rdfs:label "Path Plan"@en ,
                     "Piano di viaggio del percorso"@it ;
          owl:versionInfo "provvisoria"@it ,
                          "unstable"@en .


