                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty sm:hasWebSite ;
                        owl:someValuesFrom sm:WebSite
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :encountersPath ;
                        owl:allValuesFrom :Path
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty route:hasAltRoute ;
                        owl:allValuesFrom :Pathway
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty route:hasDeviation ;
                        owl:allValuesFrom :Pathway
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty route:hasRoute ;
                        owl:allValuesFrom :Pathway
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty route:hasTripPlan ;
                        owl:allValuesFrom :PathPlan
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty route:hasPrefRoute ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Pathway
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty l0:description ;
                        owl:someValuesFrom rdfs:Literal
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty l0:name ;
                        owl:someValuesFrom rdfs:Literal
                      ] ;
      owl:disjointWith :TravellingMethod ,
                       route:Route ,
                       route:TripPlan ;
      rdfs:comment "Questa classe rappresenta il concetto di cammino che ricomprende: la sua tematica d'interesse, i suoi diversi percorsi (principale, alternativo e deviazione), i cammini che lo incrociano, gli enti di gestione e manutenzione, il sito web."@it ,
                   "This class represents the concept of a path that includes: its theme of interest, its different pathways (main, alternative and deviation), its encountered paths, its management and maintenance's entities, its website."@en ;
      rdfs:label "Cammino"@it ,
                 "Path"@en ;
      owl:versionInfo "stabile"@it ,
                      "stable"@en .


