###  https://w3id.org/italia/onto/AtlasOfPaths/bestWhen
:bestWhen rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf ti:hasTemporalCoverage ;
          rdfs:domain :PathPlan ;
          rdfs:range ti:TemporalEntity ;
          rdfs:comment "Questa object property collega un piano di viaggio con un'entità temporale che specifica quale sia il periodo migliore dell'anno per percorrere un certo percorso."@it ,
                       "This object property correlates a path plan with a temporal entity that specifies the best period of the year to walk a certain pathway."@en ;
          rdfs:label "best when"@en ,
                     "periodo migliore"@it ;
          owl:versionInfo "provvisoria"@it ,
                          "unstable"@en .


###  https://w3id.org/italia/onto/AtlasOfPaths/encountersPath
:encountersPath rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain :Path ;
                rdfs:range :Path ;
                owl:propertyChainAxiom ( route:hasRoute
                                         l0:hasMember
                                         :encountersPathStage
                                         l0:isMemberOf
                                         [ owl:inverseOf route:hasRoute
                                         ]
                                       ) ;
                rdfs:comment "Questa object property permette di collegare un cammino con gli altri cammini incontrati."@it ,
                             "This object property specifies which paths encounter a considered path, which is the subject of triples having as predicate encountersPath."@en ;
                rdfs:label "encounters path"@en ,
                           "incontra cammino"@it ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


###  https://w3id.org/italia/onto/AtlasOfPaths/encountersPathStage
:encountersPathStage rdf:type owl:ObjectProperty ,
                              owl:SymmetricProperty ;
                     rdfs:domain :PathStage ;
                     rdfs:range :PathStage ;
                     rdfs:comment "Questa object property permette di associare una tappa di un cammino con quella di un cammino differente localizzata nello stesso luogo."@it ,
                                  "This object property allows to associate a stage of a path with that of a different path located in the same place."@en ;
                     rdfs:label "encounters path stage"@en ,
                                "incontra tappa del cammino"@it ;
                     owl:versionInfo "provvisoria"@it ,
                                     "unstable"@en .


###  https://w3id.org/italia/onto/AtlasOfPaths/forPathway
:forPathway rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasQuantityOfPaving ;
            rdfs:domain :QuantifiedPathwayPaving ;
            rdfs:range :Pathway ;
            rdfs:comment "Questa object property mette in relazione un percorso con la quantità di una sua specifica pavimentazione, ovvero l'asfalto."@it ,
                         "This object property correlates a pathway with the quantity of its specific paving, i.e. the asphalt."@en ;
            rdfs:label "for pathway"@en ,
                       "per percorso"@it ;
            owl:versionInfo "provvisoria"@it ,
                            "unstable"@en .


