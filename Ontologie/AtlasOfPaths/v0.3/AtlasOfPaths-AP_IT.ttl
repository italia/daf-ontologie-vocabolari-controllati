                    rdfs:comment "Questa object property collega un cammino con il suo organo di governo."@it ,
                                 "This object property connects a path with its governance organ."@en ;
                    rdfs:label "ha organo di governo"@it ,
                               "has governance organ"@en ;
                    owl:versionInfo "provvisoria"@it ,
                                    "unstable"@en .


