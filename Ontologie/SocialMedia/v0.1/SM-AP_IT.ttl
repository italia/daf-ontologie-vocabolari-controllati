###  http://dati.gov.it/onto/smapit/hasCreator

:hasCreator rdf:type owl:ObjectProperty ;
            
            rdfs:label "has creator"@en ,
                       "ha autore"@it ;
            
            rdfs:comment "This property states that a post has been created by a certain user account."@en ,
                         "Questa proprietà asserisce che un post è stato creato da un certo account utente."@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
            
            rdfs:domain :Post ;
            
            rdfs:range :UserAccount ;
            
            prov:wasInfluencedBy <http://rdfs.org/sioc/ns> ;
            
            rdfs:subPropertyOf owl:topObjectProperty .



###  http://dati.gov.it/onto/smapit/hasEmail

:hasEmail rdf:type owl:ObjectProperty ;
          
          rdfs:label "has email"@en ,
                     "ha email"@it ;
          
          rdfs:comment "This property links an agent with one or more email addresses of various types."@en ,
                       "This property links an email with an agent."@en ,
                       "Questa proprietà collega un agente con uno o più indirizzi email di vario tipo."@it ,
                       "Questa proprietà collega un'email con un agente."@it ;
          
          rdfs:domain l0:Agent ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
          
          rdfs:range :Email .



###  http://dati.gov.it/onto/smapit/hasEmailType

:hasEmailType rdf:type owl:ObjectProperty ;
              
              rdfs:label "has email type"@en ,
                         "ha tipo email"@it ;
              
              rdfs:comment "This property links the Email to its type."@en ,
                           "Questa proprietà collega l'email al suo tipo."@it ;
              
              rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
              
              rdfs:domain :Email ;
              
              rdfs:range :EmailType .



###  http://dati.gov.it/onto/smapit/hasImage

:hasImage rdf:type owl:ObjectProperty ;
          
          rdfs:label "has image"@en ,
                     "ha immagine"@it ;
          
          rdfs:comment "This property links anything to an image."@en ,
                       "Questa proprietà collega qualsiasi cosa ad un'immagine."@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
          
          rdfs:range :Image ;
          
          rdfs:domain owl:Thing .



###  http://dati.gov.it/onto/smapit/hasReply

:hasReply rdf:type owl:ObjectProperty ;
          
          rdfs:label "has reply"@en ,
                     "ha risposta"@it ;
          
          rdfs:comment "The has reply property links an Item to other follow-up content Items made on the same topic. Use of this property can result in a single branch of replies from the original Item, or in a tree-like structure with branches forking at different Items along the way."@en ,
                       "La proprietà 'ha risposta' permette di dire che un post ha una risposta, ovvero, permette di creare thread di post ed organizzarla in una struttura ad albero."@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
          
          rdfs:domain :Post ;
          
          rdfs:range :Post ;
          
          prov:wasInfluencedBy <http://rdfs.org/sioc/ns#has_reply> ;
          
          rdfs:subPropertyOf owl:topObjectProperty .



###  http://dati.gov.it/onto/smapit/hasUserAccount

:hasUserAccount rdf:type owl:ObjectProperty ;
                
                rdfs:label "has user account"@en ,
                           "ha account utente"@it ;
                
                rdfs:comment "This property links an agent to its user account."@en ,
                             "Questa proprietà collega un agente al suo account utente."@it ;
                
                rdfs:domain l0:Agent ;
                
                rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
                
                rdfs:range :UserAccount ;
                
                rdfs:subPropertyOf owl:topObjectProperty .



###  http://dati.gov.it/onto/smapit/hasWebSite

:hasWebSite rdf:type owl:ObjectProperty ;
            
            rdfs:label "has Web site"@en ,
                       "ha pagina Web"@it ;
            
            rdfs:comment "Relation between anything and its web site."@en ,
                         "Relazione tra una qualsiasi entità e la sua pagina Web."@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
            
            rdfs:range :WebSite ;
            
            rdfs:domain owl:Thing .



###  http://dati.gov.it/onto/smapit/isAccountIssuedBy

:isAccountIssuedBy rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "is accunt issued by"@en ,
                              "è account rilasciato da"@it ;
                   
                   rdfs:comment "This property represents the relation between a social media and the user account issued by such a social media."@en ,
                                "Questa proprietà rappresenta la relazione tra un social media e gli account utenti che sono stati rilasciati su tale media."@it ;
                   
                   rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
                   
                   rdfs:range :SocialMedia ;
                   
                   rdfs:domain :UserAccount .



###  http://dati.gov.it/onto/smapit/isCreatorOf

:isCreatorOf rdf:type owl:ObjectProperty ;
             
             rdfs:label "is creator of"@en ,
                        "è autore di"@it ;
             
             rdfs:comment "The fact that a user account creates a post."@en ,
                          "La relazione che permette di asserire che un account utente ha creato un certo post."@it ;
             
             rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
             
             rdfs:range :Post ;
             
             rdfs:domain :UserAccount ;
             
             owl:inverseOf :hasCreator ;
             
             prov:wasInfluencedBy <http://rdfs.org/sioc/ns> .



###  http://dati.gov.it/onto/smapit/isEmailOf

:isEmailOf rdf:type owl:ObjectProperty ;
           
           rdfs:label "is email of"@en ,
                      "è email di"@it ;
           
           rdfs:range l0:Agent ;
           
           rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
           
           rdfs:domain :Email ;
           
           owl:inverseOf :hasEmail ;
           
           rdfs:subPropertyOf owl:topObjectProperty .



###  http://dati.gov.it/onto/smapit/isEmailTypeOf

:isEmailTypeOf rdf:type owl:ObjectProperty ;
               
               rdfs:label "is email type of"@en ,
                          "è tipo di email di"@it ;
               
               rdfs:comment "Inverse property of has email type. Hence, this property links an email type to an email."@en ,
                            "Proprietà inversa di 'ha tipo di email'. Di conseguenza, questa proprietà collega un individuo della classe 'Tipo di email' ad un individuo della classe 'Email'."@it ;
               
               rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
               
               rdfs:range :Email ;
               
               rdfs:domain :EmailType ;
               
               owl:inverseOf :hasEmailType .



###  http://dati.gov.it/onto/smapit/isImageOf

:isImageOf rdf:type owl:ObjectProperty ;
           
           rdfs:label "is image of"@en ,
                      "è immagine di"@it ;
           
           rdfs:comment "This property links an image to anything. It is defined as the inverse property of 'has image'."@en ,
                        "Questa proprietà collega un'immagine a qualsiasi cosa. E' la proprietà inversa di 'ha immagine'."@it ;
           
           rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
           
           rdfs:domain :Image ;
           
           rdfs:range owl:Thing .



###  http://dati.gov.it/onto/smapit/isReplyOf

:isReplyOf rdf:type owl:ObjectProperty ;
           
           rdfs:label "is reply of"@en ,
                      "è risposta di"@it ;
           
           rdfs:comment "The property 'is reply of' asserts that a given post is the answer to another post."@en ,
                        "La proprietà 'è risposta di' permette di asserire che un certo post è la risposta ad un altro post."@it ;
           
           rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
           
           owl:inverseOf :hasReply ;
           
           prov:wasInfluencedBy <http://rdfs.org/sioc/ns#reply_of> .



###  http://dati.gov.it/onto/smapit/isUserAccountOf

:isUserAccountOf rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "is user account of"@en ,
                            "è account utente di"@it ;
                 
                 rdfs:comment "This property links a user account to its owner, which, in turn, is an agent. This property is the inverse of 'has user account'."@en ,
                              "Questa proprietà collega un account utente al suo proprietario, che è un istanza di agente. E' la proprietà inversa di 'ha account utente'."@it ;
                 
                 rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
                 
                 owl:inverseOf :hasUserAccount .



###  http://dati.gov.it/onto/smapit/isWebSiteOf

:isWebSiteOf rdf:type owl:ObjectProperty ;
             
             rdfs:label "is Web site of"@en ,
                        "è sito Web di"@it ;
             
             rdfs:comment "Relation between a Web site and the entity this Web site is associated with. This property is the inverse of 'has Web site'."@en ,
                          "Relazione tra un sito Web e l'entità a cui il sito Web è associato. Questa proprietà è l'inversa di 'ha sito Web'."@it ;
             
             rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
             
             rdfs:domain :WebSite ;
             
             owl:inverseOf :hasWebSite ;
             
             rdfs:range owl:Thing .



###  http://dati.gov.it/onto/smapit/issuesAccount

:issuesAccount rdf:type owl:ObjectProperty ;
               
               rdfs:label "issues account"@en ,
                          "rilascia account"@it ;
               
               rdfs:comment "This property means that a Social Media issues a user account."@en ,
                            "Questa proprietà rappresenta il rilascio di un account utente da parte di un social media."@it ;
               
               rdfs:isDefinedBy <http://dati.gov.it/onto/smapit/> ;
               
               rdfs:domain :SocialMedia ;
               
               rdfs:range :UserAccount ;
               
               owl:inverseOf :isAccountIssuedBy .





