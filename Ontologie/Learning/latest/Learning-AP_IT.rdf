    <!-- https://w3id.org/italia/onto/Learning/Enrolment -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/Learning/Enrolment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/EventOrSituation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/Learning/atInstitute"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/italia/onto/Learning/Institute"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/Learning/atInstitute"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/Learning/Institute"/>
