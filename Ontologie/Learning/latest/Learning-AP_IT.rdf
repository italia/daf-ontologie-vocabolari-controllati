    <!-- https://w3id.org/italia/onto/Learning/hasAchievedTitle -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/Learning/hasAchievedTitle">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/Learning/Enrolment"/>
