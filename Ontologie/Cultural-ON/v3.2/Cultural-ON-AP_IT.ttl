#    Object Properties
#################################################################

###  http://dati.beniculturali.it/cis/catalogue
:catalogue rdf:type owl:ObjectProperty ;
           owl:inverseOf :isCatalogueOf ;
           rdfs:domain :CulturalInstituteOrSite ;
           rdfs:range :Catalogue ;
           rdfs:comment "Property used to associate the Cultural Institute or SIte with a catalogue of cultural collections included in the institute or site. The relatioships can be used to indicate that a Cultural Institute or Site has an its own catalogue or situations where a Cultural Institute or Site contributes to a union catalogue (this latter case is represented by libraries that can contribute to or participate in a union catalogue)."@en ,
                        "Proprietà utilizzata per associare l'istituto e luogo della cultura a un  catalogo che descrive le opere in esso contenute. Tale relazione può essere utilizzata per indicare che un istituto e luogo della cultura ha un catalogo ma anche una situazione per cui un istituto e luogo della cultura partecipa a un catalogo (in quest'ultimo caso un esempio è rappresentato dalle biblioteche che possono partecipare a un catalogo collettivo)."@it ;
           rdfs:isDefinedBy : ;
           rdfs:label "catalogo"@it ,
                      "catalogue"@en ;
           owl:versionInfo "stabile"@it ,
                           "stable" .


###  http://dati.beniculturali.it/cis/describesCulturalEntity
:describesCulturalEntity rdf:type owl:ObjectProperty ;
                         owl:inverseOf :isDescribedBy ;
                         rdfs:domain :Catalogue ;
                         rdfs:range :CollectionCulEnt ;
                         rdfs:comment "Property used to associate the catalogue with the cultural heritage objects of a cultural institute or site that the catalogue describes."@en ,
                                      "Proprietà che consente di associare il catalogo alle opere di un istituto e luogo della cultura che esso descrive."@it ;
                         rdfs:isDefinedBy : ;
                         rdfs:label "describes cultural entity"@en ,
                                    "descrive entità culturale"@it ;
                         owl:versionInfo "stabile"@it ,
                                         "stable" .


###  http://dati.beniculturali.it/cis/hasAttribute
:hasAttribute rdf:type owl:ObjectProperty ;
              rdfs:domain :SiteDescription ;
              rdfs:range :Attribute ;
              rdfs:comment "Property used to associate the description of the site of the cultural institute or site an attribute (e.g., total space, shelving, etc.)"@en ,
                           "Proprietà che permette di associare una descrizione del sito dell'istituto e luogo della cultura a un attributo, inteso come caratteristica del sito (metratura, scaffalatura, etc)"@it ;
              rdfs:isDefinedBy : ;
              rdfs:label "ha attribute"@it ,
                         "has attribute"@en ;
              owl:versionInfo "stabile"@it ,
                              "stable"@en .


###  http://dati.beniculturali.it/cis/hasCISNameInTime
:hasCISNameInTime rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :CulturalInstituteOrSite
                                              :Site
                                            )
                              ] ;
                  rdfs:range :CISNameInTime ;
                  rdfs:comment "Property which associate a cultural institute or site or a site with the situation name in time"@en ,
                               "Proprietà che associa un istituto o luogo della cultura o un sito alla situazione \"denominazione nel tempo\", ovvero una determinata denominazione valida in un determinato arco temporale"@it ;
                  rdfs:isDefinedBy : ;
                  rdfs:label "ha nome luogo cultura nel tempo"@it ,
                             "has name in time"@en ;
                  owl:versionInfo "stabile"@it ,
                                  "stable" .


###  http://dati.beniculturali.it/cis/hasCISType
:hasCISType rdf:type owl:ObjectProperty ;
            rdfs:domain :CulturalInstituteOrSite ;
            rdfs:range :CISType ;
            rdfs:comment "La proprietà è utilizzata per collegare un istituto e luogo della cultura alla sua tipologia."@it ,
                         "The property is used to link a cultural institure or site to its type."@en ;
            rdfs:isDefinedBy : ;
            rdfs:label "ha tipologia luogo cultura"@it ,
                       "has cultural institute or site type"@en ;
            owl:versionInfo "unstable"@en ,
                            "versione non definitiva"@it .


###  http://dati.beniculturali.it/cis/hasCollection
:hasCollection rdf:type owl:ObjectProperty ;
               rdfs:domain :CulturalInstituteOrSite ;
               rdfs:range :CollectionCulEnt ;
               rdfs:comment "Property used to associate the Cultural Institute or Site with the set of cultural objects it contains."@en ,
                            "Proprietà che permette di associare l'istituto e luogo della cultura all'insieme delle opere in esso contenute."@it ;
               rdfs:isDefinedBy : ;
               rdfs:label "ha collezione"@it ,
                          "has collection"@en ;
               owl:versionInfo "stabile"@it ,
                               "stable" .


###  http://dati.beniculturali.it/cis/hasDiscipline
:hasDiscipline rdf:type owl:ObjectProperty ;
               rdfs:domain :CulturalInstituteOrSite ;
               rdfs:range :SubjectDiscipline ;
               rdfs:comment "Property used to associate the Cultural Institute or Site with its own field of knowledge"@en ,
                            "Proprietà che permette di specificare la categoria a cui l'istituto e luogo della cultura è tipicamente associato (es. archeologia, etnografia e antropologia, arte, ...)."@it ;
               rdfs:isDefinedBy : ;
               rdfs:label "ha ambito disciplinare"@it ,
                          "has discipline"@en ;
               owl:versionInfo "stabile"@it ,
                               "stable" .


###  http://dati.beniculturali.it/cis/hasMemberColCultEnt
:hasMemberColCultEnt rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf l0:hasMember ;
                     owl:inverseOf :isMemberOfColCultEnt ;
                     rdfs:domain :CollectionCulEnt ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( :CollectionCulEnt
                                                :CulturalHeritageObject
                                              )
                                ] ;
                     rdfs:comment "Property that permits to specify the cultural heritage objects that form a specific collection or the entire cultural heritage included in a cultural institute or site"@en ,
                                  "Proprietà che consente di specificare le opere che formano una determinata collezione o l'intero patrimonio di un istituto e luogo della cultura"@it ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "ha membro"@it ,
                                "has member"@en ;
                     owl:versionInfo "stabile"@it ,
                                     "stable" .


###  http://dati.beniculturali.it/cis/hasSite
:hasSite rdf:type owl:ObjectProperty ;
         owl:inverseOf :isSiteOf ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :CulturalInstituteOrSite ;
         rdfs:range :Site ;
         rdfs:comment "Property which links a cultural institute or site to its own physical site(s)."@en ,
                      "Proprietà che collega un istituto e luogo della cultura al proprio o ai propri siti fisici."@it ;
         rdfs:isDefinedBy : ;
         rdfs:label "ha contenitore fisico"@it ,
                    "ha sede"@it ,
                    "has site"@en ;
         owl:versionInfo "stabile"@it ,
                         "stable" .


###  http://dati.beniculturali.it/cis/hasSiteDescription
:hasSiteDescription rdf:type owl:ObjectProperty ;
                    rdfs:domain :Site ;
                    rdfs:range :SiteDescription ;
                    rdfs:comment "Propery that permits to associate a site of a cultural institute or site with its description."@en ,
                                 "Proprietà che permette di associare una descrizione alla sede dell'istituto e luogo della cultura."@it ;
                    rdfs:isDefinedBy : ;
                    rdfs:label "ha descrizione della sede"@it ,
                               "has site description"@en ;
                    owl:versionInfo "stabile"@it ,
                                    "stable" .


###  http://dati.beniculturali.it/cis/hostsCulturalEvent
:hostsCulturalEvent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf cpevapit:hosts ;
                    owl:inverseOf :isHostedBySite ;
                    rdfs:domain :Site ;
                    rdfs:range :CulturalEvent ;
                    rdfs:comment "Property used to associate a site to an event"@en ,
                                 "Proprietà usata per associare una sede del luogo della cultura a un evento."@it ;
                    rdfs:isDefinedBy : ;
                    rdfs:label "hosts cultural event"@en ,
                               "ospita evento culturale"@it ;
                    owl:versionInfo "stabile"@it ,
                                    "stable" .


###  http://dati.beniculturali.it/cis/involvesCulturalEntity
:involvesCulturalEntity rdf:type owl:ObjectProperty ;
                        owl:inverseOf :isInvolvedInCulturalEvent ;
                        rdfs:domain :CulturalEvent ;
                        rdfs:range :CulturalEntity ;
                        rdfs:comment "Involvement property that associates a cultural event with a general cultural entity"@en ,
                                     "Proprietà di coinvolgimento che associa un evento culturale a una generica entità culturale."@it ;
                        rdfs:isDefinedBy : ;
                        rdfs:label "coinvolge entità culturale"@it ,
                                   "involves cultural entity"@en ;
                        owl:versionInfo "stabile"@it ,
                                        "stable" .


###  http://dati.beniculturali.it/cis/isAbout
:isAbout rdf:type owl:ObjectProperty ;
         owl:inverseOf :isSubjectOf ;
         rdfs:domain :CreativeWork ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :CulturalEntity
                                    :CulturalEvent
                                    :Site
                                  )
                    ] ;
         rdfs:comment "Property used to associate a creative work with a cultural entity. The same property is used in a similar way to associate the creative work with an event or site"@en ,
                      "Proprietà che consente di associare un lavoro creativo a un'entità culturale. La stessa proprietà può essere utilizzata in maniera analoga per associare un lavoro creativo a un evento o a un sito."@it ;
         rdfs:isDefinedBy : ;
         rdfs:label "is about"@en ,
                    "tratta di"@it ;
         owl:versionInfo "stabile"@it ,
                         "stable" .


###  http://dati.beniculturali.it/cis/isCatalogueOf
:isCatalogueOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Catalogue ;
               rdfs:range :CulturalInstituteOrSite ;
               rdfs:comment "Inverse property of \"has catalogue\"."@en ,
                            "Proprietà inversa di \"ha catalogo\"."@it ;
               rdfs:isDefinedBy : ;
               rdfs:label "is catalogue of"@en ,
                          "è catalogo di"@it ;
               owl:versionInfo "unstable"@en ,
                               "versione non definitiva"@it .


###  http://dati.beniculturali.it/cis/isDescribedBy
:isDescribedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :CollectionCulEnt ;
               rdfs:range :Catalogue ;
               rdfs:comment "Inverse property of \"describes\""@en ,
                            "Proprietà inversa di \"descrive\""@it ;
               rdfs:isDefinedBy : ;
               rdfs:label "is described by"@en ,
                          "è descritto da"@it .


###  http://dati.beniculturali.it/cis/isHostedBySite
:isHostedBySite rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf cpevapit:takesPlaceIn ;
                rdfs:domain :CulturalEvent ;
                rdfs:range :Site ;
                rdfs:comment "Questa proprietà è l'inversa di \"ospita\" e collega un evento a una sede di un istituto e luogo della cultura"@it ,
                             "This property is the inverse of \"hosts\" and links the Event to the Site of the Cultural Institute or Site"@en ;
                rdfs:isDefinedBy : ;
                rdfs:label "is hosted by site"@en ,
                           "è ospitato da sede luogo"@it ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


###  http://dati.beniculturali.it/cis/isInSite
:isInSite rdf:type owl:ObjectProperty ;
          rdfs:domain :CulturalHeritageObject ;
          rdfs:range :Site ;
          owl:propertyChainAxiom ( :hasMemberColCultEnt
                                   :isInSite
                                 ) ;
          rdfs:comment "Property that permits to associate cultural heritage objects with the site(s) which conserve(s) them"@en ,
                       "Proprietà che consente di associare  opere culturali al luogo fisico in cui sono conservate."@it ;
          rdfs:isDefinedBy : ;
          rdfs:label "is in site"@en ,
                     "è nella sede"@it ;
          owl:versionInfo "stabile"@it ,
                          "stable" .


###  http://dati.beniculturali.it/cis/isInvolvedInCulturalEvent
:isInvolvedInCulturalEvent rdf:type owl:ObjectProperty ;
                           rdfs:domain :CulturalEntity ;
                           rdfs:range :CulturalEvent ;
                           rdfs:comment "Inverse property of \"involves cultural entity\". It is used to directly connect a cultural entity to  the cultural events that involve it."@en ,
                                        "Proprietà inversa di \"coinvolge entità culturale\" che collega direttamente l'entità culturale con gli eventi culturali che la coinvolgono"@it ;
                           rdfs:isDefinedBy : ;
                           rdfs:label "is involved in cultural event"@en ,
                                      "è coinvolta in evento culturale"@it ;
                           owl:versionInfo "stabile"@it ,
                                           "stable"@en .


###  http://dati.beniculturali.it/cis/isInvolvedInProject
:isInvolvedInProject rdf:type owl:ObjectProperty ;
                     rdfs:domain :CulturalInstituteOrSite ;
                     rdfs:range :CulturalProject ;
                     rdfs:comment "Property used to represent an involvement relationship between a cultural institute or site and a project"@en ,
                                  "Proprietà utilizzata per rappresentare una relazione di coinvolgimento tra l'istituto e luogo della cultura e un progetto."@it ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "is involved in a project"@en ,
                                "è coinvolto in progetto"@it ;
