###  http://dati.beniculturali.it/cis/CollectionCulEnt
:CollectionCulEnt rdf:type owl:Class ;
                  rdfs:subClassOf :CulturalEntity ,
                                  l0:Collection ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasMemberColCultEnt ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:unionOf ( :CollectionCulEnt
