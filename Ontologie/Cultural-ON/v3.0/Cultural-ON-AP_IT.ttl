              owl:versionInfo "stable"@en ;
              
              rdfs:comment "Proprietà che permette di associare una descrizione del sito dell'istituto e luogo della cultura a un attributo, inteso come caratteristica del sito (metratura, scaffalatura, etc)"@it ;
              
              owl:versionInfo "stabile"@it ;
              
              rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
              
              rdfs:range :Attribute ;
              
              rdfs:domain :SiteDescription .



###  http://dati.beniculturali.it/cis/hasCISType

:hasCISType rdf:type owl:ObjectProperty ;
            
            rdfs:label "has type"@en ,
                       "ha tipo"@it ,
                       <http://dati.beniculturali.it/cis/> ;
            
            rdfs:comment "The property is used to link a cultural institure or site to its type."@en ;
            
            owl:versionInfo "unstable"@en ;
            
