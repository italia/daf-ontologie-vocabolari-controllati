###  http://dati.beniculturali.it/cis/CollectionOfCulturalEntities

:CollectionOfCulturalEntities rdf:type owl:Class ;
                              
                              rdfs:label "Collection"@en ,
                                         "Collezione"@it ;
                              
                              rdfs:subClassOf :CulturalEntity ,
                                              l0:Collection ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty l0:name ;
                                                owl:someValuesFrom rdfs:Literal
                                              ] ,
