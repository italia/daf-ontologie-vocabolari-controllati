###  http://dati.beniculturali.it/cis/accessConditionFor

:accessConditionFor rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "access condition for"@en ,
                               "condizione di accesso per"@it ;
                    
                    rdfs:comment "Inverse property of \"has access condition\""@en ,
                                 "Proprietà inversa di \"ha condizione di accesso\""@it ;
                    
                    rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
                    
                    rdfs:domain :AccessCondition ;
                    
                    owl:inverseOf :hasAccessCondition ;
                    
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( :CulturalEvent
                                               :CulturalInstituteOrSite
                                             )
                               ] .



###  http://dati.beniculturali.it/cis/available

:available rdf:type owl:ObjectProperty ;
           
           rdfs:label "available"@en ,
                      "disponibile"@it ;
           
           rdfs:comment "Property used to associate a contact point with hours specification in which the contact point is available."@en ;
           
           owl:versionInfo "stable"@en ;
           
           rdfs:comment "Proprietà utilizzata per associare  un punto di contatto a fasce orarie in cui il punto di contatto è disponibile."@it ;
           
           owl:versionInfo "stabile"@it ;
           
           rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
           
           rdfs:range :OpeningHoursSpecification ;
           
           rdfs:domain smapit:OnlineContactPoint .



###  http://dati.beniculturali.it/cis/catalogue

:catalogue rdf:type owl:ObjectProperty ;
           
           rdfs:label "catalogue"@en ,
                      "catalogo"@it ;
           
           owl:versionInfo "stable" ;
           
           rdfs:comment "Property used to associate the Cultural Institute or SIte with a catalogue of cultural collections included in the institute or site. The relatioships can be used to indicate that a Cultural Institute or Site has an its own catalogue or situations where a Cultural Institute or Site contributes to a union catalogue (this latter case is represented by libraries that can contribute to or participate in a union catalogue)."@en ,
                        "Proprietà utilizzata per associare l'istituto e luogo della cultura a un  catalogo che descrive le opere in esso contenute. Tale relazione può essere utilizzata per indicare che un istituto e luogo della cultura ha un catalogo ma anche una situazione per cui un istituto e luogo della cultura partecipa a un catalogo (in quest'ultimo caso un esempio è rappresentato dalle biblioteche che possono partecipare a un catalogo collettivo)."@it ;
           
           owl:versionInfo "stabile"@it ;
           
           rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
           
           rdfs:range :Catalogue ;
           
           rdfs:domain :CulturalInstituteOrSite .



###  http://dati.beniculturali.it/cis/describes

:describes rdf:type owl:ObjectProperty ;
           
           rdfs:label "describes"@en ,
                      "descrive"@it ;
           
           owl:versionInfo "stable" ;
           
           rdfs:comment "Property used to associate the catalogue with the cultural heritage objects of a cultural institute or site that the catalogue describes."@en ,
                        "Proprietà che consente di associare il catalogo alle opere di un istituto e luogo della cultura che esso descrive."@it ;
           
           owl:versionInfo "stabile"@it ;
           
           rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
           
           rdfs:domain :Catalogue ;
           
           rdfs:range :CollectionOfCulturalEntities .



