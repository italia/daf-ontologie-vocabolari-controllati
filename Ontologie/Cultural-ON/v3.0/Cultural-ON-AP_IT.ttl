###  http://dati.beniculturali.it/cis/hasCollection

:hasCollection rdf:type owl:ObjectProperty ;
               
               rdfs:label "has collection"@en ,
                          "ha collezione"@it ;
               
               owl:versionInfo "stable" ;
               
               rdfs:comment "Property used to associate the Cultural Institute or Site with the set of cultural objects it contains."@en ,
                            "Proprietà che permette di associare l'istituto e luogo della cultura all'insieme delle opere in esso contenute."@it ;
               
               owl:versionInfo "stabile"@it ;
               
               rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
               
               rdfs:range :CollectionOfCulturalEntities ;
               
               rdfs:domain :CulturalInstituteOrSite .



###  http://dati.beniculturali.it/cis/hasContactPoint

:hasContactPoint rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has contact point"@en ,
                            "ha punto di contatto"@it ;
                 
                 rdfs:comment "Property which links a cultural insttiute or site to its contact points. The same property is used to link in a similar way the event to its contact points and to link the site of the cultural institute or site with its contact points"@en ,
                              "Proprietà che collega un istituto e luogo della cultura ai propri punti di contatto. La stessa proprietà è utilizzata in maniera analoga per collegare il luogo fisico con i propri punti di contatto, ad anche per collegare un evento ai propri contatti"@it ;
                 
                 owl:versionInfo "stable" ,
                                 "stabile"@it ;
                 
                 rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
                 
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :CulturalEvent
                                             :CulturalInstituteOrSite
                                             :Service
                                             :Site
                                           )
                             ] .



###  http://dati.beniculturali.it/cis/hasDiscipline

:hasDiscipline rdf:type owl:ObjectProperty ;
               
