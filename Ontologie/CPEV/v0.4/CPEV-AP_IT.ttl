###  https://w3id.org/italia/onto/CPEV/Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf <https://w3id.org/italia/onto/TI/TimeIndexedEvent> ,
                       l0:EventOrSituation ;
       rdfs:isDefinedBy <https://w3id.org/italia/onto/CPEV> ;
       rdfs:label "Event"@en ,
                  "Evento"@it ;
       owl:versionInfo "stabile"@it ,
                       "stable"@en ;
